			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetChildCount"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetChildCount(Inputs, Outputs)
{
	var separator:chars = &quot;.&quot;;
	var propSetInput:PropertySet;
	
	if(Inputs.GetChildCount() == 0)
	{
		Outputs.SetProperty(&quot;ChildNum&quot;, &quot;0&quot;);
		return;
	}
	else
	 	propSetInput = Inputs.GetChild(0);
	 	
	var PSPath:chars = Inputs.GetProperty(&quot;PropertySetPath&quot;);
	var PSIndex:float = 0;
	if (Inputs.PropertyExists(&quot;PathElementSeparator&quot;))
	{
		separator = Inputs.GetProperty(&quot;PathElementSeparator&quot;);
	}
	if (Inputs.PropertyExists(&quot;PropertySetIndex&quot;))
	{
		PSIndex = parseInt(Inputs.GetProperty(&quot;PropertySetIndex&quot;), 10);
	}
	
	var leafPS = GetChildPS(propSetInput, PSPath, separator, PSIndex, true);
	
	Outputs.SetProperty(&quot;ChildNum&quot;, leafPS.GetChildCount());
}"

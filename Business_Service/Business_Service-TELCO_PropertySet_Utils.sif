			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetMultiplePropertiesValues"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetMultiplePropertiesValues(Inputs:PropertySet, Outputs:PropertySet)
{
	var propValue:chars = &quot;&quot;;
	var separator:chars = &quot;.&quot;;
	var propName:chars = &quot;&quot;;
	var leafPS:PropertySet;
	var validPath:String = &quot;Y&quot;;
	var propSetInput:PropertySet = Inputs.GetChild(0);
	var PSPath:chars = Inputs.GetProperty(&quot;PropertySetPath&quot;);
	Inputs.RemoveProperty(&quot;PropertySetPath&quot;);
	var PSIndex:float = 0;
	if (Inputs.PropertyExists(&quot;PathElementSeparator&quot;))
	{
		separator = Inputs.GetProperty(&quot;PathElementSeparator&quot;);
		Inputs.RemoveProperty(&quot;PathElementSeparator&quot;);
	}
	if (Inputs.PropertyExists(&quot;PropertySetIndex&quot;))
	{
		PSIndex = parseInt(Inputs.GetProperty(&quot;PropertySetIndex&quot;), 10);
		Inputs.RemoveProperty(&quot;PropertySetIndex&quot;);
	}
	
	try
	{
		leafPS = GetChildPS(propSetInput, PSPath, separator, PSIndex, true);
	}
	catch (e)
	{
		validPath = &quot;N&quot;;
	}
	
	propName = Inputs.GetFirstProperty();
	
	while(propName != null &amp;&amp; propName != &quot;&quot;)
	{	
		if(validPath == &quot;Y&quot;)		
		{
			if(propName.toLowerCase() == &quot;&lt;value&gt;&quot;)
				propValue = leafPS.GetValue();
			else
				propValue = leafPS.GetProperty(propName);
		}
		else
			propValue = &quot;&quot;;
		
		Outputs.SetProperty(propName, propValue);
		propName = Inputs.GetNextProperty();
	}
	
	Outputs.SetProperty(&quot;ValidPropSetPath&quot;, validPath);
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetChildPS"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetChildPS(SourcePS:PropertySet, PSPath:chars, PathSeparator:chars, PSIndex:Number, bDisplayError:bool)
{
	if (PSPath == &quot;&quot;)
	{
		return SourcePS;
	}
	
	var propPathArray = PSPath.split(PathSeparator);
	// Iterate all elements in the Path
	var arrLength = propPathArray.length;
	var currPropSet:PropertySet = SourcePS;
	for(var i=0;i&lt;arrLength;i++)
	{
		// Find Child
		var childFound:bool = false;
		
		//Find PropertySet
		var matchIndex = -1;
		for(var c=0;c&lt;currPropSet.GetChildCount();c++)
		{
			var regExpProp:RegExp = new RegExp(propPathArray[i], &quot;g&quot;);
			if(regExpProp.test(currPropSet.GetChild(c).GetType()))
			{
				matchIndex++;
				
				// Is not the last level or
				// Is the last level and the correct index
				if(i&lt;arrLength-1 || (i==arrLength-1 &amp;&amp; PSIndex==matchIndex))
				{
					childFound = true;
					currPropSet = currPropSet.GetChild(c);
					break;
				}
			}
		}
		
		// Check if Child was found
		if(childFound == false)
		{
			if (bDisplayError == true)
			{
				TheApplication().RaiseError(&apos;9136&apos;,PSPath);
			}
			else
			{
				//return empty PropertySet
				//currPropSet = TheApplication().NewPropertySet();
			}
		}
	}
	
	return currPropSet;
}"

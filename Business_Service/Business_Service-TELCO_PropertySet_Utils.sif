			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="MergePropertySets"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function MergePropertySets(Inputs:PropertySet, Outputs:PropertySet)
{
	var pTargetPS:PropertySet = Inputs.GetChild(0);
	var pSourcePS:PropertySet = Inputs.GetChild(1);
	var pSourceleafPS:PropertySet;
	var pTargetleafPS:PropertySet;
	
	//Source Vars
	var SourceSeparator:chars = &quot;.&quot;;
	var SourcePSIndex:float = 0;
	var SourceChildPath:chars = Inputs.GetProperty(&quot;SourceChildPath&quot;);
	
	//Target Vars
	var TargetSeparator:chars = &quot;.&quot;;
	var TargetPSIndex:float = 0;
	var TargetChildPath:chars = Inputs.GetProperty(&quot;TargetChildPath&quot;);

	if (Inputs.PropertyExists(&quot;SourcePathElementSeparator&quot;))
	{
		SourceSeparator = Inputs.GetProperty(&quot;SourcePathElementSeparator&quot;);
		Inputs.RemoveProperty(&quot;SourcePathElementSeparator&quot;);
	}
	if (Inputs.PropertyExists(&quot;SourcePropertySetIndex&quot;))
	{
		SourcePSIndex = parseInt(Inputs.GetProperty(&quot;SourcePropertySetIndex&quot;), 10);
		Inputs.RemoveProperty(&quot;SourcePropertySetIndex&quot;);
	}
	
	if (Inputs.PropertyExists(&quot;TargetPathElementSeparator&quot;))
	{
		TargetSeparator = Inputs.GetProperty(&quot;TargetPathElementSeparator&quot;);
		Inputs.RemoveProperty(&quot;TargetPathElementSeparator&quot;);
	}
	if (Inputs.PropertyExists(&quot;TargetPropertySetIndex&quot;))
	{
		TargetPSIndex = parseInt(Inputs.GetProperty(&quot;TargetPropertySetIndex&quot;), 10);
		Inputs.RemoveProperty(&quot;TargetPropertySetIndex&quot;);
	}

	try
	{
		//Check Source PropertySet - Remove childs
		pSourceleafPS = GetChildPS(pSourcePS, SourceChildPath, SourceSeparator, SourcePSIndex, false);
		//Check Target PropertySet - Add childs
		pTargetleafPS = GetChildPS(pTargetPS, TargetChildPath, TargetSeparator, TargetPSIndex, true);
		
		for(var c=0;c&lt;pSourceleafPS.GetChildCount();c++)
		{
			pTargetleafPS.AddChild(pSourceleafPS.GetChild(c));
		}
			
		pTargetPS.SetType(&quot;MergedPropertySet&quot;);
		Outputs.AddChild(pTargetPS);		
	}
	catch (e)
	{
		TheApplication().RaiseErrorText(e);
	}
}"

			   SCRIPT="function StringToPropSet(Inputs : PropertySet, Outputs : PropertySet) {
    var names : String = Inputs.GetProperty(&quot;Names&quot;);
    var values : String = Inputs.GetProperty(&quot;Values&quot;);
    var delimeter : String = Inputs.GetProperty(&quot;Delimeter&quot;);
    var namesArray = names.split(delimeter);
    var valuesArray = values.split(delimeter);
    var result : PropertySet = TheApplication().NewPropertySet();
    if(namesArray.length == valuesArray.length) {
        for(var i = 0; i &lt; namesArray.length; i++) {
            result.SetProperty(namesArray[i], valuesArray[i]);
        }
    }
    result.SetType(&quot;SiebelMessage&quot;);
    Outputs.AddChild(result);
}"

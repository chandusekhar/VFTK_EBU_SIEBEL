			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CheckSortingTypes"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CheckSortingTypes(psleafPS:PropertySet, psOrderedPS:PropertySet, sFieldName:String, sFieldType:String, sSortOrder:String) 
{
	/* 
	Method created to check the type of field to be compared for sorting
	- At the momment the only sort supported is for Type = DTYPE_DATETIME. If required add more types here.
	*/ 

	var bResult:bool = false;
	
	if (sFieldType == &quot;DTYPE_DATETIME&quot;)
	{
		if((Date.parse(psleafPS.GetProperty(sFieldName))) &gt;= (Date.parse(psOrderedPS.GetProperty(sFieldName))) &amp;&amp; sSortOrder==&quot;DESCENDING&quot;)
		{
			bResult = true;
		}
		else if((Date.parse(psleafPS.GetProperty(sFieldName))) &lt;= (Date.parse(psOrderedPS.GetProperty(sFieldName))) &amp;&amp; sSortOrder==&quot;ASCENDING&quot;) 
		{
			bResult = true;
		}
		else
		{
			bResult = false;
		}
	}
	//++PSANTOS 042016 - VFTRCBU RM Tool Improvements
	if (sFieldType == &quot;DTYPE_NUMBER&quot;)
	{
		if(ToNumber(psleafPS.GetProperty(sFieldName)) &gt;= ToNumber(psOrderedPS.GetProperty(sFieldName)) &amp;&amp; sSortOrder==&quot;DESCENDING&quot;)
		{
			bResult = true;
		}
		else if(ToNumber(psleafPS.GetProperty(sFieldName)) &lt;= ToNumber(psOrderedPS.GetProperty(sFieldName)) &amp;&amp; sSortOrder==&quot;ASCENDING&quot;) 
		{
			bResult = true;
		}
		else
		{
			bResult = false;
		}
	}
	//--PSANTOS 042016 - VFTRCBU RM Tool Improvements
	return 	bResult;	
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="ValidateRemoveEmptyChilds"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function ValidateRemoveEmptyChilds(Inputs:PropertySet, Outputs:PropertySet)
{
	var separator:chars = &quot;.&quot;;
	var propSetInput:PropertySet;
	var c=0;
	
	if(Inputs.GetChildCount() == 0)
	{
		Outputs.SetProperty(&quot;ChildNum&quot;, &quot;0&quot;);
		return;
	}
	else
	{
	 	propSetInput = Inputs.GetChild(0);
	}
	
	//Process parent hierarchy 	
	var PSPathParent:chars = Inputs.GetProperty(&quot;PropertySetPathParent&quot;);
	var PSPathParentLength:float = PSPathParent.length;
	var PSIndexParent:float = 0;
	if (Inputs.PropertyExists(&quot;PathElementSeparator&quot;))
	{
		separator = Inputs.GetProperty(&quot;PathElementSeparator&quot;);
	}
	/*if (Inputs.PropertyExists(&quot;PropertySetIndex&quot;))
	{
		PSIndex = parseInt(Inputs.GetProperty(&quot;PropertySetIndex&quot;), 10);
	}*/
	
	//Get Parent Hierarchy
	var leafPSParent = GetChildPS(propSetInput, PSPathParent, separator, PSIndexParent, false);
	
	if (leafPSParent.GetChildCount() &gt; 0)
	{
		//Process childs hierarchy 	
		var PSPathChilds:chars = Inputs.GetProperty(&quot;PropertySetPathChilds&quot;);
		//var PSPathParentIndex:float = 0;
		var PSPathChildsIndex:float = 0;
		var PSIndexChilds:float = 0;
	
		PSPathChildsIndex = PSPathChilds.indexOf(separator, PSPathParent.lastIndexOf(separator)+1);
		PSPathChilds = PSPathChilds.substring(PSPathChildsIndex+1, PSPathChilds.length);
		//Move in the hierarchy	
		PSPathChilds = PSPathChilds.substring(PSPathChilds.indexOf(separator)+1, PSPathChilds.length);

		while(c&lt;leafPSParent.GetChildCount())
		{
			var leafPSChild = leafPSParent.GetChild(c);
			leafPSChild = GetChildPS(leafPSChild, PSPathChilds, separator, 0, false);
			if(leafPSChild.GetChildCount() == 0)
			{
				//empty child. Remove it
				leafPSParent.RemoveChild(c);
				
			}
			else
			{
				c++;
			}
			
		}	
		
	}	
	Outputs.SetProperty(&quot;ChildNum&quot;, leafPSParent.GetChildCount());
	propSetInput.SetType(&quot;TargetPropertySet&quot;);
	Outputs.AddChild(propSetInput);
	
}"

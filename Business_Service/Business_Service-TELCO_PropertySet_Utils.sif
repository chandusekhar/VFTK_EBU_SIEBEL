*/
function GetPropertyValue(Inputs:PropertySet, Outputs:PropertySet)
{
	var propValue:chars = &quot;&quot;;
	var separator:chars = &quot;.&quot;;
	var propSetInput:PropertySet = Inputs.GetChild(0);
	var PSPath:chars = Inputs.GetProperty(&quot;PropertySetPath&quot;);
	var propName:chars = Inputs.GetProperty(&quot;PropertyName&quot;);
	var PSIndex:float = 0;
	var propSetFilterField:chars = Inputs.GetProperty(&quot;PropertySetFilterField&quot;);
	var propSetFilterValue:chars = Inputs.GetProperty(&quot;PropertySetFilterValue&quot;);
	var leafPS:PropertySet;
	
	if (Inputs.PropertyExists(&quot;PathElementSeparator&quot;))
	{
		separator = Inputs.GetProperty(&quot;PathElementSeparator&quot;);
	}
	if (Inputs.PropertyExists(&quot;PropertySetIndex&quot;))
	{
		PSIndex = parseInt(Inputs.GetProperty(&quot;PropertySetIndex&quot;), 10);
	}
	if (propSetFilterField != null &amp;&amp; propSetFilterField != &quot;&quot;)
	{
		leafPS = GetChildPSbyFilter(propSetInput, PSPath, separator, propSetFilterField, propSetFilterValue);
	}
	else	
		leafPS = GetChildPS(propSetInput, PSPath, separator, PSIndex, true);
		
	if(propName.toLowerCase() == &quot;&lt;value&gt;&quot;)
	{
		propValue = leafPS.GetValue();
	}
	else
	{
		if(leafPS.PropertyExists(propName))
		{
			propValue = leafPS.GetProperty(propName);
		}
		else
		{
			TheApplication().RaiseError(&apos;9137&apos;,propName,leafPS.GetType());
		}
	}

	Outputs.SetProperty(&quot;PropertyValue&quot;, propValue);
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CreatePropertySet"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="/*
 * Creates a Property Set base on the Inputs Names and Values
 */

function CreatePropertySet(Inputs:PropertySet, Outputs:PropertySet)
{
	var psArguments:PropertySet = TheApplication().NewPropertySet();
	var sPropname:String = &quot;&quot;;
	var sPropval:String = &quot;&quot;;
	
	sPropname = Inputs.GetFirstProperty();
	
	while (sPropname != &quot;&quot;)
	{
		sPropval = Inputs.GetProperty(sPropname);		
		psArguments.SetProperty(sPropname, sPropval);
		
		sPropname = Inputs.GetNextProperty();
	}	
	
	psArguments.SetType(&quot;SiebelMessage&quot;);
	Outputs.AddChild(psArguments);
}"

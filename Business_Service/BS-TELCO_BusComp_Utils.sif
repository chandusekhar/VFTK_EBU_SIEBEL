			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="PickRecord"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function PickRecord (Inputs:PropertySet, Outputs:PropertySet)
{
	var bo:BusObject = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BusinessObject&quot;));
	var bc:BusComp = bo.GetBusComp(Inputs.GetProperty(&quot;BusinessComponent&quot;));
	var searchExpr:chars = Inputs.GetProperty(&quot;SearchExpression&quot;);
	
	var field1:chars = Inputs.GetProperty(&quot;FieldName&quot;);
	var value1:chars = Inputs.GetProperty(&quot;FieldValue&quot;);
	var constrain:chars = Inputs.GetProperty(&quot;Constrain&quot;);
	var constrainField:chars = Inputs.GetProperty(&quot;ConstrainField&quot;);
	
	var rowCount:Number = 0;
	var valueFromField1 = &quot;&quot;;

	var primaryBCname:chars       = Inputs.GetProperty(&quot;PrimaryBC&quot;);
	var primaryBCsearchExpr:chars = Inputs.GetProperty(&quot;PrimaryBCsearchExpression&quot;);
	var sortSpec:chars            = Inputs.GetProperty(&quot;SortSpecification&quot;);
	var maxUpdates:chars          = Inputs.GetProperty(&quot;MaxUpdates&quot;);
	var max:Number = 0;
	var pickList;
	
	try
	{
		if(constrainField == null || constrainField == &quot;&quot;) 
		{
			constrainField = field1;
		}
		// Query Primary BC
		if((primaryBCname != &quot;&quot;) &amp;&amp; (primaryBCsearchExpr != &quot;&quot;))	
		{
			var primaryBC:BusComp = bo.GetBusComp(primaryBCname);
			primaryBC.SetViewMode(AllView);
			primaryBC.SetSearchExpr(primaryBCsearchExpr);
			primaryBC.ExecuteQuery(ForwardOnly);
		}
		
		bc.SetViewMode(AllView);
		bc.SetSearchExpr(searchExpr);
		
		// activate fields
		if(field1 != &quot;&quot;) bc.ActivateField(field1);
		
		bc.ExecuteQuery(ForwardOnly);
		
		// Iterate records
		var rowFound = bc.FirstRecord();
		if(rowFound)
		{
			pickList = bc.GetPicklistBusComp(field1);
	        var strCheck=/[a-zA-Z]/;
	
	        if(value1 != &quot;&quot; &amp;&amp; value1.search(strCheck) == -1){
	           var strSplit = value1.split(&quot;,&quot;);
	           value1 = &quot;&quot;;
	           for (var i = 0; i &lt; strSplit.length; i++){
	              value1 += strSplit[i];   
	           }
	        }
	
	        
			TheApplication().SetProfileAttr(&quot;TELCOSystemUpdate&quot;, &quot;Y&quot;);
			
			//if(field1 != &quot;&quot;) bc.SetFieldValue(field1, value1);
			with (pickList)
 			{
      			ClearToQuery();
      			if(constrain == &quot;Y&quot;)
      				SetSearchSpec(constrainField, value1);
      			else {
      					SetSearchSpec(&quot;Value&quot;, value1);
      			}
      			ExecuteQuery(ForwardOnly);
      			if (FirstRecord())
        			 Pick();
   			}
			bc.WriteRecord();
		}
		Outputs.SetProperty(&quot;NumberOfUpdatedRows&quot;, rowCount);
	}
	catch (e)
	{
		var sErrorDesc   = e.toString();
		var indexOfError = sErrorDesc.indexOf(&quot;SBL-DAT-00523&quot;);
			throw e;
	}
	finally
	{
		bc = null;
		bo = null;
		
		TheApplication().SetProfileAttr(&quot;TELCOSystemUpdate&quot;, &quot;N&quot;);

		if ((primaryBCname != &quot;&quot;) &amp;&amp; (primaryBCsearchExpr != &quot;&quot;))
		{
			primaryBC = null;
		}
	}
}"

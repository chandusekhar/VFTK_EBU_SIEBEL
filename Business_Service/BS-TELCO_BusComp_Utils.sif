			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="DeleteRows"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function DeleteRows(Inputs:PropertySet, Outputs:PropertySet)
{
    var sBO:String = Inputs.GetProperty(&quot;Business Object&quot;);
	var sBC:String = Inputs.GetProperty(&quot;Business Component&quot;);
	var sSearchSpec:String = Inputs.GetProperty(&quot;SearchExpression&quot;);
	var oBO:BusObject;
	var oBC:BusComp;
	var Success = &quot;N&quot;;
	
	
	try
	{
		oBO = TheApplication().GetBusObject(sBO);
		oBC = oBO.GetBusComp(sBC);
		
		oBC.SetViewMode(AllView);
		oBC.SetSearchExpr(sSearchSpec);
		oBC.ExecuteQuery(ForwardBackward);
		
		var aux = oBC.FirstRecord();
		while(oBC.FirstRecord())
		{
			oBC.DeleteRecord();
			Success = &quot;Y&quot;;
		}
		
	}
	catch(e)
	{
		Success = &quot;N&quot;;
	}
	finally
	{
		oBC = null;
		oBO = null;
		
		Outputs.SetProperty(&quot;Deleted&quot;, Success);
	}
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="PickFromActiveBusObject"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function PickFromActiveBusObject(Inputs, Outputs)
{
	try
	{
		var boActive : BusObject = null;
		var bcActive : BusComp   = null;
		var bcPick   : BusComp   = null;
		
		var sBusCompName    = Inputs.GetProperty(&quot;BusCompName&quot;);
		var sPickFieldName  = Inputs.GetProperty(&quot;PickFieldName&quot;);
		var sPickSearchExpr = Inputs.GetProperty(&quot;PickSearchExpr&quot;);

		if (sBusCompName == null || sBusCompName == &quot;&quot;)
		{
			return;
		}

		if (sPickSearchExpr == null || sPickSearchExpr == &quot;&quot;)
		{
			return;
		}

		boActive = TheApplication().ActiveBusObject()

		if (boActive == null)
		{
			return;
		}

		bcActive = boActive.GetBusComp(sBusCompName);

		if (bcActive == null)
		{
			return;
		}

		bcPick = bcActive.GetPicklistBusComp(sPickFieldName);
	
		bcPick.SetViewMode(AllView);
		bcPick.ClearToQuery();
		bcPick.SetSearchExpr(sPickSearchExpr);
		bcPick.ExecuteQuery(ForwardOnly);
		
		if (!bcPick.FirstRecord())
		{
			return;
		}

		bcPick.Pick();

		bcActive.WriteRecord();
	}
	finally
	{
		bcPick   = null;
		bcActive = null;
		boActive = null;
	}
}"

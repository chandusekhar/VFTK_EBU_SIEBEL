			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="AssocMVG"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function AssocMVG(Inputs:PropertySet, Outputs:PropertySet)
{

	var bOParent:BusObject = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BO&quot;));
	var bcParent:BusComp = bOParent.GetBusComp(Inputs.GetProperty(&quot;BC&quot;));

	var oMVF = Inputs.GetProperty(&quot;Field&quot;);
	var sIdDefault = Inputs.GetProperty(&quot;RowId&quot;);
	var sPrimaryId = Inputs.GetProperty(&quot;PrimaryId&quot;);
	var sPrimaryFlag = Inputs.GetProperty(&quot;PrimaryFlag&quot;);
	var sParentFieldAssocId = Inputs.GetProperty(&quot;FieldAssocPrimaryId&quot;);

	with(bcParent)
	{
		ActivateField(sParentFieldAssocId);
		ClearToQuery();
		SetSearchSpec(&quot;Id&quot;, sIdDefault);
		ExecuteQuery();
		
		if(FirstRecord()){

			var oMVGBusComp = bcParent.GetMVGBusComp(oMVF);
			var sAssocBC = oMVGBusComp.GetAssocBusComp();

			with (sAssocBC)
			{	
				ClearToQuery();
				SetSearchSpec(&quot;Id&quot;, sPrimaryId);
				ExecuteQuery();
				
				if(FirstRecord()){
					Associate(NewBefore);
				}
			}
			
			if(sPrimaryFlag == &quot;Y&quot;){
				SetFieldValue(sParentFieldAssocId, sPrimaryId);
			}
			
			sAssocBC = null;
			oMVGBusComp = null;
		}
	}
	
	bcParent = null;
	bOParent = null;
}"

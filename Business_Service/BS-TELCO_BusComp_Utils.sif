			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="IterateRowsAndExecuteWF"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function IterateRowsAndExecuteWF(Inputs:PropertySet, Outputs:PropertySet)
{
	var bo:BusObject;
	var bc:BusComp;
	
	try
	{
		bo = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BusinessObject&quot;));
		bc = bo.GetBusComp(Inputs.GetProperty(&quot;BusinessComponent&quot;));
		var sortSpec:chars = Inputs.GetProperty(&quot;SortSpecification&quot;);
		var searchExpr:chars = Inputs.GetProperty(&quot;SearchExpression&quot;);
		var numRows = 0;
		var maxRows = -1;
		
		// Get BC fields Inputs
		var bcObjectId = Inputs.GetProperty(&quot;BC Field Object Id&quot;);
		var	wfName = Inputs.GetProperty(&quot;WorkflowName&quot;);
		var bcField1 = Inputs.GetProperty(&quot;BC Field Input 1&quot;);
		var bcField2 = Inputs.GetProperty(&quot;BC Field Input 2&quot;);
		var bcField3 = Inputs.GetProperty(&quot;BC Field Input 3&quot;);
		var bcField4 = Inputs.GetProperty(&quot;BC Field Input 4&quot;);
		var bcField5 = Inputs.GetProperty(&quot;BC Field Input 5&quot;);
		var bcField6 = Inputs.GetProperty(&quot;BC Field Input 6&quot;);
		var bcField7 = Inputs.GetProperty(&quot;BC Field Input 7&quot;);
		var bcField8 = Inputs.GetProperty(&quot;BC Field Input 8&quot;);
		var bcField9 = Inputs.GetProperty(&quot;BC Field Input 9&quot;);
		var bcField10 = Inputs.GetProperty(&quot;BC Field Input 10&quot;);
		
		// Get WF Inputs
		var wfInput1 = Inputs.GetProperty(&quot;WF Input 1&quot;);
		var wfInput2 = Inputs.GetProperty(&quot;WF Input 2&quot;);
		var wfInput3 = Inputs.GetProperty(&quot;WF Input 3&quot;);
		var wfInput4 = Inputs.GetProperty(&quot;WF Input 4&quot;);
		var wfInput5 = Inputs.GetProperty(&quot;WF Input 5&quot;);
		var wfInput6 = Inputs.GetProperty(&quot;WF Input 6&quot;);
		var wfInput7 = Inputs.GetProperty(&quot;WF Input 7&quot;);
		var wfInput8 = Inputs.GetProperty(&quot;WF Input 8&quot;);
		var wfInput9 = Inputs.GetProperty(&quot;WF Input 9&quot;);
		var wfInput10 = Inputs.GetProperty(&quot;WF Input 10&quot;);
		var wfFixedInput1 = Inputs.GetProperty(&quot;WF Fixed Input 1&quot;);
		var wfFixedInput2 = Inputs.GetProperty(&quot;WF Fixed Input 2&quot;);
		var wfFixedInput3 = Inputs.GetProperty(&quot;WF Fixed Input 3&quot;);
		var wfFixedInput4 = Inputs.GetProperty(&quot;WF Fixed Input 4&quot;);
		var wfFixedInput5 = Inputs.GetProperty(&quot;WF Fixed Input 5&quot;);
		var wfFixedInput6 = Inputs.GetProperty(&quot;WF Fixed Input 6&quot;);
		var wfFixedInput7 = Inputs.GetProperty(&quot;WF Fixed Input 7&quot;);
		var wfFixedInput8 = Inputs.GetProperty(&quot;WF Fixed Input 8&quot;);
		var wfFixedInput9 = Inputs.GetProperty(&quot;WF Fixed Input 9&quot;);
		var wfFixedInput10 = Inputs.GetProperty(&quot;WF Fixed Input 10&quot;);
		
		if (Inputs.PropertyExists(&quot;MaxRows&quot;))
		{
			maxRows = Inputs.GetProperty(&quot;MaxRows&quot;);
		}
		
		// Query BC	
		bc.SetViewMode(AllView);
		bc.SetSearchExpr(searchExpr);
		bc.SetSortSpec(sortSpec);
		
		// activate fields
		if (bcObjectId != &quot;&quot;) bc.ActivateField(bcObjectId);
		if (bcField1 != &quot;&quot;) bc.ActivateField(bcField1);
		if (bcField2 != &quot;&quot;) bc.ActivateField(bcField2);
		if (bcField3 != &quot;&quot;) bc.ActivateField(bcField3);
		if (bcField4 != &quot;&quot;) bc.ActivateField(bcField4);
		if (bcField5 != &quot;&quot;) bc.ActivateField(bcField5);
		if (bcField6 != &quot;&quot;) bc.ActivateField(bcField6);
		if (bcField7 != &quot;&quot;) bc.ActivateField(bcField7);
		if (bcField8 != &quot;&quot;) bc.ActivateField(bcField8);
		if (bcField9 != &quot;&quot;) bc.ActivateField(bcField9);
		if (bcField10 != &quot;&quot;) bc.ActivateField(bcField10);
		
		bc.ExecuteQuery(ForwardOnly);
	
		// Check if rows returned
		var rec = bc.FirstRecord();
		
		if(rec)
		{
			var wfBS:Service = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
			var wfInputs:PropertySet = TheApplication().NewPropertySet();
			var wfOutputs:PropertySet = TheApplication().NewPropertySet();
			
			// Iterate all rows
			while(rec)
			{
				wfInputs.SetProperty(&quot;ProcessName&quot;, wfName);
			
				// Set Fixed Inputs
				if (wfFixedInput1 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput1, Inputs.GetProperty(&quot;WF Fixed Input Value 1&quot;));
				if (wfFixedInput2 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput2, Inputs.GetProperty(&quot;WF Fixed Input Value 2&quot;));
				if (wfFixedInput3 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput3, Inputs.GetProperty(&quot;WF Fixed Input Value 3&quot;));
				if (wfFixedInput4 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput4, Inputs.GetProperty(&quot;WF Fixed Input Value 4&quot;));
				if (wfFixedInput5 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput5, Inputs.GetProperty(&quot;WF Fixed Input Value 5&quot;));
				if (wfFixedInput6 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput6, Inputs.GetProperty(&quot;WF Fixed Input Value 6&quot;));
				if (wfFixedInput7 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput7, Inputs.GetProperty(&quot;WF Fixed Input Value 7&quot;));
				if (wfFixedInput8 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput8, Inputs.GetProperty(&quot;WF Fixed Input Value 8&quot;));
				if (wfFixedInput9 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput9, Inputs.GetProperty(&quot;WF Fixed Input Value 9&quot;));
				if (wfFixedInput10 != &quot;&quot;) wfInputs.SetProperty(wfFixedInput10, Inputs.GetProperty(&quot;WF Fixed Input Value 10&quot;));
			
				if (bcObjectId != &quot;&quot;) wfInputs.SetProperty(&quot;Object Id&quot;, bc.GetFieldValue(bcObjectId));
				if (bcField1 != &quot;&quot;) wfInputs.SetProperty(wfInput1, bc.GetFieldValue(bcField1));
				if (bcField2 != &quot;&quot;) wfInputs.SetProperty(wfInput2, bc.GetFieldValue(bcField2));
				if (bcField3 != &quot;&quot;) wfInputs.SetProperty(wfInput3, bc.GetFieldValue(bcField3));
				if (bcField4 != &quot;&quot;) wfInputs.SetProperty(wfInput4, bc.GetFieldValue(bcField4));
				if (bcField5 != &quot;&quot;) wfInputs.SetProperty(wfInput5, bc.GetFieldValue(bcField5));
				if (bcField6 != &quot;&quot;) wfInputs.SetProperty(wfInput6, bc.GetFieldValue(bcField6));
				if (bcField7 != &quot;&quot;) wfInputs.SetProperty(wfInput7, bc.GetFieldValue(bcField7));
				if (bcField8 != &quot;&quot;) wfInputs.SetProperty(wfInput8, bc.GetFieldValue(bcField8));
				if (bcField9 != &quot;&quot;) wfInputs.SetProperty(wfInput9, bc.GetFieldValue(bcField9));
				if (bcField10 != &quot;&quot;) wfInputs.SetProperty(wfInput10, bc.GetFieldValue(bcField10));
				
								
				// Execute WF
				wfBS.InvokeMethod(&quot;RunProcess&quot;, wfInputs, wfOutputs);
				
				
				//Validate that WF has NumOfRowsProcessed output
				if (wfOutputs.PropertyExists(&quot;NumOfRowsProcessed&quot;))
				{
					numRows = numRows + ToInteger(wfOutputs.GetProperty(&quot;NumOfRowsProcessed&quot;));
				}
				else
				{
					numRows++;
				}				
				
				
				if (maxRows != -1)
				{
					if (maxRows == numRows)
					{
						rec = false;
					}
					else
					{
						rec = bc.NextRecord();
					}
				}
				else 
				{
					rec = bc.NextRecord();
				}
				
				//Reset PSs
				wfInputs.Reset();
				wfOutputs.Reset();
			}
			
			/* 	
				ESTEVAJF - 140102013
				The output value of NumOfRowsProcessed is the same as TotalNumberOfRows. The difference it is that 
				NumOfRowsProcessed is used inside the above while in order to force that numRows only be incremented when 
				the WF return this property. If this property exists, it means that the WF it is returning 1 when wants to increment 
				numRows counter and 0 when does NOT want numRows counter to be incremented.
			*/
			Outputs.SetProperty(&quot;TotalNumberOfRows&quot;, numRows);
			Outputs.SetProperty(&quot;NumOfRowsProcessed&quot;,numRows);
			
			wfOutputs = null;
			wfInputs = null;
			wfBS = null;
		}
	}
	catch(e)
	{
		throw e;
	}	
	finally
	{
		bc = null;
		bo = null;
	}
}"

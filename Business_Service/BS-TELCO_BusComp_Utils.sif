			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="InsertRow"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function InsertRow(Inputs:PropertySet, Outputs:PropertySet)
{
try
	{
		// Declaration section
		var sNewRecordId:chars = &quot;&quot;;
		var sSkipNullValue:chars = &quot;&quot;;

		var bo:BusObject;
		var bc:BusComp;

		bo = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BusinessObject&quot;));
		bc = bo.GetBusComp(Inputs.GetProperty(&quot;BusinessComponent&quot;));
		sSkipNullValue = Inputs.GetProperty(&quot;SkipNull&quot;);
		
		var sNewIndicator = Inputs.GetProperty(&quot;NewWhereIndicator&quot;);

		var cFIELD_NAME_LABEL  = &quot;FieldName&quot;;
		var cFIELD_VALUE_LABEL = &quot;FieldValue&quot;;

		var sFieldNameAux:chars  = &quot;&quot;;
		var sFieldValueAux:chars = &quot;&quot;;
		var sPropertyName:chars  = &quot;&quot;;
		var sPropertyValue:chars = &quot;&quot;;

		var nFieldIndex:Number = 1;

		// Checks the existence of creation indicator
		if(sNewIndicator == &quot;&quot;)
		{
			sNewIndicator = &quot;NewBefore&quot;;
		}

		// Creates the record
		bc.NewRecord(sNewIndicator);

		// Fills in the provided values
		sPropertyName = cFIELD_NAME_LABEL + Clib.rsprintf(&quot;%03u&quot;, nFieldIndex);
		sFieldNameAux = Inputs.GetProperty(sPropertyName);

		while(sFieldNameAux != &quot;&quot;)
		{
			sPropertyValue = cFIELD_VALUE_LABEL + Clib.rsprintf(&quot;%03u&quot;, nFieldIndex);
			sFieldValueAux = Inputs.GetProperty(sPropertyValue);

			if (sFieldValueAux == &quot;&quot; &amp;&amp; sSkipNullValue == &quot;Y&quot;)
			{
				//do nothing
			}
			else
				bc.SetFieldValue(sFieldNameAux, sFieldValueAux);

			nFieldIndex++;

			sPropertyName = cFIELD_NAME_LABEL + Clib.rsprintf(&quot;%03u&quot;, nFieldIndex);
			sFieldNameAux = Inputs.GetProperty(sPropertyName);
		}

		// Commits the record
		bc.WriteRecord();

		sNewRecordId = bc.GetFieldValue(&quot;Id&quot;);
	}
	finally
	{
		Outputs.SetProperty(&quot;NewRecordId&quot;, sNewRecordId);

		bc = null;
		bo = null;
	}
}"

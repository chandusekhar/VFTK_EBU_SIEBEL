			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetFieldsFromFirstRow"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetFieldsFromFirstRow(Inputs:PropertySet, Outputs:PropertySet, Formatted:String)
{
	var bo:BusObject = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BusinessObject&quot;));
	var bc:BusComp = bo.GetBusComp(Inputs.GetProperty(&quot;BusinessComponent&quot;));
	var primaryBC:BusComp;
	var sortSpec:chars    = Inputs.GetProperty(&quot;SortSpecification&quot;);
	var searchExpr:chars  = Inputs.GetProperty(&quot;SearchExpression&quot;);
	var returnCount:chars = Inputs.GetProperty(&quot;ReturnRecordCount&quot;);
	var ignoreQueryError:chars = Inputs.GetProperty(&quot;IgnoreQueryError&quot;);
	var primaryBCname:chars       = Inputs.GetProperty(&quot;PrimaryBC&quot;);
	var primaryBCsearchExpr:chars = Inputs.GetProperty(&quot;PrimaryBCsearchExpression&quot;);
	var sQueryField1:chars = Inputs.GetProperty(&quot;Query Field 1&quot;);
	var sQueryField2:chars = Inputs.GetProperty(&quot;Query Field 2&quot;);
	var sQueryField3:chars = Inputs.GetProperty(&quot;Query Field 3&quot;);

	try
	{
		// Remove properties from Inputs:
		Inputs.RemoveProperty(&quot;BusinessObject&quot;);
		Inputs.RemoveProperty(&quot;BusinessComponent&quot;);
		Inputs.RemoveProperty(&quot;SearchExpression&quot;);
		Inputs.RemoveProperty(&quot;SortSpecification&quot;);
		Inputs.RemoveProperty(&quot;ReturnRecordCount&quot;);
		Inputs.RemoveProperty(&quot;IgnoreQueryError&quot;);
		Inputs.RemoveProperty(&quot;PrimaryBC&quot;);
		Inputs.RemoveProperty(&quot;PrimaryBCsearchExpression&quot;);
		Inputs.RemoveProperty(&quot;Query Field 1&quot;);
		Inputs.RemoveProperty(&quot;Query Field 2&quot;);
		Inputs.RemoveProperty(&quot;Query Field 3&quot;);

		//Query Primary BC
		if((primaryBCname != &quot;&quot;) &amp;&amp; (primaryBCsearchExpr != &quot;&quot;))	
		{
			primaryBC = bo.GetBusComp(primaryBCname);
			primaryBC.SetViewMode(AllView);
			primaryBC.SetSearchExpr(primaryBCsearchExpr);
			primaryBC.ExecuteQuery(ForwardOnly);
		}

		bc.SetViewMode(AllView);
		bc.SetSearchExpr(searchExpr);
		bc.SetSortSpec(sortSpec);
		
		//check query fields
		if (sQueryField1 != &quot;&quot;)
		{
			bc.ActivateField(sQueryField1);
		}
		else if (sQueryField2 != &quot;&quot;)
		{
			bc.ActivateField(sQueryField2);
		}
		else if (sQueryField3 != &quot;&quot;)
		{
			bc.ActivateField(sQueryField3);		
		}		
		
		// activate fields
		var prop:chars = Inputs.GetFirstProperty();
		while(prop != &quot;&quot;)
		{
			bc.ActivateField(prop);
			Outputs.SetProperty(prop, &quot;&quot;); // to avoid errors in WF
			prop = Inputs.GetNextProperty();
		}

		try
		{

			if (returnCount == &quot;Y&quot; || returnCount == &quot;y&quot;)
			{
				bc.ExecuteQuery(ForwardBackward); // Necessary to use CountRecords
	
				var numRows = bc.CountRecords();
				Outputs.SetProperty(&quot;TotalNumberOfRows&quot;, numRows);
				returnCount = &quot;Y&quot;;
			}
			else
			{
				bc.ExecuteQuery(ForwardOnly);
				Outputs.SetProperty(&quot;TotalNumberOfRows&quot;, &quot;0&quot;);
				returnCount = &quot;N&quot;;
			}
		}
		catch(e)
		{	
			if(!(ignoreQueryError == &quot;Y&quot;))
				throw e;	
			Outputs.SetProperty(&quot;TotalNumberOfRows&quot;, &quot;&quot;); // to avoid errors in WF
		}

		// Check if rows returned
		if (bc.FirstRecord())
		{
			if (returnCount == &quot;N&quot;)
			{
				Outputs.SetProperty(&quot;TotalNumberOfRows&quot;, &quot;1&quot;);
			}
		
			if(Formatted == &quot;Y&quot;)
			{
				//Return as variables
				if (sQueryField1 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 1&quot;, bc.GetFormattedFieldValue(sQueryField1));
				}
				else if (sQueryField2 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 2&quot;, bc.GetFormattedFieldValue(sQueryField2));
				}
				else if (sQueryField3 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 3&quot;, bc.GetFormattedFieldValue(sQueryField3));
				}
			}
			else
			{
				//Return as variables
				if (sQueryField1 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 1&quot;, bc.GetFieldValue(sQueryField1));
				}
				else if (sQueryField2 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 2&quot;, bc.GetFieldValue(sQueryField2));
				}
				else if (sQueryField3 != &quot;&quot;)
				{
					Outputs.SetProperty(&quot;Query Field Result 3&quot;, bc.GetFieldValue(sQueryField3));		
				}
			}			
					
			// set all field in Outputs
			prop = Inputs.GetFirstProperty();
			while(prop != &quot;&quot;)
			{	
				if(Formatted == &quot;Y&quot;)
				{
					Outputs.SetProperty(prop, bc.GetFormattedFieldValue(prop));
				}
				else
				{
					Outputs.SetProperty(prop, bc.GetFieldValue(prop));
				}
				prop = Inputs.GetNextProperty();
			}
		}
	}
	finally
	{
		bc = null;
		primaryBC = null;
		bo = null;
	}

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetRecordsForRepeating"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetRecordsForRepeating(Inputs:PropertySet, Outputs:PropertySet)
{
	var bo:BusObject = TheApplication().GetBusObject(Inputs.GetProperty(&quot;BusinessObject&quot;));
	var bc:BusComp = bo.GetBusComp(Inputs.GetProperty(&quot;BusinessComponent&quot;));
	var sortSpec:chars    = Inputs.GetProperty(&quot;SortSpecification&quot;);
	var searchExpr:chars  = Inputs.GetProperty(&quot;SearchExpression&quot;);
	var soutField:chars = Inputs.GetProperty(&quot;OutField&quot;);
	var maxRecords:Number = ToNumber(Inputs.GetProperty(&quot;RecordsToRetrieve&quot;));
	var currRecords:Number = 0;
	var currRecordsStr:chars = &quot;&quot;; 
	var isRecord;
	var lastRecordId:chars =&quot;&quot;;
	
	//DEF-24161-IBMRDAS
	var filterField1:chars	= Inputs.GetProperty(&quot;filterField1&quot;);
	var filterField2:chars	= Inputs.GetProperty(&quot;filterField2&quot;);
	var filterValue1:chars	= Inputs.GetProperty(&quot;filterValue1&quot;);
	var filterValue2:chars	= Inputs.GetProperty(&quot;filterValue2&quot;);
	var msisdn:String = &quot;&quot;;
	//DEF-24161-IBMRDAS

	try
	{
		// Remove properties from Inputs:
		Inputs.RemoveProperty(&quot;BusinessObject&quot;);
		Inputs.RemoveProperty(&quot;BusinessComponent&quot;);
		Inputs.RemoveProperty(&quot;SearchExpression&quot;);
		Inputs.RemoveProperty(&quot;SortSpecification&quot;);
		Inputs.RemoveProperty(&quot;soutField&quot;);
		Inputs.RemoveProperty(&quot;RecordsToRetrieve&quot;);
		
		//DEF-24161-IBMRDAS
		if(filterValue1 != &quot;&quot;)
		{
			Inputs.RemoveProperty(&quot;filterValue1&quot;);
			Inputs.RemoveProperty(&quot;filterValue2&quot;);
			Inputs.RemoveProperty(&quot;filterField1&quot;);
			Inputs.RemoveProperty(&quot;filterField2&quot;);
		}	
		
		bc.SetViewMode(AllView);
		bc.SetSearchExpr(searchExpr);
		bc.SetSortSpec(sortSpec);
		
		
		if (soutField != &quot;&quot;)
		{
			bc.ActivateField(soutField);
		}
		
		if(filterValue1 != &quot;&quot;)
		{
			bc.ActivateField(filterField1);
			bc.ActivateField(filterField2);
		}
		
        // activate remaining fields
		var prop:chars = Inputs.GetFirstProperty();
		while(prop != &quot;&quot;)
		{
			bc.ActivateField(prop);
			Outputs.SetProperty(prop, &quot;&quot;); // to avoid errors in WF
			prop = Inputs.GetNextProperty();
		}


		bc.ExecuteQuery(ForwardOnly);
		isRecord = bc.FirstRecord();
		
		while (isRecord)
		{			
			//DEF-24161-IBMRDAS
			if(filterValue1 != &quot;&quot;)
			{
				if ( bc.GetFieldValue(filterField1) == filterValue1)
				{
					msisdn = bc.GetFieldValue(filterField2);
					if (msisdn.indexOf(filterValue2) != 0) // not like 54
					{
						currRecords++;
						lastRecordId = bc.GetFieldValue(&quot;Id&quot;);
						currRecordsStr = currRecordsStr +bc.GetFieldValue(soutField)+&quot;;&quot;;
				
						isRecord = bc.NextRecord();
						if (currRecords &gt;= maxRecords)
							isRecord = false;
						
					}
					else
						isRecord = bc.NextRecord();

				}
				else
					isRecord = bc.NextRecord();
			}
			else
			{
				currRecords++;
				lastRecordId = bc.GetFieldValue(&quot;Id&quot;);
				currRecordsStr = currRecordsStr +bc.GetFieldValue(soutField)+&quot;;&quot;;
				
				isRecord = bc.NextRecord();
				if (currRecords &gt;= maxRecords)
					isRecord = false;
			}
			
			Outputs.SetProperty(&quot;RecordList&quot;,currRecordsStr);
			Outputs.SetProperty(&quot;RecordCount&quot;,currRecords);
			Outputs.SetProperty(&quot;LastRecordId&quot;,lastRecordId);
		}	
	}
	finally
	{
		bc = null;
		bo = null;
	}
}"

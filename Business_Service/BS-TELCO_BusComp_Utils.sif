			return (CancelOperation);
		case &quot;BCInvokeMethod&quot;:
			BCInvokeMethod(Inputs, Outputs);
			return (CancelOperation);
		case &quot;PickRecord&quot;:
			PickRecord(Inputs, Outputs);
			return (CancelOperation);

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="LoggingMernis"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function LoggingMernis(Inputs,Outputs)
{
   /***********************************************************************************
	* VFTK Mamta 16-Sep-11 : Created                                                 
	* Description      : This script takes its input from three interfaces-Cep Aran 
	*                    Blacklist,Black/Grey List and Credit Card Blacklist in 
						 order to log the information for failed transactions of 
						 the interfaces	
	* Modified VFTK Junaid 23-Nov-11 for WP01			
	***********************************************************************************/
	try
	{
		var strMernisLog = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_MERNIS_LOG_ABLE&quot;,&quot;Y&quot;);
		if(strMernisLog==&quot;Y&quot;)
		{
			var strVshopCode = &quot;&quot;;
			var boAccount : BusObject = TheApplication().GetBusObject(&quot;Account&quot;);
			var bcLogging : BusComp = boAccount.GetBusComp(&quot;VF Mernis Logging&quot;);
			var bcAccount : BusComp = boAccount.GetBusComp(&quot;Account HP&quot;);
			var bcContact : BusComp = boAccount.GetBusComp(&quot;Contact HP&quot;);
			var bcEmployee : BusComp = boAccount.GetBusComp(&quot;Employee&quot;);
			var strLogin = TheApplication().LoginName();
			var strLoginId =TheApplication().LoginId();
			var strModeInput  = Inputs.GetProperty(&quot;Inquiry Scenario&quot;);
			var strInquiryType = Inputs.GetProperty(&quot;Inquiry Type&quot;);
			var strMode = &quot;&quot;;
			
			//--BEGIN--//serkan kasirga. Added for BTK EndUser Project
			var inpChannel = Inputs.GetProperty(&quot;Channel&quot;);
			var inpUserId = Inputs.GetProperty(&quot;UserId&quot;);
			var ipAddress = Inputs.GetProperty(&quot;IpAddress&quot;);
			//--END--//
			
			/*FGUEDES-&gt;*VFTRCBU WA#05 Client IP Address*/
			var remHost = GetRemoteHost();
			
			if(inpChannel==&quot;OSS&quot;)
			{
			remHost	= ipAddress;
			}
			
			/*&lt;-FGUEDES*/
			if((strModeInput == &quot;Ekle&quot; || strModeInput == &quot;Çıkar&quot;) &amp;&amp; strModeInput != null &amp;&amp; strModeInput != &quot;&quot;)//VFTK Lilanath 24032012 : Changed the search spec as per Oracle Script Review 15
			{
				if(strModeInput == &quot;Ekle&quot;)
					strMode   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_VF_INQUIRY_SCENARIO&quot;,&quot;Add&quot;);
				else
					strMode   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_VF_INQUIRY_SCENARIO&quot;,&quot;Remove&quot;);
			}
			else
			strMode   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_VF_INQUIRY_SCENARIO&quot;,strModeInput);
			var strInquirytype = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_VF_INQUIRY_TYPE&quot;,strInquiryType);
			var strBACode = Inputs.GetProperty(&quot;Billing Account ID&quot;);
			var strTransaView = Inputs.GetProperty(&quot;Transaction View Name&quot;);
			//Vaibhav 02/04/2012: Changed the logic to get transaction View Name - C_MSP00000883
					//Value was hardcoded in WF - &quot;VF Cep Aran Inquiry WF&quot; as &quot;Cep Aran Black List View&quot;
			var strTranView = GetTransactionViewName();
			var strFirmOut = Inputs.GetProperty(&quot;Firm Name&quot;);
			var strBirthPlaceOut = Inputs.GetProperty(&quot;Place of Birth&quot;);
			var strMotherOut = Inputs.GetProperty(&quot;Mother Name&quot;);
			var strGenderOut = Inputs.GetProperty(&quot;Gender&quot;);
			var strFatherOut = Inputs.GetProperty(&quot;Father Name&quot;);
			var BGTransaction = Inputs.GetProperty(&quot;BGTransaction&quot;);
			var strBirthDateOut = Inputs.GetProperty(&quot;Birth Date&quot;);
			
			
			if(BGTransaction == &quot;&quot;)
			{
				var sDay = Inputs.GetProperty(&quot;Day Out&quot;);
				var sMonth = Inputs.GetProperty(&quot;Month Out&quot;);
				var sYear = Inputs.GetProperty(&quot;Year Out&quot;);
				if(sDay != &quot;&quot; &amp;&amp; sMonth != &quot;&quot; &amp;&amp; sYear != &quot;&quot;)
				{
					strBirthDateOut = sMonth+&quot;/&quot;+sDay+&quot;/&quot;+sYear;
				}
			}
			var strCCBId = Inputs.GetProperty(&quot;CCB Customer ID&quot;);
			var strCCReason = Inputs.GetProperty(&quot;CC_BL_Reason&quot;);
			var sChan = TheApplication().GetProfileAttr(&quot;ApplicationName&quot;);
			var strChannel = &quot;&quot;;

			if(sChan == &quot;TeXCo Application&quot;)
		   		strChannel = &quot;Call Center&quot;;
		   	else
		   		strChannel = &quot;Partner Portal&quot;;
		   	
		   	//serkan kasirga. Added for BTK EndUser Project
		   	if(inpChannel!=null &amp;&amp; inpChannel!=&quot;&quot;)
		   		strChannel = inpChannel;
		   	else
		   		strChannel = strChannel;
		   	
			var strDBInput = Inputs.GetProperty(&quot;Birthdate Input&quot;);
			var strFatherName = Inputs.GetProperty(&quot;Father Name Input&quot;);
			var strFirm = Inputs.GetProperty(&quot;Firm Name Input&quot;);
			var strFNIn = Inputs.GetProperty(&quot;First Name Input&quot;);
			var strFNOut = Inputs.GetProperty(&quot;First Name&quot;);
			var strGenIn = Inputs.GetProperty(&quot;Gender Input&quot;);
			var strInRes = Inputs.GetProperty(&quot;Inquiry Result&quot;);
			var strLNOut = Inputs.GetProperty(&quot;Last Name&quot;);
			var strLNIn = Inputs.GetProperty(&quot;Surname Input&quot;);
			var strMNIn = Inputs.GetProperty(&quot;Mother Name Input&quot;);
			var strOrderId = Inputs.GetProperty(&quot;Order ID&quot;);
			var strPlace = Inputs.GetProperty(&quot;BirthPlace Input&quot;);
			var strCustId = Inputs.GetProperty(&quot;AccountId&quot;);
			var strTCIDIn = Inputs.GetProperty(&quot;TCIDInput&quot;);
			var strTCIDOut = Inputs.GetProperty(&quot;TC ID&quot;);
			var strTaxID = Inputs.GetProperty(&quot;TaxID Input&quot;);
			var strTaxOff = Inputs.GetProperty(&quot;TaxOffice Input&quot;);			
			var strPrimary = Inputs.GetProperty(&quot;Primary Contact Id&quot;);
			var strTime = Inputs.GetProperty(&quot;Time Stamp&quot;);
			var sStartDate = Inputs.GetProperty(&quot;Start Date Input&quot;);
			var sTaxOut = Inputs.GetProperty(&quot;Tax ID&quot;);
			var sTaxOffOut = Inputs.GetProperty(&quot;Tax Office&quot;);
			var sCustIdOut = Inputs.GetProperty(&quot;CUSTOMER CODE&quot;);
			var sPlatform = Inputs.GetProperty(&quot;Platform&quot;);
			var sCustTypeOut = Inputs.GetProperty(&quot;RECORD_FLAG&quot;);
			var sDesc = Inputs.GetProperty(&quot;Description Input&quot;);
			var sMSISDN = Inputs.GetProperty(&quot;MSISDN Input&quot;);
			var strCustType = Inputs.GetProperty(&quot;Market Class&quot;);
			if(BGTransaction != &quot;&quot;)
				strCustType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ACCOUNT_TYPE&quot;,strCustType);
			// VFTK Junaid 20120328 - Added for the defect C_MSP00000808
			if(strCustType == &quot;&quot;)
			{
				var sCust = TheApplication().GetProfileAttr(&quot;VFCorporate&quot;);
				if(sCust == &quot;Y&quot;)
					strCustType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ACCOUNT_TYPE&quot;,&quot;101&quot;);
				else
					strCustType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ACCOUNT_TYPE&quot;,&quot;102&quot;);
			}
			//End
			var strConsumerLook = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ACCOUNT_TYPE&quot;,&quot;102&quot;);
			var strCorp = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ACCOUNT_TYPE&quot;,&quot;101&quot;);
			var strTypeLookup= TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;CONTACT_TYPE&quot;,&quot;End-user&quot;);
			var strTypeLOwner= TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;CONTACT_TYPE&quot;,&quot;Contract Owner&quot;);
			var strConRep = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;CONTACT_TYPE&quot;,&quot;Contact Representative&quot;);
			var strType =Inputs.GetProperty(&quot;Type&quot;);
			if(BGTransaction != &quot;&quot;)
				strType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;CONTACT_TYPE&quot;,strType);
			var sPrim = &quot;&quot;;
			var sInqSubType = &quot;&quot;;
			if(((strCustType==strConsumerLook)&amp;&amp;((strType==strTypeLookup)||(strType==strTypeLOwner)))||((strCustType==strCorp)&amp;&amp;(strType==strConRep)))
			{
				sPrim = &quot;Y&quot;;
	  		}
	  		else //VFTK Diva 20120405: Added for C_MSP00000939
			{
				strType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;CONTACT_TYPE&quot;, strType);
				if(((strCustType == strConsumerLook) &amp;&amp; ((strType == strTypeLookup) || (strType == strTypeLOwner))) || ((strCustType == strCorp) &amp;&amp; (strType == strConRep)))
				{
					sPrim = &quot;Y&quot;;
	  			}
	  			else if(((strCustType == &quot;Consumer&quot;) &amp;&amp; ((strType == strTypeLookup) || (strType == strTypeLOwner))) || ((strCustType == &quot;Business&quot;) &amp;&amp; (strType == strConRep)))
	  			{
	  				sPrim = &quot;Y&quot;;
	  			}
			} //VFTK Diva 20120405: Added for C_MSP00000939 - End
	  		if((strMode == &quot;Add&quot;) ||(strMode == &quot;Remove&quot;) || (strMode == &quot;Ekleme&quot;) || (strMode == &quot;Çıkarma&quot;))//C_MSP00000619
			{
				sPrim = &quot;Y&quot;;
	  		}
	  		
			if(BGTransaction == &quot;Y&quot; || sPrim == &quot;Y&quot;)
			{
				bcEmployee.ClearToQuery();
		        bcEmployee.SetViewMode(AllView);
		        bcEmployee.ActivateField(&quot;VF Shop Id&quot;);
		        bcEmployee.SetSearchSpec(&quot;Login Name&quot;,strLogin);
		        bcEmployee.ExecuteQuery(ForwardOnly);
		        if(bcEmployee.FirstRecord())
		        {
				 	strVshopCode = bcEmployee.GetFieldValue(&quot;VF Shop Id&quot;);
				}
				bcLogging.NewRecord(NewAfter);
				bcLogging.SetFieldValue(&quot;VF Billing Account ID&quot;,strBACode);
				bcLogging.SetFieldValue(&quot;VF CC BL Reason&quot;,strCCReason);
				bcLogging.SetFieldValue(&quot;VF CCB Customer ID&quot;,strCCBId);
				bcLogging.SetFieldValue(&quot;VF Channel&quot;,strChannel);
				bcLogging.SetFieldValue(&quot;VF Birth Date&quot;,strDBInput);
				bcLogging.SetFieldValue(&quot;VF Father Name&quot;,strFatherName);
				bcLogging.SetFieldValue(&quot;VF Firm Name&quot;,strFirm);
				bcLogging.SetFieldValue(&quot;VF First Name&quot;,strFNIn);
				bcLogging.SetFieldValue(&quot;VF First Name Out&quot;,strFNOut);
				bcLogging.SetFieldValue(&quot;VF Gender&quot;,strGenIn);
				bcLogging.SetFieldValue(&quot;VF Inquiry&quot;,strTime);
				bcLogging.SetFieldValue(&quot;VF Inquiry Result&quot;,strInRes);
				bcLogging.SetFieldValue(&quot;VF Inquiry Scenario&quot;,strMode);
				bcLogging.SetFieldValue(&quot;VF Inquiry Type&quot;,strInquirytype);
				bcLogging.SetFieldValue(&quot;VF Mother Name&quot;,strMNIn);
				bcLogging.SetFieldValue(&quot;VF Last Name&quot;,strLNIn);
				bcLogging.SetFieldValue(&quot;VF Last Name Out&quot;,strLNOut);
				bcLogging.SetFieldValue(&quot;VF Order ID&quot;,strOrderId);
				bcLogging.SetFieldValue(&quot;VF Place of Birth&quot;,strPlace);
				bcLogging.SetFieldValue(&quot;VF Siebel Customer ID&quot;,strCustId);
				bcLogging.SetFieldValue(&quot;VF TC ID&quot;,strTCIDIn);
				bcLogging.SetFieldValue(&quot;VF TC ID Out&quot;,strTCIDOut);
				bcLogging.SetFieldValue(&quot;VF Tax ID&quot;,strTaxID);
				bcLogging.SetFieldValue(&quot;VF Tax Office&quot;,strTaxOff);
				bcLogging.SetFieldValue(&quot;VF Contact ID&quot;,strPrimary);
				bcLogging.SetFieldValue(&quot;VF Transaction View&quot;,strTranView);
				
				var strbatch = Inputs.GetProperty(&quot;MernisBatch&quot;);
				
				if(inpChannel!=null &amp;&amp; inpChannel!=&quot;&quot;)
				{
					bcLogging.SetFieldValue(&quot;VF User ID&quot;,inpUserId);
					bcLogging.SetFieldValue(&quot;VF Inquiry Sub Type&quot;,&quot;Process&quot;);
				}
				
				else if(strbatch==&quot;Y&quot;)
				{
					bcLogging.SetFieldValue(&quot;VF User ID&quot;,&quot;SADMIN&quot;);
					bcLogging.SetFieldValue(&quot;VF Inquiry Sub Type&quot;,&quot;Process&quot;);
				}
				else
				{
					bcLogging.SetFieldValue(&quot;VF User ID&quot;,strLogin);
					bcLogging.SetFieldValue(&quot;VF Inquiry Sub Type&quot;,&quot;Manual&quot;);
				}
				bcLogging.SetFieldValue(&quot;VF VShop Code&quot;,strVshopCode);
				bcLogging.SetFieldValue(&quot;VF Father Name Out&quot;,strFatherOut);
				bcLogging.SetFieldValue(&quot;VF Firm Name Out&quot;,strFirmOut);
				bcLogging.SetFieldValue(&quot;VF Birth Place Out&quot;,strBirthPlaceOut);
				bcLogging.SetFieldValue(&quot;VF Date of Birth Out&quot;,strBirthDateOut);
				bcLogging.SetFieldValue(&quot;VF Gender Out&quot;,strGenderOut);
				bcLogging.SetFieldValue(&quot;VF Mother Name Out&quot;,strMotherOut);
				bcLogging.SetFieldValue(&quot;VF Description&quot;,sDesc);
				bcLogging.SetFieldValue(&quot;VF MSISDN&quot;,sMSISDN);
				bcLogging.SetFieldValue(&quot;VF Start Date&quot;,sStartDate);
				bcLogging.SetFieldValue(&quot;VF Siebel Customer ID Out&quot;,sCustIdOut);
				bcLogging.SetFieldValue(&quot;VF Tax ID Out&quot;,sTaxOut);
				bcLogging.SetFieldValue(&quot;VF Tax Office Out&quot;,sTaxOffOut);
				bcLogging.SetFieldValue(&quot;VF Platform Out&quot;,sPlatform);
				bcLogging.SetFieldValue(&quot;VFTRCBU Client IP Address&quot;, remHost);//FGUEDES
				if(sCustTypeOut == &quot;A&quot;)
					sCustTypeOut = &quot;Person&quot;;
				if(sCustTypeOut == &quot;C&quot;)
					sCustTypeOut = &quot;Company&quot;;
				bcLogging.SetFieldValue(&quot;VF Customer Type Out&quot;,sCustTypeOut);
				bcLogging.WriteRecord();
				Outputs.SetProperty(&quot;Result&quot;,&quot;OK&quot;);
			}//Logging only for Consumer Legal Owner/End User Customers	OR Corporate&apos;s Contact Representative
		}//Based on Parameteric LOV logging is called
	}     
	catch(e)
   	{
		Outputs.SetProperty(&quot;Result&quot;,&quot;KO&quot;);
     	throw(e);
   	}
   	finally
   	{
   		bcEmployee = null;
		bcContact = null;
		bcAccount = null;
		bcLogging = null;
		boAccount = null;
		Outputs.SetProperty(&quot;MernisBatch&quot;,&quot;&quot;);
   	}
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="Service_PreInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function Service_PreInvokeMethod (MethodName, Inputs, Outputs)
{
	if (MethodName == &quot;LoggingGIB&quot;)
	{
		LoggingGIB(Inputs, Outputs);
		return (CancelOperation);
	}
	if (MethodName ==&quot;LoggingMernis&quot;)//Added as part of WP05 R2.1 by Mamta
	{
		LoggingMernis(Inputs, Outputs);
		return (CancelOperation);
	}
	if (MethodName ==&quot;LoggingBGInquiry&quot;)//Added as part of WP01 R2.1 by Junaid
	{
		LoggingBGInquiry(Inputs, Outputs);
		return (CancelOperation);
	}
	return (ContinueOperation);
}"

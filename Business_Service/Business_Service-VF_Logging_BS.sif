			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="LoggingBGInquiry"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function LoggingBGInquiry(Inputs, Outputs)
{
   /***********************************************************************************
	* VFTK Junaid 13-Dec-11 : Created                                                 
	* Description      : This script takes its input from three interfaces-Cep Aran 
	*                    Blacklist,Black/Grey List and Credit Card Blacklist inquiry 
	***********************************************************************************/
	try
	{
		var strMernisLog = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_MERNIS_LOG_ABLE&quot;,&quot;Y&quot;);
		if(strMernisLog==&quot;Y&quot;)
		{
			var sCount = Inputs.GetChild(0).GetChild(0).GetChildCount();
			var sInpProp : PropertySet = TheApplication().NewPropertySet();
			var i = &quot;&quot;;
			var sRecFlg = &quot;&quot;;
			var sTyp = &quot;&quot;;
			for(i=0;i&lt;sCount;i++)
			{
				sInpProp = Inputs.GetChild(0).GetChild(0).GetChild(i);
				sTyp = Inputs.GetChild(0).GetChild(0).GetChild(i).GetType();
				if(sTyp == &quot;VF BlackList IC&quot;)
					sRecFlg = sInpProp.GetProperty(&quot;Record Flag&quot;);
				sInpProp.SetProperty(&quot;Inquiry Scenario&quot;,Inputs.GetProperty(&quot;Inquiry Scenario&quot;));
				sInpProp.SetProperty(&quot;Inquiry Type&quot;,Inputs.GetProperty(&quot;Inquiry Type&quot;));
				sInpProp.SetProperty(&quot;Transaction View Name&quot;,Inputs.GetProperty(&quot;Transaction View Name&quot;));
				sInpProp.SetProperty(&quot;CC_BL_Reason&quot;,Inputs.GetProperty(&quot;CC_BL_Reason&quot;));
				sInpProp.SetProperty(&quot;Birthdate Input&quot;,Inputs.GetProperty(&quot;Birthdate Input&quot;));
				sInpProp.SetProperty(&quot;Father Name Input&quot;,Inputs.GetProperty(&quot;Father Name Input&quot;));
				sInpProp.SetProperty(&quot;Firm Name Input&quot;,Inputs.GetProperty(&quot;Firm Name Input&quot;));
				sInpProp.SetProperty(&quot;First Name Input&quot;,Inputs.GetProperty(&quot;First Name Input&quot;));
				sInpProp.SetProperty(&quot;Gender Input&quot;,Inputs.GetProperty(&quot;Gender Input&quot;));
				sInpProp.SetProperty(&quot;Inquiry Result&quot;,Inputs.GetProperty(&quot;Inquiry Result&quot;));
				sInpProp.SetProperty(&quot;Surname Input&quot;,Inputs.GetProperty(&quot;Surname Input&quot;));
				sInpProp.SetProperty(&quot;Mother Name Input&quot;,Inputs.GetProperty(&quot;Mother Name Input&quot;));
				sInpProp.SetProperty(&quot;Order ID&quot;,Inputs.GetProperty(&quot;Order ID&quot;));
				sInpProp.SetProperty(&quot;BirthPlace Input&quot;,Inputs.GetProperty(&quot;BirthPlace Input&quot;));
				sInpProp.SetProperty(&quot;AccountId&quot;,Inputs.GetProperty(&quot;AccountId&quot;));
				sInpProp.SetProperty(&quot;TCIDInput&quot;,Inputs.GetProperty(&quot;TCIDInput&quot;));
				sInpProp.SetProperty(&quot;TaxID Input&quot;,Inputs.GetProperty(&quot;TaxID Input&quot;));
				sInpProp.SetProperty(&quot;TaxOffice Input&quot;,Inputs.GetProperty(&quot;TaxOffice Input&quot;));
				sInpProp.SetProperty(&quot;Primary Contact Id&quot;,Inputs.GetProperty(&quot;Primary Contact Id&quot;));
				sInpProp.SetProperty(&quot;Time Stamp&quot;,Inputs.GetProperty(&quot;Time Stamp&quot;));
				sInpProp.SetProperty(&quot;BGTransaction&quot;,Inputs.GetProperty(&quot;BGTransaction&quot;));
				sInpProp.SetProperty(&quot;Market Class&quot;,Inputs.GetProperty(&quot;Market Class&quot;));
				sInpProp.SetProperty(&quot;Type&quot;,Inputs.GetProperty(&quot;Type&quot;));
				sInpProp.SetProperty(&quot;Description Input&quot;,Inputs.GetProperty(&quot;Description Input&quot;));
				sInpProp.SetProperty(&quot;MSISDN Input&quot;,Inputs.GetProperty(&quot;MSISDN Input&quot;));
				sInpProp.SetProperty(&quot;Billing Account ID&quot;,Inputs.GetProperty(&quot;Billing Account ID&quot;));
				sInpProp.SetProperty(&quot;CCB Customer ID&quot;,Inputs.GetProperty(&quot;CCB Customer ID&quot;));
				sInpProp.SetProperty(&quot;Start Date Input&quot;,Inputs.GetProperty(&quot;Start Date Input&quot;));
				sInpProp.SetProperty(&quot;RECORD_FLAG&quot;,sRecFlg);
				LoggingMernis(sInpProp,Outputs);
			}
		}
	}
	finally
	{
		sInpProp = null;
	}
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetTransactionViewName"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetTransactionViewName()
{
	//Vaibhav 02/04/2012: Added the function - C_MSP00000883
	var lsActiveView 	= TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
	var lsTranViewName 	= &quot;&quot;;
	var lsTranViewLIC 	= &quot;&quot;;
	
	switch (lsActiveView)
	{
		case &quot;VF Account List View&quot;:
			lsTranViewLIC = &quot;Account list View&quot;;
			break;
			
		case &quot;VF BlackList Transaction View&quot;:
			lsTranViewLIC = &quot;Black List Transaction View&quot;;
			break;				
			
		case &quot;VF B/G List View&quot;:
			lsTranViewLIC = &quot;B/G List View&quot;;
			break;
							
		case &quot;VF Cep Aran Black List View&quot;:
			lsTranViewLIC = &quot;Cep Aran Black List View&quot;;
			break;
							
		case &quot;VF Credit Card Black List View&quot;:
			lsTranViewLIC = &quot;Credit Card Black List View&quot;;
			break;
	}
	
	if(lsTranViewLIC != &quot;&quot;)
		lsTranViewName = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_TRANS_VIEW&quot;, lsTranViewLIC);

	return(lsTranViewName);
}"

			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="LoggingGIB"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function LoggingGIB(Inputs,Outputs)
{
   try
   {
	   var app = TheApplication();
	   var boAccount : BusObject = app.GetBusObject(&quot;Account&quot;);
	   var bcLogging : BusComp = boAccount.GetBusComp(&quot;VF GIB Logging&quot;);
	   var bcAccount : BusComp = boAccount.GetBusComp(&quot;Account&quot;);
	   var strLoggingId = Inputs.GetProperty(&quot;Logging Id&quot;);
	   var strOrderId = Inputs.GetProperty(&quot;Order Id&quot;);
	   var strCustCode = Inputs.GetProperty(&quot;Customer Code&quot;);
	   var strModeInput  = Inputs.GetProperty(&quot;Profile Mode&quot;);
	   var strMode   = app.InvokeMethod(&quot;LookupValue&quot;,&quot;GIB_SCENARIO&quot;,strModeInput);
	   var boParameters : BusObject = app.GetBusObject (&quot;ACS NI Parameter&quot;);
	   var bcParameters : BusComp = boParameters.GetBusComp (&quot;ACS NI Parameter&quot;);
	   var strConstraint1   = &quot;&quot;;
	   var strConstraint2   = &quot;&quot;;
	   var strConstraint3   = &quot;&quot;;
	   var strConstraint4   = &quot;&quot;;
	   var strConstraint5   = &quot;&quot;;
       var strConstraint6	= &quot;&quot;;
       var bcParametersExist = &quot;&quot;;
	   var strParameterSrcExpr       = &quot;&quot;;
       strParameterSrcExpr = &quot;[Function] = &apos;VFGIBLogging&apos; AND [Value] = &apos;&quot;+strLoggingId+&quot;&apos;&quot;;;
       
       var inProp 	:PropertySet	= TheApplication().NewPropertySet();
	   var outProp : PropertySet	= TheApplication().NewPropertySet();
	   var BS 		:Service		= TheApplication().GetService(&quot;VF TRK Utilities&quot;);
	   inProp.SetProperty(&quot;strCustCode&quot;,strCustCode); //Id Validation
	   BS.InvokeMethod(&quot;Input Validation&quot;,inProp, outProp); // Query Input Validation
		
       bcAccount.ClearToQuery();
       bcAccount.SetViewMode(AllView);
       bcAccount.ActivateField(&quot;VF GIB Status Code&quot;);
       bcAccount.ActivateField(&quot;VF GIB Last Ver Date&quot;);
       bcAccount.ActivateField(&quot;VF GIB Ver Status&quot;);
       bcAccount.SetSearchSpec(&quot;Id&quot;,strCustCode);
       bcAccount.ExecuteQuery(ForwardOnly);
       if(bcAccount.FirstRecord())
       {
       		var strStatCode = bcAccount.GetFieldValue(&quot;VF GIB Status Code&quot;);
       		var strLastTransDate = bcAccount.GetFieldValue(&quot;VF GIB Last Ver Date&quot;);
       		var strGIBVerStatus = bcAccount.GetFieldValue(&quot;VF GIB Ver Status&quot;);
       		bcParameters.ClearToQuery ();
			bcParameters.SetViewMode (AllView);
			bcParameters.SetSearchExpr (strParameterSrcExpr);
			bcParameters.ExecuteQuery (ForwardOnly);
			bcParametersExist = bcParameters.FirstRecord ();
			if(bcParametersExist)
			{
				strConstraint1      = bcParameters.GetFieldValue (&quot;Condition1&quot;);
				strConstraint2      = bcParameters.GetFieldValue (&quot;Comments&quot;);
				strConstraint3      = bcParameters.GetFieldValue (&quot;Condition3&quot;);
				strConstraint4      = bcParameters.GetFieldValue (&quot;Condition4&quot;);
				if (strConstraint1 == &quot;Same&quot; || strConstraint2 == &quot;Same&quot; || strConstraint3 == &quot;Same&quot;){
					var psOutput = app.NewPropertySet();
					CheckSameGIB(strCustCode, strConstraint1, strConstraint2, strConstraint3, psOutput);
					strConstraint1 = psOutput.GetProperty(&quot;strConstraint1&quot;);
					strConstraint2 = psOutput.GetProperty(&quot;strConstraint2&quot;);
					strConstraint3 = psOutput.GetProperty(&quot;strConstraint3&quot;);	
				}
				bcLogging.NewRecord(NewAfter);
				bcLogging.SetFieldValue(&quot;VF Active/Passive Flag&quot;,strConstraint4);
				bcLogging.SetFieldValue(&quot;VF Customer unique identifier&quot;,strCustCode);
				bcLogging.SetFieldValue(&quot;VF GIB Result&quot;,strConstraint1);
				bcLogging.SetFieldValue(&quot;VF Last Result&quot;,strConstraint3);
				bcLogging.SetFieldValue(&quot;VF Last Transaction Date&quot;,strLastTransDate);
				bcLogging.SetFieldValue(&quot;VF Order Id&quot;,strOrderId);
				bcLogging.SetFieldValue(&quot;VF Logging Id&quot;,strLoggingId);
				bcLogging.SetFieldValue(&quot;VF Scenario&quot;,strMode);
				bcLogging.SetFieldValue(&quot;VF Tax_id No Verification Status&quot;,strGIBVerStatus);
				bcLogging.SetFieldValue(&quot;VF Transaction Reason&quot;,strConstraint2);
				bcLogging.WriteRecord();
				bcParametersExist = bcParameters.NextRecord();
				Outputs.SetProperty(&quot;Result&quot;,&quot;OK&quot;);
			}
       else
       {
       			inProp.SetProperty(&quot;strLoggingId&quot;,strLoggingId); //Id Validation
				BS.InvokeMethod(&quot;Input Validation&quot;,inProp, outProp); // Query Input Validation      
   		}
   	}
   }
   catch(e)
   {
	 Outputs.SetProperty(&quot;Result&quot;,&quot;KO&quot;);
     throw(e);
   }
   finally
   {
    	bcLogging = null;
	    bcAccount = null;
	    bcParameters = null;
	    boAccount = null;
	    boParameters = null;
	    psOutput = null;
		inProp 	= null;
		outProp	 = null;
		BS			 = null;
		app = null;
	}
}"

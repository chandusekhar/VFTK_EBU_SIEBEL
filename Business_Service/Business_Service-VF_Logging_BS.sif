			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CheckSameGIB"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CheckSameGIB(strCustCode,strConstraint1,strConstraint2,strConstraint3,psOutput)
{
   try
   {
		var boAccount : BusObject = TheApplication().GetBusObject(&quot;Account&quot;);
		var bcLogging : BusComp = boAccount.GetBusComp(&quot;VF GIB Logging&quot;);
 		
 		var inProp :PropertySet     = TheApplication().NewPropertySet();
		var outProp :PropertySet    = TheApplication().NewPropertySet();
		var BS :Service 			= TheApplication().GetService(&quot;VF TRK Utilities&quot;);
		inProp.SetProperty(&quot;strCustCode&quot;, strCustCode); 
		BS.InvokeMethod(&quot;Input Validation&quot;,inProp, outProp); 
		
 		bcLogging.ClearToQuery ();
		bcLogging.SetViewMode (AllView);
		bcLogging.ActivateField(&quot;VF GIB Result&quot;);
		bcLogging.ActivateField(&quot;VF Transaction Reason&quot;);
		bcLogging.ActivateField(&quot;VF Last Result&quot;);
		bcLogging.SetSearchSpec(&quot;VF Customer unique identifier&quot;,strCustCode);
		bcLogging.SetSortSpec( &quot;Created(DESCENDING)&quot;);
		bcLogging.ExecuteQuery (ForwardOnly);
		var bcLoggingExist = bcLogging.FirstRecord ();
		while(bcLoggingExist &amp; (strConstraint1 == &quot;Same&quot; || strConstraint2 == &quot;Same&quot; || strConstraint3 == &quot;Same&quot;))
		{
			if(strConstraint1 == &quot;Same&quot;) strConstraint1 = bcLogging.GetFieldValue(&quot;VF GIB Result&quot;);
			if(strConstraint2 == &quot;Same&quot;) strConstraint2 = bcLogging.GetFieldValue(&quot;VF Transaction Reason&quot;);	
			if(strConstraint3 == &quot;Same&quot;) strConstraint3 = bcLogging.GetFieldValue(&quot;VF Last Result&quot;);	
			bcLoggingExist = bcLogging.NextRecord();
		}
		if(strConstraint1 == &quot;Same&quot;) strConstraint1 = &quot;&quot;;
		if(strConstraint2 == &quot;Same&quot;) strConstraint2 = &quot;&quot;;
		if(strConstraint3 == &quot;Same&quot;) strConstraint3 = &quot;&quot;;
		psOutput.SetProperty(&quot;strConstraint1&quot;,strConstraint1);
		psOutput.SetProperty(&quot;strConstraint2&quot;,strConstraint2);
		psOutput.SetProperty(&quot;strConstraint3&quot;,strConstraint3);         
   }
   catch(e)
   {
		psOutput.SetProperty(&quot;strConstraint1&quot;,strConstraint1);
		psOutput.SetProperty(&quot;strConstraint2&quot;,strConstraint2);
		psOutput.SetProperty(&quot;strConstraint3&quot;,strConstraint3);
     throw(e);
   }
   finally
   {
   	bcLogging = null;
   	boAccount = null;
   	inProp = null;
	outProp = null;
	BS = null;
   }
}"

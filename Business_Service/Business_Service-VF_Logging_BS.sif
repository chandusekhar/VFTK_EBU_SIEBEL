			<BUSINESS_SERVICE_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GetRemoteHost"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GetRemoteHost()
{
	try
	{
		var bsServ : Service = TheApplication().GetService(&quot;Web Engine HTTP TXN&quot;);
		var psServIn : PropertySet = TheApplication().NewPropertySet();
		var psServOut : PropertySet = TheApplication().NewPropertySet();
		
		bsServ.InvokeMethod(&quot;GetAllServerVariables&quot;, psServIn, psServOut);
		
		return(psServOut.GetProperty(&quot;REMOTE_HOST&quot;));
	}     
	catch(e)
   	{
     	throw(e);
   	}
   	finally
   	{
   		psServOut = null;
		psServIn = null;
		bsServ = null;
   	}
}"

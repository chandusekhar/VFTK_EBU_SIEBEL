			<APPLET_SERVER_SCRIPT
			   COMMENTS="Inactivated for OR"
			   INACTIVE="N"
			   NAME="OldestInvoiceAgeValidation"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function OldestInvoiceAgeValidation()
{
//*****************************************************
//** Function: OldestInvoiceAgeValidation
//** Purpose:  
//** 1) we need to take the oldest invioce due date
//** 2) sysdate minus oldest invoice due date ---- A
//** 3) A &gt;= AGE_INVOICE
//** Inputs :  None.
//** Outputs:  None.
//** Returns:  None.
//*****************************************************
//** Revision History:
//** 01 - 05/13/2011 - Parasuram Mainam
//**    R142 - WP 15 Billing Inquiry - Created New.
//*****************************************************
try
{
	var bcIP=this.BusComp();
	var bTraverse=false;
	var oApp=TheApplication();
	bTraverse=bcIP.LastRecord();
	if(bTraverse)
	{
		var iAgeInDays=oApp.InvokeMethod(&quot;LookupValue&quot;,&quot;AGE_INVOICE&quot;,&quot;Age of the Invoice&quot;);
		var strInvoiceDate=bcIP.GetFieldValue(&quot;INVOICE_DUE_DATE&quot;);
		var iDays=bcIP.GetFieldValue(&quot;VF Diff Days&quot;);
		TheApplication().Trace(this.Name() + &quot; :: OldestInvoiceAge :: iAgeInDays =&quot;+iAgeInDays+&quot; strInvoiceDate=&quot;+strInvoiceDate+&quot;  iDays=&quot;+iDays);
		if(ToNumber(iDays)&gt;=ToNumber(iAgeInDays))
		{
			TheApplication().Trace(this.Name() + &quot; :: OldestInvoiceAge :: Before RaiseerrorText&quot;);
			oApp.RaiseErrorText(oApp.LookupMessage(&quot;VF Message Category&quot;, &quot;292&quot;));
		}
	}
}
catch(e)
{
	throw e;
}
finally
{
	oApp=null;
	bcIP = null;
}
}"

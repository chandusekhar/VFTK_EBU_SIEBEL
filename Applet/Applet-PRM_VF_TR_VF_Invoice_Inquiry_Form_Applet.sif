			<APPLET_SERVER_SCRIPT
			   COMMENTS="Inactivated for OR"
			   INACTIVE="N"
			   NAME="ThresholdValidations"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function ThresholdValidations()
{

	try
	{
		var bcIP : BusComp =this.BusComp();
		if(bcIP!=null)
		{
			var bTraverse=bcIP.FirstRecord();
			var bAtleastOneBig=false;
			var bBelowInvoiceLevel=false;
			var oApp : Application;
			oApp=TheApplication();
			oApp.SetProfileAttr(&quot;IPThresholdCheck&quot;, &quot;Y&quot;);
			var strInvType=bcIP.GetFieldValue(&quot;INVOICE_TYPE_DESC&quot;);
			var strInvPayStatus=bcIP.GetFieldValue(&quot;PAYMENT_STATUS_DESC&quot;);

			while(bTraverse &amp;&amp; strInvType!=&quot;&quot; &amp;&amp; strInvPayStatus!=&quot;&quot;)
			{

				var iInvAmt=bcIP.GetFieldValue(&quot;INVOICE_DUE_AMOUNT&quot;);
				strInvType=bcIP.GetFieldValue(&quot;INVOICE_TYPE_DESC&quot;);
				strInvPayStatus=bcIP.GetFieldValue(&quot;PAYMENT_STATUS_DESC&quot;);

				var iThresholdMinVal=oApp.InvokeMethod(&quot;LookupValue&quot;,&quot;THRESHOLD_MIN_INVOICE&quot;,&quot;Threshold Min Invoice&quot;);
				var iThresholdInvoice=oApp.InvokeMethod(&quot;LookupValue&quot;,&quot;THRESHOLD_INVOICE&quot;,&quot;Threshold Invoice&quot;);
				var sPayStatus=oApp.InvokeMethod(&quot;LookupValue&quot;,&quot;VF_PAYMENT_STATUS_DESC&quot;,&quot;Unpaid&quot;);
				var sInvType=oApp.InvokeMethod(&quot;LookupValue&quot;,&quot;VF_INVOICE_TYPE_DESC&quot;,&quot;Standard&quot;);

				if(strInvType==sInvType &amp;&amp; strInvPayStatus==sPayStatus)
				{

					if(ToNumber(iInvAmt)&gt;ToNumber(iThresholdMinVal))
					{
						bAtleastOneBig=true;
					}

					if(ToNumber(iInvAmt)&lt;ToNumber(iThresholdInvoice))
					{
						bBelowInvoiceLevel=true;
						break;
					}
					bTraverse=bcIP.NextRecord();
				}
			}//while
			if((!bAtleastOneBig) || (bBelowInvoiceLevel))
			{
				oApp.RaiseErrorText(oApp.LookupMessage(&quot;VF Message Category&quot;, &quot;292&quot;));
			}
	   }
	}
	catch(e)
	{
		throw e;
	}
	finally
	{
		bcIP = null;
		oApp=null;
	}
}"

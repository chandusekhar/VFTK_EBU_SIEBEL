			<APPLET_SERVER_SCRIPT
			   COMMENTS="Inactivated for OR"
			   INACTIVE="N"
			   NAME="WebApplet_PreCanInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_PreCanInvokeMethod (MethodName, &amp;CanInvoke)
{
	//Oracle Review October 2011 - Deep Code
	try
	{
		var psInputs: PropertySet;
		var psOutputs: PropertySet;
		var bsGeneralOperations: Service;
		if(MethodName == &quot;OutbalInquiry&quot; || MethodName == &quot;PayInstalments&quot;)
		{
		  var activeView=TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
		  var isReadOnly=TheApplication().InvokeMethod(&quot;IsViewReadOnly&quot;, activeView);
		  if (isReadOnly != &quot;TRUE&quot;)
		  { 
			psInputs=TheApplication().NewPropertySet();
			psOutputs=TheApplication().NewPropertySet();
			bsGeneralOperations = TheApplication().GetService (&quot;ACS General Operations&quot;);
			psInputs.SetProperty (&quot;Function&quot;, MethodName);
			psInputs.SetProperty (&quot;Name&quot;, MethodName);
			bsGeneralOperations.InvokeMethod (&quot;GetParameter_3Inputs&quot;, psInputs, psOutputs);
		    var buttonVisibility = psOutputs.GetProperty (&quot;Cons1&quot;);		     
			if(buttonVisibility.toUpperCase()==&quot;FALSE&quot;)
			{
			   	CanInvoke=&quot;FALSE&quot;;
	        }
			else
			{
			   	CanInvoke=&quot;TRUE&quot;;
		    }
		  }
		  else
		  {
				CanInvoke=&quot;FALSE&quot;;
		  }
		  return(CancelOperation);
		}//if MethodName
		if (MethodName == &quot;InvoiceInquiry&quot;)
		{
			psInputs = TheApplication().NewPropertySet();
			psOutputs = TheApplication().NewPropertySet();
			bsGeneralOperations = TheApplication().GetService(&quot;ACS General Operations&quot;);
			psInputs.SetProperty(&quot;Function&quot;,&quot;PRMInvoiceInquiry&quot;);
			psInputs.SetProperty(&quot;Name&quot;,&quot;Invoice Inquiry&quot;);
			bsGeneralOperations.InvokeMethod(&quot;GetParameter_3Inputs&quot;,psInputs,psOutputs);
			var buttonVisibility = psOutputs.GetProperty(&quot;Cons1&quot;);
			if(buttonVisibility==&quot;True&quot;)
			{
				CanInvoke = &quot;TRUE&quot;;
			}
			else
			{
				CanInvoke = &quot;FALSE&quot;;				
			}
			return (CancelOperation);	
		}
			return (ContinueOperation);
	}
	catch(e)
	{
		throw(e);
	}	
	finally{
	 psOutputs = null;
	 psInputs = null;
	 bsGeneralOperations = null;
	}
}"

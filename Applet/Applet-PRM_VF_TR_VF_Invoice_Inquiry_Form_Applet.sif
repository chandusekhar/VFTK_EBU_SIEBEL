			<APPLET_SERVER_SCRIPT
			   COMMENTS="Inactivated for OR"
			   INACTIVE="N"
			   NAME="InitDateAttributes"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function InitDateAttributes()
{
//*****************************************************
//** Function: Initializes date controls.
//** Purpose:  
//** Inputs :  None.
//** Outputs:  None.
//** Returns:  None.
//*****************************************************
//** Revision History:
//** 01 - 05/17/2011 - Parasuram Mainam
//**    R142 - WP 15 Billing Inquiry - Created New.
//*****************************************************
 try
 {
		var dateVar = new Date();
		
		var sLastNMonths = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_INVOICE_LAST_MONTHS&quot;,&quot;VF_INVOICE_LAST_MONTHS&quot;);
		
		dateVar.setMonth(dateVar.getMonth() - ToNumber(sLastNMonths));
		var strStart=(dateVar.getMonth()+1) + &apos;/&apos; + dateVar.getDate() +&apos;/&apos; + dateVar.getFullYear();

		var dtToday = new Date();
		var strEnd=(dtToday.getMonth()+1) + &apos;/&apos; + dtToday.getDate() +&apos;/&apos; + dtToday.getFullYear();
		TheApplication().SetProfileAttr(&quot;IPStartDate&quot;,strStart);
		TheApplication().SetProfileAttr(&quot;IPEndDate&quot;,strEnd); 
		TheApplication().SetProfileAttr(&quot;InvoiceFromLoad&quot;,&quot;Y&quot;); 
		TheApplication().Trace(this.Name() + &quot; :: InitDate :: Start Date = &quot;+ strStart + &quot;  :: End Date = &quot;+ strEnd);
 }
 catch(e)
 {
 	throw e;
 }
}"

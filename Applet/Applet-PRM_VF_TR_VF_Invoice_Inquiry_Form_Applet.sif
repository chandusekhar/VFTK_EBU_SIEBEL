	          var sOutBalLastUpd=psOutputs.GetProperty(&quot;sOutBalLastUpd&quot;);
	          var DivsOutBalance= &quot;&quot;;
	          if(sOutBal!=&quot;&quot;)
	          {
	        	this.BusComp().SetFieldValue(&quot;VF_TOT_OUTBAL&quot;,          sOutBal);
	        	this.BusComp().SetFieldValue(&quot;VF_TOT_OUTBAL_LAST_UPD&quot;, sOutBalLastUpd);
	        	this.BusComp().WriteRecord();
	        //	SetSplitBill(BAId);//VFTK Amul Added for CR317 - Masked
	          }
			  return(CancelOperation);
			  break;
			case &quot;PayInstalments&quot;:
				parentBus = this.BusComp().ParentBusComp();
				var grandParentBus : BusComp;
				var parentBusName=&quot;&quot;;
				var grandParentBusName=&quot;&quot;;
				
				if( parentBus != null)
				{
					parentBusName = parentBus.Name();
					if(parentBusName == &quot;Billing Account&quot;)
					{
						bcBA=parentBus;

						grandParentBus = parentBus.ParentBusComp();
						if( grandParentBus != null)
						{
							grandParentBusName = grandParentBus.Name();
							if(grandParentBusName == &quot;Account&quot; || 
								grandParentBusName == &quot;X Order Entry - Orders (Customer Portal)&quot; || 
								grandParentBusName == &quot;Order Entry - Orders&quot;)
							{
								bcAc=grandParentBus;					
							}
							else
							{
								return (CancelOperation);
							}	
						}
						else
						{
							return (CancelOperation);
						}						
					}
					else
					{
						return (CancelOperation);
					}	
				}
				else
				{
					return (CancelOperation);
				}				
							
				var strYTS=&quot;&quot;;
				var strCustClass=&quot;&quot;;
				var bErrorMsg=false;
				if(bcBA!=null)
				{
					// Check 1
					strYTS=bcBA.GetFieldValue(&quot;VF YTS Source&quot;);
					if(strYTS==&quot;Y&quot;)
					{
						oApp.RaiseErrorText(oApp.LookupMessage(&quot;VF Message Category&quot;, &quot;292&quot;));
					}
				}
				//Check 2
				strCustClass=bcAc.GetFieldValue(&quot;Market Type&quot;);
				CustomerClassEligible(strCustClass);
				//Check 3/4
				ThresholdValidations();
				OldestInvoiceAgeValidation();
				var strAcId=&quot;&quot;;
				if(bcAc!=null)
				{
					strAcId=bcAc.GetFieldValue(&quot;Account Id&quot;);
				}
	    	boAccount=oApp.GetBusObject(&quot;Account&quot;);
	   	 	bcAccount=boAccount.GetBusComp(&quot;Account&quot;);
	   	 	with(bcAccount)
	   	 	{
		   	 	ClearToQuery ();
					SetViewMode(AllView);
					SetSearchSpec(&quot;Id&quot;, strAcId);
					ExecuteQuery(ForwardOnly);
					if(FirstRecord())
					{
						if(oApp.GetProfileAttr(&quot;ApplicationName&quot;)==&quot;TeXCo Application&quot;)
						{
							//The following profile attribute is used in order to display a message informative
							TheApplication().SetProfileAttr(&quot;FromInvoiceView&quot;,&quot;Y&quot;);
							oApp.GotoView(&quot;VF Payment Installments View&quot;, boAccount);
						}
					}
				}
				break;
		}
		return(CancelOperation);
	}

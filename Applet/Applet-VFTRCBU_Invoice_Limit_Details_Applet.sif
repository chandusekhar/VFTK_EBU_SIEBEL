			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="WebApplet_PreCanInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_PreCanInvokeMethod (MethodName, &amp;CanInvoke)
{
	//VFTRCBU 29122014: JIRA VFTRCBU-4627 - FTOSCANO - START
	if (MethodName == &quot;VFTRCBURequest&quot;) 
	{
		try
		{
			var xInput = TheApplication().NewPropertySet();
		    var xOutput = TheApplication().NewPropertySet();
		    var BSGetRoles = TheApplication().GetService(&quot;ACS General Operations&quot;);
		    var strPosition;
		    var thisBC = this.BusComp();
			var strBillingAccountStatus = thisBC.ParentBusComp().GetFieldValue(&quot;Billing Account Status&quot;);
			var strBillingAccountType = thisBC.ParentBusComp().GetFieldValue(&quot;Billing Account Type&quot;);
			//var strSubscriberStatus = thisBC.ParentBusComp().GetFieldValue(&quot;VF Final Status&quot;); VFTRCBU-22944 - 20160109
			var strSubscriberStatusCalc = thisBC.ParentBusComp().GetFieldValue(&quot;VF Final Status Calc&quot;);
			
			var lovBillingAccountStatus = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;BILLING_ACCOUNT_STATUS&quot;,&quot;100&quot;);
			var lovBillingAccountType = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_PAYMENT_MODALITY&quot;,&quot;Postpaid&quot;);
			var lovSubscriberStatus = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_FINAL_STATUS&quot;,&quot;Active&quot;);
			
		   	xInput.SetProperty(&quot;Function&quot;, MethodName);
		    xInput.SetProperty(&quot;Name&quot;, MethodName);
		    xInput.SetProperty(&quot;Value&quot;, &quot;&quot;);
		
		    BSGetRoles.InvokeMethod(&quot;GetParameter_3Inputs&quot;, xInput, xOutput);
		    strPosition = xOutput.GetProperty(&quot;Cons1&quot;);
		    
		       
		    if (strPosition == &quot;True&quot;)
		    {
		    	if(strBillingAccountStatus == lovBillingAccountStatus &amp;&amp; strBillingAccountType == lovBillingAccountType &amp;&amp; strSubscriberStatusCalc == lovSubscriberStatus)
				{
					CanInvoke = &quot;TRUE&quot;;
				}
				else {
					CanInvoke = &quot;FALSE&quot;;
				}
			}
		    else
		    {
				CanInvoke = &quot;FALSE&quot;;
			}
			return (CancelOperation);
		}
		finally
		{
			BSGetRoles = null;
    		xInput = null;
    		xOutput = null;
		}
		
	}
	if (MethodName == &quot;VFTRCBUCancel&quot;) 
	{
		try
		{
			var xInput = TheApplication().NewPropertySet();
		    var xOutput = TheApplication().NewPropertySet();
		    var BSGetRoles = TheApplication().GetService(&quot;ACS General Operations&quot;);
		    var strPosition;
		    var thisBC = this.BusComp();
			var strAmount = thisBC.GetFieldValue(&quot;VFTRCBU Amount&quot;);
			//var strApplyTime = thisBC.GetFieldValue(&quot;VFTRCBU Apply Time&quot;); VFTRCBU-8254
			var strRequest = thisBC.GetFieldValue(&quot;VFTRCBU Requested Limit&quot;);
		
		   	xInput.SetProperty(&quot;Function&quot;, MethodName);
		    xInput.SetProperty(&quot;Name&quot;, MethodName);
		    xInput.SetProperty(&quot;Value&quot;, &quot;&quot;);
		
		    BSGetRoles.InvokeMethod(&quot;GetParameter_3Inputs&quot;, xInput, xOutput);
		    strPosition = xOutput.GetProperty(&quot;Cons1&quot;);
		
		    if (strPosition == &quot;True&quot;)
		    {
		    	if(strAmount != &quot;&quot; /*&amp;&amp; strApplyTime != &quot;&quot;*/ &amp;&amp; strRequest == strAmount) //VFTRCBU-8254
				{
					CanInvoke = &quot;TRUE&quot;;
				}
				else
				{
					CanInvoke = &quot;FALSE&quot;;
				}
			}
		    else
		    {
				CanInvoke = &quot;FALSE&quot;;
			}
			return (CancelOperation);
		}
		finally
		{
			BSGetRoles = null;
		    xInput = null;
		    xOutput = null;
		}
	}	
	//VFTRCBU 29122014: JIRA VFTRCBU-4627 - FTOSCANO - END
	return (ContinueOperation);
}"

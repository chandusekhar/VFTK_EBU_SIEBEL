			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="WebApplet_PreInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_PreInvokeMethod (MethodName)
{
	//VFTRCBU 29122014: JIRA VFTRCBU-4627 - FTOSCANO - START
	if(MethodName==&quot;VFTRCBURequest&quot;)
	{
		var thisBC = this.BusComp();
		var App = TheApplication();
		var strWFBS	= &quot;&quot;;
		var psCreateOut = &quot;&quot;;
		var psCreateIn	= &quot;&quot;;
		var errCode = &quot;&quot;;			
		var strRequestedLimit = thisBC.GetFieldValue(&quot;VFTRCBU Requested Limit&quot;);
		var strAmount = thisBC.GetFieldValue(&quot;VFTRCBU Amount&quot;);
		var strMSISDN = thisBC.ParentBusComp().GetFieldValue(&quot;VF MSISDN&quot;);
		var strMSISDNid = thisBC.ParentBusComp().GetFieldValue(&quot;Id&quot;);
		var strBillingAccount = thisBC.ParentBusComp().GetFieldValue(&quot;Billing Account&quot;);
		var strShop = TheApplication().GetProfileAttr(&quot;ShopID&quot;);
		var strQuoteType = thisBC.GetFieldValue(&quot;VFTRCBU Quota Type&quot;);
		var bsRefreshBC = App.GetService(&quot;SIS OM PMT Service&quot;);
		var strBO = &quot;Asset Management - Asset Item&quot;;
		var strBC = &quot;VFTRCBU Invoice Limit History VBC&quot;;
		var historyIn;
		var historyOut;
		var sNoLimit = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VFTRCBU_REQ_LIMIT_AMOUNT&quot;,&quot;No Limit&quot;); //PSANTOS VFTRCBU-31307

		if (strRequestedLimit == &quot;&quot;)
		{
			//Requested Limit cannot be empty.
			var strMessageCategory 	= &quot;VFTRCBU User Defined Errors&quot;;
			var strCode				= &quot;0153&quot;;
			var sMsg				= App.LookupMessage(strMessageCategory, strCode);
			App.RaiseErrorText(sMsg);
			return (CancelOperation);
		}
		else if (strRequestedLimit != &quot;&quot; &amp;&amp; strRequestedLimit == strAmount)
		{
			//Requested Limit should be different from Current Limit Amount.
			var strMessageCategory 	= &quot;VFTRCBU User Defined Errors&quot;;
			var strCode				= &quot;0154&quot;;
			var sMsg				= App.LookupMessage(strMessageCategory, strCode);
			App.RaiseErrorText(sMsg);
			return (CancelOperation);	
		}
		//PFONSECA - 20.07.2016 - Additional validation considering the amount
		else if (strRequestedLimit != &quot;&quot; &amp;&amp; strAmount == sNoLimit) 
		{
			//Activate
			psCreateIn = App.NewPropertySet();
			psCreateOut = App.NewPropertySet();
			strWFBS = App.GetService(&quot;Workflow Process Manager&quot;);
			psCreateIn.SetProperty(&quot;ProcessName&quot;,&quot;VFTRCBU Activate/Update Invoice Limit WF&quot;);
		//	psCreateIn.SetProperty(&quot;AccountNumber&quot;, strBillingAccount);//
			psCreateIn.SetProperty(&quot;MSISDN&quot;, strMSISDN);
			psCreateIn.SetProperty(&quot;Operation&quot;, &quot;0&quot;);
		//	psCreateIn.SetProperty(&quot;QuotaType&quot;, strQuoteType);
		//	psCreateIn.SetProperty(&quot;NotifyType&quot;, &quot;1&quot;);
		//	psCreateIn.SetProperty(&quot;ApplyTime&quot;, this.Timestamp());
		//	psCreateIn.SetProperty(&quot;ExpiryTime&quot;, this.Timestamp());
			psCreateIn.SetProperty(&quot;Amount&quot;, strRequestedLimit);
			psCreateIn.SetProperty(&quot;Shop&quot;, strShop);		
						
			strWFBS.InvokeMethod(&quot;RunProcess&quot;, psCreateIn, psCreateOut);
			
			errCode = psCreateOut.GetProperty(&quot;Error Code&quot;);
			if (errCode == &quot;0&quot;)
			{
				TheApplication().SetProfileAttr(&quot;VFSkipQueryWF&quot;,&quot;N&quot;);
				historyIn = App.NewPropertySet();
				historyOut = App.NewPropertySet();
				historyIn.SetProperty(&quot;Business Object Name&quot;,strBO);
				historyIn.SetProperty(&quot;Business Component Name&quot;,strBC);
				bsRefreshBC.InvokeMethod(&quot;Refresh Business Component&quot;,historyIn, historyOut);

				//Request is submitted. Invoice Limit notification will be activated in the next invoice cycle 
				//and notification will be sent via SMS.
				var strMessageCategory 	= &quot;VFTRCBU User Defined Errors&quot;;
				var strCode				= &quot;0155&quot;;
				var sMsg				= App.LookupMessage(strMessageCategory, strCode);
				App.RaiseErrorText(sMsg);	
			}
			return (CancelOperation);
		}	
		else if (strRequestedLimit != &quot;&quot; &amp;&amp; strRequestedLimit != strAmount)
		{
			//Update/Modify
			psCreateIn = App.NewPropertySet();
			psCreateOut = App.NewPropertySet();
			strWFBS = App.GetService(&quot;Workflow Process Manager&quot;);
			psCreateIn.SetProperty(&quot;ProcessName&quot;,&quot;VFTRCBU Activate/Update Invoice Limit WF&quot;);
		//	psCreateIn.SetProperty(&quot;AccountNumber&quot;, strBillingAccount);//
			psCreateIn.SetProperty(&quot;MSISDN&quot;, strMSISDN);
			psCreateIn.SetProperty(&quot;Operation&quot;, &quot;1&quot;);
		//	psCreateIn.SetProperty(&quot;QuotaType&quot;, strQuoteType);//
		//	psCreateIn.SetProperty(&quot;NotifyType&quot;, &quot;1&quot;);//
		//	psCreateIn.SetProperty(&quot;ApplyTime&quot;, this.Timestamp());//
		//	psCreateIn.SetProperty(&quot;ExpiryTime&quot;, this.Timestamp());//
			psCreateIn.SetProperty(&quot;Amount&quot;, strRequestedLimit);
			psCreateIn.SetProperty(&quot;Shop&quot;, strShop);
			
			strWFBS.InvokeMethod(&quot;RunProcess&quot;, psCreateIn, psCreateOut);
			
			errCode = psCreateOut.GetProperty(&quot;Error Code&quot;);
			if (errCode == &quot;0&quot;)
			{
				
				TheApplication().SetProfileAttr(&quot;VFSkipQueryWF&quot;,&quot;N&quot;);
				historyIn = App.NewPropertySet();
				historyOut = App.NewPropertySet();
				historyIn.SetProperty(&quot;Business Object Name&quot;,strBO);
				historyIn.SetProperty(&quot;Business Component Name&quot;,strBC);
				bsRefreshBC.InvokeMethod(&quot;Refresh Business Component&quot;,historyIn, historyOut);
				
				
				//TheApplication().ActiveBusObject().GetBusComp(strBC).ExecuteQuery(ForwardOnly);
				//Request is submitted. Invoice Limit notification service will be updated in the next invoice cycle 
				//and notification will be sent via SMS.
				var strMessageCategory 	= &quot;VFTRCBU User Defined Errors&quot;;
				var strCode				= &quot;0156&quot;;
				var sMsg				= App.LookupMessage(strMessageCategory, strCode);
				App.RaiseErrorText(sMsg);	
			}
			return (CancelOperation);			
		}		
	}
	else if	(MethodName==&quot;VFTRCBUCancel&quot;)
	{
		var thisBC = this.BusComp();
		var App = TheApplication();
		var strWFBS	= &quot;&quot;;
		var psCreateOut = &quot;&quot;;
		var psCreateIn	= &quot;&quot;;
		var errCode = &quot;&quot;;		
		var strRequestedLimit = thisBC.GetFieldValue(&quot;VFTRCBU Requested Limit&quot;);
		var strAmount = thisBC.GetFieldValue(&quot;VFTRCBU Amount&quot;);
		var strMSISDN = thisBC.ParentBusComp().GetFieldValue(&quot;VF MSISDN&quot;);
		var strMSISDNid = thisBC.ParentBusComp().GetFieldValue(&quot;Id&quot;);
		var strBillingAccount = thisBC.ParentBusComp().GetFieldValue(&quot;Billing Account&quot;);
		var strShop = thisBC.ParentBusComp().GetFieldValue(&quot;VF Originating Shop Code&quot;);
		var strQuoteType = thisBC.GetFieldValue(&quot;VFTRCBU Quota Type&quot;);
		var bsRefreshBC = App.GetService(&quot;SIS OM PMT Service&quot;);

		psCreateIn = App.NewPropertySet();
		psCreateOut = App.NewPropertySet();
		strWFBS = App.GetService(&quot;Workflow Process Manager&quot;);
		psCreateIn.SetProperty(&quot;ProcessName&quot;,&quot;VFTRCBU Activate/Update Invoice Limit WF&quot;);
	//	psCreateIn.SetProperty(&quot;AccountNumber&quot;, strBillingAccount);//
		psCreateIn.SetProperty(&quot;MSISDN&quot;, strMSISDN);
		psCreateIn.SetProperty(&quot;Operation&quot;, &quot;2&quot;); // changed by JFONSECA from 4 to 2 - 08.04.2015
	//	psCreateIn.SetProperty(&quot;QuotaType&quot;, strQuoteType);
	//	psCreateIn.SetProperty(&quot;NotifyType&quot;, &quot;1&quot;);
	//	psCreateIn.SetProperty(&quot;ApplyTime&quot;, this.Timestamp());
	//	psCreateIn.SetProperty(&quot;ExpiryTime&quot;, this.Timestamp());
	//	psCreateIn.SetProperty(&quot;Amount&quot;, strRequestedLimit);
		psCreateIn.SetProperty(&quot;Shop&quot;, strShop);
		
		strWFBS.InvokeMethod(&quot;RunProcess&quot;, psCreateIn, psCreateOut);
		
		errCode = psCreateOut.GetProperty(&quot;Error Code&quot;);
		if (errCode == &quot;0&quot;)
		{

			TheApplication().SetProfileAttr(&quot;VFSkipQueryWF&quot;,&quot;N&quot;);
			historyIn = App.NewPropertySet();
			historyOut = App.NewPropertySet();
			historyIn.SetProperty(&quot;Business Object Name&quot;,strBO);
			historyIn.SetProperty(&quot;Business Component Name&quot;,strBC);
			bsRefreshBC.InvokeMethod(&quot;Refresh Business Component&quot;,historyIn, historyOut);
			
			//Cancellation request is submitted. Notification SMS will be sent at the execution of the request.
			var strMessageCategory 	= &quot;VFTRCBU User Defined Errors&quot;;
			var strCode				= &quot;0157&quot;;
			var sMsg				= App.LookupMessage(strMessageCategory, strCode);
			App.RaiseErrorText(sMsg);	
		}	
		return (CancelOperation);
	}
	return (ContinueOperation);
	//VFTRCBU 29122014: JIRA VFTRCBU-4627 - FTOSCANO - END
}"

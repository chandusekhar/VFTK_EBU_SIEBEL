			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="PendingActivationOrder"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function PendingActivationOrder(Inputs,Outputs)
{
	try 
	{
		var BOLineItem : BusObject = TheApplication().GetBusObject(&quot;TELCO Order Item - Simple&quot;);
		var BCLineItem : BusComp = BOLineItem.GetBusComp(&quot;TELCO Line Items - Simple&quot;);
		var sCustomerId = &quot;&quot;;
		var sdonor = &quot;&quot;;
		
		var sMSISDN = Inputs.GetProperty(&quot;sMSISDN&quot;);
		
		with(BCLineItem) 
		{
			SetViewMode(AllView);
			ClearToQuery();
			ActivateField(&quot;Owner Account Id&quot;);
			ActivateField(&quot;Donor Operator&quot;);
			SetSearchSpec(&quot;MSISDN&quot;, sMSISDN);
			SetSearchSpec(&quot;Root Order Item Id&quot;, &quot;[Id]&quot;);
			SetSearchSpec(&quot;TELCO Action Type LIC&quot;, &quot;= &apos;100&apos; OR [TELCO Action Type LIC] = &apos;102&apos; OR [TELCO Action Type LIC] = &apos;106&apos;&quot;);
			SetSearchSpec(&quot;TELCO Status LIC&quot;, &quot;&lt;&gt; &apos;Complete&apos; AND [TELCO Status LIC] &lt;&gt; &apos;Cancelled&apos;&quot;);
			ExecuteQuery(ForwardOnly);
						
			if(FirstRecord()) {
				sCustomerId = GetFieldValue(&quot;Owner Account Id&quot;);
				sdonor = GetFieldValue(&quot;Donor Operator&quot;);
			}
			
			Outputs.SetProperty(&quot;sCustomerId&quot;, sCustomerId);
			Outputs.SetProperty(&quot;sdonor&quot;, sdonor);	
		}
	}
	finally
	{
		BCLineItem = null;
		BOLineItem = null;
	}
}"

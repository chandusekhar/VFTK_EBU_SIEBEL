			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="fSubmitSearch"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function fSubmitSearch() {
	try {
		var isEBU = &quot;&quot;;
		var SearchBO = TheApplication().GetBusObject(&quot;VF TRK PRM Account&quot;);
		var SearchBC = SearchBO.GetBusComp(&quot;Account&quot;);
		var BOBilling = TheApplication().GetBusObject(&quot;VF TRK PRM Account&quot;);
		var SearchBillAcc = BOBilling.GetBusComp(&quot;VF PRM Billing Account&quot;);
		var SearchAssetBC = SearchBO.GetBusComp(&quot;Asset Mgmt - Asset&quot;);
		var Bo = TheApplication().GetBusObject(&quot;Asset Management&quot;);
		var BC = Bo.GetBusComp(&quot;Asset Mgmt - Asset - Header (Simple)&quot;);
		var CustAcq = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;ACCOUNT_STATUS&quot;, &quot;100&quot;);
		var CustAct = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;ACCOUNT_STATUS&quot;, &quot;95&quot;);
		var strAssetSearchExpr = &quot;&quot;;
		var TcId = &quot;&quot;;
		var AccountId,lsAccId,lsAccType,AccType,vf_sub_class,sCustomerClass,recordId,BillAcc,ICCID;
		var PassportNo = &quot;&quot;;
		var MSISDN = &quot;&quot;;
		var CustomerId = &quot;&quot;;
		var TaxId = &quot;&quot;;
		var len = 0;
		var ActTaxId = false;
		var boc : BusObject = this.BusObject();
		var bcc : BusComp;
		var InDrill : PropertySet = TheApplication().NewPropertySet();
		var OutDrill : PropertySet = TheApplication().NewPropertySet();
		var DrillBS = TheApplication().GetService(&quot;FINS Workflow UI Utilities&quot;);
		this.BusComp().ActivateField(&quot;VFTRCBU Customer Identification&quot;);
		var ICCIDMSISDN = &quot;&quot;;
		var sContext = &quot;&quot;;
		var sOutputResult = &quot;&quot;;
		var donor = &quot;&quot;;
		var sOTPValidView = Clib.strstr(TheApplication().GetProfileAttr(&quot;VF_OTP_VIEWS&quot;), TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;));
		MSISDN = this.BusComp().GetFieldValue(&quot;VF MSISDN&quot;);
		TheApplication().SetProfileAttr(&quot;VF_NBA_MSISDN&quot;, MSISDN);
		FillProfileAttributes();
		CustomerId = this.BusComp().GetFieldValue(&quot;VF Customer ID&quot;);
		TaxId = this.BusComp().GetFieldValue(&quot;VF TAX ID&quot;);
		ICCID = this.BusComp().GetFieldValue(&quot;VF ICCID&quot;)
		PassportNo = this.BusComp().GetFieldValue(&quot;VFTRCBU Customer Identification&quot;);//VFTRCBU WA#02
		BillAcc = this.BusComp().GetFieldValue(&quot;VF Billing Account ID&quot;);
		TheApplication().SetProfileAttr(&quot;Mob_His_Blank&quot;, &quot;Y&quot;);

		if ((MSISDN != &quot;&quot; &amp;&amp; MSISDN.indexOf(&quot;*&quot;) != -1)
			 || (ICCID != &quot;&quot; &amp;&amp; ICCID.indexOf(&quot;*&quot;) != -1)
			 || (CustomerId != &quot;&quot; &amp;&amp; CustomerId.indexOf(&quot;*&quot;) != -1)
			 || (TaxId != &quot;&quot; &amp;&amp; TaxId.indexOf(&quot;*&quot;) != -1)
			 || (BillAcc != &quot;&quot; &amp;&amp; BillAcc.indexOf(&quot;*&quot;) != -1)
			 || (PassportNo != &quot;&quot; &amp;&amp; PassportNo.indexOf(&quot;*&quot;) != -1)//VFTRCBU WA#02
		){
			TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;CR059_001&quot;));
			return (CancelOperation);
		}
		ValidateQueryRules(CustomerId, TaxId, ICCID, BillAcc, MSISDN);//ACAMBAS:VFTRCBU-23235
		//search customer using Ids/Passport No or at least one of Ids and the MSISDN
		if ((TcId != &quot;&quot;) || (PassportNo != &quot;&quot;) || CustomerId != &quot;&quot; || TaxId != &quot;&quot; || (BillAcc != &quot;&quot;)) {
			SearchBC.ClearToQuery();
			SearchBC.SetViewMode(AllView);
			SearchBC.ActivateField(&quot;Org Id&quot;);
			SearchBC.ActivateField(&quot;VF Sub Class&quot;);
			SearchBC.ActivateField(&quot;VF Cust Name Calc&quot;);//VFTRCBU-31632-HRODRIGUES
			if (TcId != &quot;&quot;) {
				SearchBC.SetSearchSpec(&quot;Contact SSN&quot;, TcId);
				if (sOTPValidView)//36516 (TC-4730)
					sContext = CreateSearchCriteria(&quot;Contact SSN&quot;, TcId, &quot;AND&quot;, sContext);//36516TC-4730)
			}
			if (PassportNo != &quot;&quot;) {
				SearchBC.SetSearchSpec(&quot;Doc Number&quot;, PassportNo);
				if (sOTPValidView)//36516 (TC-4730)
					sContext = CreateSearchCriteria(&quot;Doc Number&quot;, PassportNo, &quot;AND&quot;, sContext);//36516TC-4730)
			}
			if (CustomerId != &quot;&quot;) {
				SearchBC.SetSearchSpec(&quot;CSN&quot;, CustomerId);
				if (sOTPValidView)//36516(TC-4730)
					sContext = CreateSearchCriteria(&quot;CSN&quot;, CustomerId, &quot;AND&quot;, sContext);//36516TC-4730)
			}
			if (TaxId != &quot;&quot; &amp;&amp; CustomerId == &quot;&quot;) {
				ActTaxId = true;
				SearchBC.SetSearchExpr(&quot;[VF Tax Number]= &apos;&quot; + TaxId + &quot;&apos; AND ([Account Status Code]=&apos;&quot; + CustAcq + &quot;&apos; OR [Account Status Code] = &apos;&quot; + CustAct + &quot;&apos;)&quot;);
			}
			if (TaxId != &quot;&quot; &amp;&amp; CustomerId != &quot;&quot;)
				SearchBC.SetSearchSpec(&quot;VF Tax Number&quot;, TaxId);
			if (BillAcc != &quot;&quot;) {
				SearchBillAcc.ClearToQuery();
				SearchBillAcc.SetViewMode(AllView);
				SearchBillAcc.ActivateField(&quot;Parent Account Id&quot;);
				SearchBillAcc.SetSearchSpec(&quot;Account Code&quot;, BillAcc);
				if (sOTPValidView) //36516 (TC-4730)
					sContext = CreateSearchCriteria(&quot;Account Code&quot;, BillAcc, &quot;AND&quot;, sContext); //36516(TC-4730)
				TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
				SearchBillAcc.ExecuteQuery(ForwardOnly);
				var len1 = SearchBillAcc.CountRecords();
				if (len1 == 1) {
					AccountId = SearchBillAcc.GetFieldValue(&quot;Parent Account Id&quot;);
					SearchBC.SetSearchSpec(&quot;Id&quot;, AccountId);
				} else {
					TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;ACS Messages&quot;, &quot;125&quot;));
					return (CancelOperation);
				}
			}
			TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
			RetryTax : SearchBC.ExecuteQuery(ForwardOnly);
			len = SearchBC.CountRecords();
			if (len &gt;= 1) {
				lsAccId = SearchBC.GetFieldValue(&quot;Id&quot;);
				lsAccType = SearchBC.GetFieldValue(&quot;Market Class&quot;);
				//FCARRIL:VFTRCBU-27188
				if (lsAccType == TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;ACCOUNT_TYPE&quot;, &quot;101&quot;))
					TheApplication().SetProfileAttr(&quot;Customer_Type&quot;, &quot;EBU&quot;);
				else
					TheApplication().SetProfileAttr(&quot;Customer_Type&quot;, &quot;CBU&quot;);
				//END-FCARRIL
				TheApplication().SetProfileAttr(&quot;SearchedAccId&quot;, lsAccId);
				TheApplication().SetProfileAttr(&quot;SearchedAccType&quot;, lsAccType);
				if (MSISDN != &quot;&quot; || ICCID != &quot;&quot;) {
					strAssetSearchExpr = &quot;[Owner Account Id] = &apos;&quot; + SearchBC.GetFieldValue(&quot;Org Id&quot;) + &quot;&apos;&quot;;
					if (MSISDN != &quot;&quot; &amp;&amp; ICCID == &quot;&quot;) {
						strAssetSearchExpr = strAssetSearchExpr + &quot;AND [Serial Number] = &apos;&quot; + MSISDN + &quot;&apos;&quot;;
						//36516(TC-4730)-Init
						if (sOTPValidView) {
							sContext = CreateSearchCriteria(&quot;Serial Number&quot;, MSISDN, &quot;AND&quot;, sContext);
							sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, MSISDN, &quot;/AND&quot;, sContext);//36516(TC-4730)-End
						}
					} else if (MSISDN == &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
						strAssetSearchExpr = strAssetSearchExpr + &quot; AND [VF ICCID] = &apos;&quot; + ICCID + &quot;&apos;&quot;
							if (sOTPValidView) //36516 (TC-4730)
								sContext = CreateSearchCriteria(&quot;VF ICCID&quot;, ICCID, &quot;AND&quot;, sContext);//36516(TC-4730)
					} else if (MSISDN != &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
						strAssetSearchExpr = strAssetSearchExpr + &quot; AND [Serial Number] = &apos;&quot; + MSISDN + &quot;&apos;  AND [VF ICCID] = &apos;&quot; + ICCID + &quot;&apos;&quot;
							//36516(TC-4730) - Init
							if (sOTPValidView) {
								sContext = CreateSearchCriteria(&quot;Serial Number&quot;, MSISDN, &quot;AND&quot;, sContext);
								sContext = CreateSearchCriteria(&quot;VF ICCID&quot;, ICCID, &quot;AND&quot;, sContext);
								sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, MSISDN, &quot;/AND&quot;, sContext);//36516 TC-4730)-End
							}
					}
					LogInsert(&quot;&gt;&gt;&gt;&gt;SearchExpr: &quot;, strAssetSearchExpr);
					BC.ClearToQuery();
					BC.SetViewMode(AllView);
					BC.ActivateField(&quot;VF Sub Class&quot;);
					BC.ActivateField(&quot;Market Type&quot;);//VFTRCBU-26342
					BC.ActivateField(&quot;VF MSISDN&quot;);
					BC.ActivateField(&quot;VF Tariff Plan&quot;);
					BC.ActivateField(&quot;VF Cust Name Calc&quot;);//VFTRCBU-31632
					BC.SetSearchExpr(strAssetSearchExpr);
					TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
					BC.ExecuteQuery(ForwardBackward);
					if (BC.CountRecords() &gt; 0) {
						//36516 (TC-4730) - Init
						if (MSISDN == &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
							ICCIDMSISDN = BC.GetFieldValue(&quot;VF MSISDN&quot;);
							if (sOTPValidView)
								sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, ICCIDMSISDN, &quot;/AND&quot;, sContext);
						}//36516(TC-4730)-End
						vf_sub_class = BC.GetFieldValue(&quot;VF Sub Class&quot;);
						sCustomerClass = BC.GetFieldValue(&quot;Market Type&quot;);//VFTRCBU-26342
						
						var tariffPlan2=BC.GetFieldValue(&quot;VF Tariff Plan&quot;);
						FillCustomerTypeProfileAttributes(lsAccType,tariffPlan2);		
						
						if(BC.GetFieldValue(&quot;VF Cust Name Calc&quot;) == &quot;***&quot;)//VFTRCBU-31632
							IsVIPCustomer(vf_sub_class, sCustomerClass);
						recordId = BC.GetFieldValue(&quot;Owner Account Id&quot;);
						bcc = boc.GetBusComp(&quot;Account&quot;);
						bcc.ClearToQuery();
						bcc.SetViewMode(AllView);
						bcc.SetSearchSpec(&quot;Id&quot;, recordId);
						TheApplication().SetProfileAttr(&quot;DrilledAssetId&quot;, BC.GetFieldValue(&quot;Id&quot;));
						TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
						bcc.ExecuteQuery(ForwardOnly);
						if (sOTPValidView)
							sContext = CreateSearchCriteria(&quot;recordId&quot;, BC.GetFieldValue(&quot;Id&quot;), &quot;AND&quot;, sContext);
						isEBU = bcc.GetFieldValue(&quot;Market Type&quot;);
						if (isEBU == TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;CUSTOMER_CLASS&quot;, &quot;EBU&quot;)) {
							TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;PRM_STA_CC_8&quot;));
						}
						//36516 TC-4730)-Init
						if (sOTPValidView) {
							if (TaxId == &quot;&quot;) {
								sOutputResult = CustomerValidation(recordId, &quot;Customer&quot;, recordId, sContext);
								if (sOutputResult == &quot;Y&quot;) {
									GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
								}
							} else {
								GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
							}
						} else
							GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
					} else {
						TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;ACS Messages&quot;, &quot;125&quot;));
						return (CancelOperation);
					}
				} else {
					vf_sub_class = SearchBC.GetFieldValue(&quot;VF Sub Class&quot;);
					sCustomerClass = SearchBC.GetFieldValue(&quot;Market Type&quot;);//VFTRCBU-26342-ESANTOS VFTRCBU-27812 and VFTRCBU-27864
					if(SearchBC.GetFieldValue(&quot;VF Cust Name Calc&quot;) == &quot;***&quot;)//VFTRCBU-31632-HRODRIGUES
						IsVIPCustomer(vf_sub_class, sCustomerClass);
					recordId = SearchBC.GetFieldValue(&quot;Id&quot;);
					bcc = boc.GetBusComp(&quot;Account&quot;);
					bcc.ClearToQuery();
					bcc.SetViewMode(AllView);
					bcc.SetSearchSpec(&quot;Id&quot;, recordId);
					TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
					bcc.ExecuteQuery(ForwardOnly);
					isEBU = bcc.GetFieldValue(&quot;Market Type&quot;);
					if (isEBU == TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;CUSTOMER_CLASS&quot;, &quot;EBU&quot;)) {
						TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;PRM_STA_CC_8&quot;));
					}
					//36516TC-4730)-Init
					if (sOTPValidView) {
						if (TaxId == &quot;&quot;) {
							sOutputResult = CustomerValidation(recordId, &quot;Customer&quot;, recordId, sContext);
							if (sOutputResult == &quot;Y&quot;) {
								GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
							}
						} else
							GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
					} else
						GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
					//36516(TC-4730)-End
					return (CancelOperation);
				}
			} else {
				if (ActTaxId) {
					ActTaxId = false;
					SearchBC.ClearToQuery();
					SearchBC.SetViewMode(AllView);
					SearchBC.ActivateField(&quot;Org Id&quot;);
					SearchBC.ActivateField(&quot;VF Sub Class&quot;);
					SearchBC.SetSearchSpec(&quot;VF Tax Number&quot;, TaxId);
					goto RetryTax;
				}
				TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;ACS Messages&quot;, &quot;125&quot;));
				return (CancelOperation);
			}
		} else {
			if (MSISDN != &quot;&quot; || ICCID != &quot;&quot;) {
				if (MSISDN != &quot;&quot; &amp;&amp; ICCID == &quot;&quot;) {
					strAssetSearchExpr = &quot;[VF MSISDN] = &apos;&quot; + MSISDN + &quot;&apos;&quot;;
					//36516 (TC-4730)-Init
					if (sOTPValidView) {
						sContext = CreateSearchCriteria(&quot;VF MSISDN&quot;, MSISDN, &quot;AND&quot;, sContext);
						sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, MSISDN, &quot;/AND&quot;, sContext);
					}
					//36516 (TC-4730)-End
				} else if (MSISDN == &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
					strAssetSearchExpr = &quot;[VF ICCID] = &apos;&quot; + ICCID + &quot;&apos;&quot;
						if (sOTPValidView)//36516(TC-4730)
							sContext = CreateSearchCriteria(&quot;VF ICCID&quot;, ICCID, &quot;AND&quot;, sContext); //36516(TC-4730)
				} else if (MSISDN != &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
					strAssetSearchExpr = &quot;[VF MSISDN] = &apos;&quot; + MSISDN + &quot;&apos;  AND [VF ICCID] = &apos;&quot; + ICCID + &quot;&apos;&quot;
						//36516(TC-4730)-Init
						if (sOTPValidView) {
							sContext = CreateSearchCriteria(&quot;VF MSISDN&quot;, MSISDN, &quot;AND&quot;, sContext);
							sContext = CreateSearchCriteria(&quot;VF ICCID&quot;, ICCID, &quot;AND&quot;, sContext);
							sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, MSISDN, &quot;/AND&quot;, sContext);
						}//36516 (TC-4730)-End
				}
				LogInsert(&quot;&gt;&gt;&gt;&gt;SearchExpr: &quot;, strAssetSearchExpr);
				BC.ClearToQuery();
				BC.SetViewMode(AllView);
				BC.ActivateField(&quot;VF MSISDN&quot;);//36516(TC-4730)
				BC.ActivateField(&quot;VF Donor Operator&quot;);
				BC.ActivateField(&quot;VF Customer Type&quot;);
				BC.ActivateField(&quot;VF Tariff Plan&quot;);
				BC.SetSearchExpr(strAssetSearchExpr);
				TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
				BC.ExecuteQuery(ForwardBackward);
				len = BC.CountRecords();
				BC.FirstRecord();
				if (BC.FirstRecord()) {
					donor = BC.GetFieldValue(&quot;VF Donor Operator&quot;);
					lsAccType = BC.GetFieldValue(&quot;VF Customer Type&quot;);
                    var tariffPlan=BC.GetFieldValue(&quot;VF Tariff Plan&quot;);
					FillCustomerTypeProfileAttributes(lsAccType,tariffPlan);	
				}

				switch (len) {
				case 0:
					if (MSISDN != null &amp;&amp; MSISDN != &quot;&quot;)//JSARMENTO-VFTRCBU-3642
					{//ACAMBAS-Start
						InDrill.SetProperty(&quot;sMSISDN&quot;, MSISDN);
						PendingActivationOrder(InDrill, OutDrill);
						var sCustomerId = OutDrill.GetProperty(&quot;sCustomerId&quot;);
						donor = OutDrill.GetProperty(&quot;sdonor&quot;);
						if (sCustomerId != &quot;&quot;) {
							if (donor != null &amp;&amp; donor != &quot;&quot;)
								GotoMNPTrackingNewView(MSISDN);
							else
								GotoCustomerView(sCustomerId);
						} else//ACAMBAS-End
							TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;ACS Messages&quot;, &quot;125&quot;));
					} else
						TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;ACS Messages&quot;, &quot;125&quot;));
					break;
				case 1:
					//36516(TC-4730)-Init
					if (MSISDN == &quot;&quot; &amp;&amp; ICCID != &quot;&quot;) {
						ICCIDMSISDN = BC.GetFieldValue(&quot;VF MSISDN&quot;);
						if (sOTPValidView)
							sContext = CreateSearchCriteria(&quot;VF OTP MSISDN&quot;, ICCIDMSISDN, &quot;/AND&quot;, sContext);
					}//36516(TC-4730)-End
					//DEF-10630-SJOAO-JFONSECA-START
					
				
					//END
					SearchBC.ClearToQuery();
					SearchBC.SetViewMode(AllView);
					SearchBC.ActivateField(&quot;VF Sub Class&quot;);
					BC.ActivateField(&quot;Market Type&quot;);//VFTRCBU-26342
					SearchBC.ActivateField(&quot;VF Cust Name Calc&quot;)//VFTRCBU-31632 - HRODRIGUES
					SearchBC.SetSearchSpec(&quot;Org Id&quot;, BC.GetFieldValue(&quot;Owner Account Id&quot;));
					TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
					SearchBC.ExecuteQuery(ForwardOnly);
					lsAccId = SearchBC.GetFieldValue(&quot;Id&quot;);
					lsAccType = SearchBC.GetFieldValue(&quot;Market Class&quot;);
					TheApplication().SetProfileAttr(&quot;SearchedAccId&quot;, lsAccId);
					TheApplication().SetProfileAttr(&quot;SearchedAccType&quot;, lsAccType);
					TheApplication().SetProfileAttr(&quot;SearchedMSISDN&quot;, MSISDN); //TLG-2474
					vf_sub_class = SearchBC.GetFieldValue(&quot;VF Sub Class&quot;);
					sCustomerClass = BC.GetFieldValue(&quot;Market Type&quot;);//VFTRCBU-26342
					if(SearchBC.GetFieldValue(&quot;VF Cust Name Calc&quot;) == &quot;***&quot;)//VFTRCBU-31632 - HRODRIGUES
						IsVIPCustomer(vf_sub_class, sCustomerClass);
					recordId = SearchBC.GetFieldValue(&quot;Id&quot;);
					bcc = boc.GetBusComp(&quot;Account&quot;);
					bcc.ClearToQuery();
					bcc.SetViewMode(AllView);
					bcc.SetSearchSpec(&quot;Id&quot;, recordId);
					TheApplication().SetProfileAttr(&quot;DrilledAssetId&quot;, BC.GetFieldValue(&quot;Id&quot;));
					TheApplication().SetProfileAttr(&quot;Take_Logs&quot;, &quot;Y&quot;);
					bcc.ExecuteQuery(ForwardOnly);
					isEBU = bcc.GetFieldValue(&quot;Market Type&quot;);
					if (isEBU == TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;CUSTOMER_CLASS&quot;, &quot;EBU&quot;)) {
						TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;PRM_STA_CC_8&quot;));
					}
					//36516(TC-4730)-Init
					if (sOTPValidView) {
						sOutputResult = CustomerValidation(recordId, &quot;Customer&quot;, recordId, sContext);
						if (sOutputResult == &quot;Y&quot;) {
							GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
						}
					} else
						GoToDrilldown(&quot;VF PRM Search View Temp Applet - Account&quot;, &quot;Id&quot;);
					//36516(TC-4730)-End
					break;
				default:
					TheApplication().GotoView(&quot;PRM VF Asset Mgmt - Assets View&quot;, Bo);
				}
				return (CancelOperation);
			}
		}
		LogInsert(&quot;&quot;, &quot;&quot;);
	}
	finally {
		PassportNo = null;
		SearchBO = null;
		SearchBillAcc = null;
		SearchBC = null;
		BillAcc = null;
		TcId = null;
		MSISDN = null;
		CustomerId = null;
		TaxId = null;
		len = null;
		Bo = null;
		BC = null;
		strAssetSearchExpr = null;
		sContext = null;
		sOutputResult = null;
		ICCIDMSISDN = null;
		sOTPValidView = null;
	}
}"

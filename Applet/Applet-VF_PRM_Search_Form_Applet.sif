			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GoToDrilldown"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GoToDrilldown(AppletName, DrillDownField)
{
	//Function created to invoke drilldown
	try
	{
		var InDrill : PropertySet = TheApplication().NewPropertySet();
		var OutDrill : PropertySet = TheApplication().NewPropertySet();
		var DrillBS = TheApplication().GetService(&quot;FINS Workflow UI Utilities&quot;);
		
		InDrill.SetProperty(&quot;Applet&quot;,AppletName);
		InDrill.SetProperty(&quot;DrillDown&quot;,DrillDownField);
		DrillBS.InvokeMethod(&quot;GotoDrillDown&quot;,InDrill,OutDrill);
	}
	catch(e)
	{
		throw(e.toString());
	}
	finally
	{
		InDrill = null;
		OutDrill = null;
		DrillBS = null;
	}
}"

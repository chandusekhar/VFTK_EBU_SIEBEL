			   SCRIPT="function WebApplet_PreInvokeMethod (MethodName) 
{ 		
	if(MethodName == &quot;ExecuteQuery&quot;)
	{ 			
		TheApplication().SetProfileAttr(&quot;Take_Logs&quot;,&quot;Y&quot;); 			
		return (ContinueOperation); 		
	} 		
	if(MethodName == &quot;Submit&quot;) 		
	{ 
		//SIEBELLOGGING
		var MSISDN = &quot;&quot;;
		var ICCID = &quot;&quot;;
		var BA = &quot;&quot;;
		var taxId = &quot;&quot;;
		var custId = &quot;&quot;;
		var searchSpec = &quot;&quot;;
		var trName = &quot;&quot;;
		MSISDN = this.BusComp().GetFieldValue(&quot;VF MSISDN&quot;);
		ICCID = this.BusComp().GetFieldValue(&quot;VF ICCID&quot;);

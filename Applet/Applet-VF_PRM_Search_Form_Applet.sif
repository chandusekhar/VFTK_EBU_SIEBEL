			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="SiebelLogging"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function SiebelLogging(trName, searchSpec)
{
	try
	{
		var activeFlagRTE = &quot;N&quot;;
		var activeFlagLOV = &quot;N&quot;;
		
		TheApplication().SetProfileAttr(&quot;SIEBEL_LOGGING_TRTYPE&quot;, &quot;Query&quot;);
		TheApplication().SetProfileAttr(&quot;SIEBEL_LOGGING_TRNAME&quot;, trName);
		TheApplication().SetProfileAttr(&quot;SIEBEL_LOGGING_BCNAME&quot;, &quot;&quot;);
		TheApplication().SetProfileAttr(&quot;SIEBEL_LOGGING_FIELDNAME&quot;,&quot;&quot;);
		TheApplication().SetProfileAttr(&quot;SIEBEL_LOGGING_LOGFIELD&quot;, &quot;&quot;);
		
		var rteBO : BusObject;
		var rteBC : BusComp;
		
		rteBO = TheApplication().GetBusObject(&quot;Personalization Events&quot;);
		rteBC= rteBO.GetBusComp(&quot;Personalization Event&quot;);
		with(rteBC)
		{
			ClearToQuery();
			SetViewMode(AllView);
			SetSearchExpr(&quot;[Object Name] = &apos;&quot;+this.Name()+&quot;&apos; AND [VFTR Type] = &apos;SIEBELLOGGING&apos; AND [Event] = &apos;DisplayApplet&apos;&quot;);
			ExecuteQuery(ForwardBackward);
			if(FirstRecord())
			{
				activeFlagRTE = GetFieldValue(&quot;VFTR Active&quot;);
			}
		}	
		activeFlagLOV = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;VFTR_SIEBEL_LOGGING_ENABLED&quot;, &quot;Siebel Logging Enabled&quot;);
		if(activeFlagRTE == &quot;Y&quot; &amp;&amp; activeFlagLOV == &quot;Y&quot;)
		{
			var wkfService = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
			var wkfIn = TheApplication().NewPropertySet();
			var wkfOut = TheApplication().NewPropertySet();
			
			wkfIn.SetProperty(&quot;ProcessName&quot;, &quot;VFTR Siebel Logging Log File WF&quot;);
			wkfIn.SetProperty(&quot;searchExp&quot;, searchSpec);
			wkfService.InvokeMethod(&quot;RunProcess&quot;, wkfIn, wkfOut);
			}
	}
	catch (e)
	{
		throw (e);
	}
	finally
	{
		wkfService = null;
		wkfIn = null;
		wkfOut = null;
		rteBO = null;
		rteBC = null;
	}
}"

			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CheckReviseAllowance"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CheckReviseAllowance()
{
    try
    {
        var exit = &quot;OK&quot;;
        var bcOrderEntry: BusComp = this.BusComp();
        var strOrderId = bcOrderEntry.GetFieldValue(&quot;Order Number&quot;);

        //VFTRCBU-35100 - Start
        var strObjectId = bcOrderEntry.GetFieldValue(&quot;Order Id&quot;);
        var strOMOrderPublicId = bcOrderEntry.GetFieldValue(&quot;TELCO OM Order Public Id&quot;);
        //VFTRCBU-35100 - End

        var boOrderManagement: BusObject = TheApplication().GetBusObject(&quot;HP Order Entry&quot;);
        var bcOrderManagement: BusComp = boOrderManagement.GetBusComp(&quot;HP Order Entry - Orders&quot;);
        var express = &quot;[Order Number] = &apos;&quot; + strOrderId + &quot;&apos;&quot;

        bcOrderManagement.SetViewMode(AllView);
        bcOrderManagement.ClearToQuery();
        bcOrderManagement.ActivateField(&quot;Parent Order Id&quot;); //VFTRCBU-35100
        bcOrderManagement.SetSearchExpr(express);
        //bcOrderManagement.SetSortSpec(&quot;Revision (DESCENDING)&quot;); //VFTRCBU-35100
        bcOrderManagement.ExecuteQuery(ForwardOnly);

        var recordExistsOnLineItems = bcOrderManagement.FirstRecord();

        if (recordExistsOnLineItems)
        {
            //var revision = bcOrderManagement.GetFieldValue(&quot;Revision&quot;); //VFTRCBU-35100
            var currentRevision = bcOrderManagement.GetFieldValue(&quot;Revising&quot;);
            var existParent = bcOrderManagement.GetFieldValue(&quot;Parent Order Id&quot;); //VFTRCBU-35100

            //if (revision &gt; 1 &amp;&amp; currentRevision == &quot;Y&quot;) //VFTRCBU-35100
            if (existParent != &quot;&quot; &amp;&amp; currentRevision == &quot;Y&quot;) //VFTRCBU-35100
            {
                var exit = &quot;KO&quot;;
            }

            //}
            //VFTRCBU-35100 - Start
            var svc = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
            var psInput = TheApplication().NewPropertySet();
            var psOutput = TheApplication().NewPropertySet();

            psInput.SetProperty(&quot;Object Id&quot;, strObjectId);
            psInput.SetProperty(&quot;ProcessName&quot;, &quot;TELCO Order Cancellable OSM&quot;);
            psInput.SetProperty(&quot;OM Order Public Id&quot;, strOMOrderPublicId);
            svc.InvokeMethod(&quot;RunProcess&quot;, psInput, psOutput);

            if (psOutput.GetProperty(&quot;Error Code&quot;) != &quot;0&quot;)
            {
                var exit = &quot;KO1&quot;;
            }
            //VFTRCBU-35100 - End
        }
    }
    finally
    {
        bcOrderManagement = null;
        boOrderManagement = null;
        bcOrderEntry = null;
        psInput = null;
        psOutput = null;
        svc = null;

    }
    return (exit);
}"

			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="WebApplet_PreCanInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_PreCanInvokeMethod (MethodName, &amp;CanInvoke)
{
//Oracle Review October 2011 - User Prop not supported in this Class
  try
  {
	var thisBC = this.BusComp();
	var psInputs   : PropertySet  =  TheApplication().NewPropertySet();
	var psOutputs  : PropertySet  =  TheApplication().NewPropertySet();		  
	var bsGeneralOperations : Service = TheApplication().GetService (&quot;ACS General Operations&quot;);
	var strRoles = TheApplication().GetProfileAttr(&quot;TELCO.PositionProfile:Role.AvailableRoles&quot;);
	var BoAcc:BusObject;
	var bcAccount:BusComp;
	//Added for WP17 as per Enrico&apos;s Instruction
	if (TheApplication().GetProfileAttr(&quot;RefreshRequired&quot;)==&quot;Y&quot;)
	{
	  TheApplication().SetProfileAttr(&quot;RefreshRequired&quot;,&quot;N&quot;);
	  thisBC.InvokeMethod(&quot;RefreshBusComp&quot;);
	}//Added for WP17 as per Enrico&apos;s Instruction
		
	//Baran Bozoglu 04/02/2011 Query button was not visible if there was no record after searching order ids.
	var CountRecord;
		
	//TLG-5412
	try{
	  TheApplication().SetProfileAttr(&quot;VF Shop Id&quot;,thisBC.GetFieldValue(&quot;VF Shop Id&quot;));
	}
	catch(e){
	  //try-catch in order to manage the no-record scenario
	}
		
	//Baran Bozoglu	04/02/2011 If there is no record after querying, Back To MNP SR button should not be seen.
	if (MethodName == &quot;BackToSR&quot;)
	{
	  var activeView = TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
	  var isReadOnly = TheApplication().InvokeMethod(&quot;IsViewReadOnly&quot;, activeView);
	  if ((CountRecord != 0))
	  {
		CanInvoke=&quot;True&quot;;
		return (CancelOperation);
	  }
	  else
	  {
		CanInvoke=&quot;False&quot;; 
		return (CancelOperation);
	  }                                                                                     
	} 
			
	/*****   WA#18-MNP  Begin: This method has been deprecated. Replaced by method TELCOUserSubmitOrder  **********		
	if (MethodName == &quot;SubmitOrder&quot;)
	{
	  var actualPosition = TheApplication().GetProfileAttr(&quot;Position&quot;);
	  var smePosition = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_APPROVER_POSITION&quot;,&quot;SME Dealer&quot;);	
	  var orderMnpPortIn = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ORDER_SUB_TYPE&quot;,&quot;106&quot;);	
	  var currentOrderSubType =thisBC.GetFieldValue(&quot;Order Sub Type&quot;);
	  var smeSubmitter = ((actualPosition == smePosition) &amp;&amp; (orderMnpPortIn == currentOrderSubType)) ? &quot;Y&quot; : &quot;N&quot;;
	  var strOrderStatus 	= thisBC.GetFieldValue (&quot;Order Status&quot;);
	  var lstResp = thisBC.GetFieldValue (&quot;VF MNP In Submission&quot;);
	  var lstResp2 = thisBC.GetFieldValue (&quot;VF Port In Sup&quot;);//PROD00031210
	  var lstResp3 = thisBC.GetFieldValue (&quot;VF Port In Agent&quot;);//PROD00031210
	  var strWaiting	= TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Waiting&quot;);
	  var strPending	= TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Pending&quot;);
	  var strOrderStatus 	= thisBC.GetFieldValue (&quot;Order Status&quot;);
	  var strAddNewServiceLOV = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_TYPE&quot;, &quot;Add New Service&quot;);   
	  var checkconfirmation = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;VF_ORDER_CONFIRM_CONTROL&quot;, &quot;CONTROL_PASS&quot;);
						
	  psInputs.SetProperty (&quot;Function&quot;, &quot;0034Confirm&quot;);
	  psInputs.SetProperty (&quot;Name&quot;, &quot;Confirm&quot;);
	  bsGeneralOperations.InvokeMethod (&quot;GetParameter_3Inputs&quot;, psInputs, psOutputs);
	  var buttonVisibility = psOutputs.GetProperty (&quot;Cons1&quot;);
	  //if (strOrderStatus == strWaiting &amp;&amp; lstResp == &quot;Y&quot;)	//PROD00031210
	  if (strOrderStatus == strWaiting &amp;&amp; (lstResp == &quot;Y&quot; || lstResp2 == &quot;Y&quot; || lstResp3 == &quot;Y&quot;))
	  {
		CanInvoke = &quot;TRUE&quot;;
	  }
				
	  // CR 268   --- Dealer Confirmation Check
	  else if (strOrderStatus == strPending )
	  {
		if (checkconfirmation == &quot;Y&quot;)
		{
		  if (thisBC.GetFieldValue(&quot;Order Sub Type&quot;) == orderMnpPortIn &amp;&amp; thisBC.GetFieldValue(&quot;Order Type&quot;) == strAddNewServiceLOV)
		  {
			var DealFlag = thisBC.GetFieldValue(&quot;Dealer Confirmation&quot;);
			if (DealFlag == &quot;&quot;)
			{
			  CanInvoke = &quot;FALSE&quot;;
			}
			else
			{	
			  CanInvoke = &quot;TRUE&quot;;
			}
		  }
		  else
		  {
			CanInvoke = &quot;TRUE&quot;;
		  }
		}
		else
		{
		  CanInvoke = &quot;TRUE&quot;;
		}
	  }
	  else 
	  {
		CanInvoke = &quot;FALSE&quot;;
	  }
	  // End of Dealer Confirmation check  		
	  return (CancelOperation);				
	}
	*****   WA#18-MNP End: This method has been deprecated. Replaced by method TELCOUserSubmitOrder  *********/

	/*** WA#18:20150114:MS: Begin - Copied  this if block from above (deprecated) method &apos;SubmitOrder&apos; and updated it ***/	
	if (MethodName == &quot;TELCOUserSubmitOrder&quot;)
	{
	  var actualPosition = TheApplication().GetProfileAttr(&quot;Position&quot;);
	  var smePosition = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_APPROVER_POSITION&quot;,&quot;SME Dealer&quot;);	
	  var orderMnpPortIn = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ORDER_SUB_TYPE&quot;,&quot;106&quot;);	
	  var currentOrderSubType =thisBC.GetFieldValue(&quot;Order Sub Type&quot;);
	  var smeSubmitter = ((actualPosition == smePosition) &amp;&amp; (orderMnpPortIn == currentOrderSubType)) ? &quot;Y&quot; : &quot;N&quot;;
	  var strOrderStatus 	= thisBC.GetFieldValue (&quot;Status&quot;);
	  var lstResp = thisBC.GetFieldValue (&quot;VF MNP In Submission&quot;);
	  var lstResp2 = thisBC.GetFieldValue (&quot;VF Port In Sup&quot;);//PROD00031210
	  var lstResp3 = thisBC.GetFieldValue (&quot;VF Port In Agent&quot;);//PROD00031210
	  var strWaiting = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Waiting&quot;);
	  
	  var strOrderStatus 	= thisBC.GetFieldValue (&quot;Status&quot;);
	  var strAddNewServiceLOV = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_TYPE&quot;, &quot;Add New Service&quot;);   
	  var checkconfirmation = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;VF_ORDER_CONFIRM_CONTROL&quot;, &quot;CONTROL_PASS&quot;);
	  var StrRole   =  TheApplication().GetProfileAttr(&quot;TELCO.PositionProfile:Role.AvailableRoles&quot;); //Edited JSARMENTO

	  psInputs.SetProperty (&quot;Function&quot;, &quot;0034Confirm&quot;);
	  psInputs.SetProperty (&quot;Name&quot;, &quot;Confirm&quot;);
	  psInputs.SetProperty (&quot;Value&quot;, StrRole);
	  bsGeneralOperations.InvokeMethod (&quot;GetParameter_3Inputs&quot;, psInputs, psOutputs);
	  var buttonVisibility = psOutputs.GetProperty (&quot;Cons1&quot;);
	  //if (strOrderStatus == strWaiting &amp;&amp; lstResp == &quot;Y&quot;)	//PROD00031210
	  
      /*** WA#18:MNP:20150203:MS: Begin - Changed this existing if-else-if block to allow order with status other than Pending to be submitted ***/

	  /*** WA#18:- Begin- Waiting status has been deprecated .  
	  if (strOrderStatus == strWaiting &amp;&amp; (lstResp == &quot;Y&quot; || lstResp2 == &quot;Y&quot; || lstResp3 == &quot;Y&quot;))
	  {
		CanInvoke = &quot;TRUE&quot;;
	  }	
	  *** WA#18:- End-Waiting status is deprecated ***/
  
	
      // CR 268   --- Dealer Confirmation Check
	  /*** WA#18: Begin - Quick enable submission of order for status other than Pending ***/ 
	  //else if (strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Pending&quot;) )   
	  if (strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Pending&quot;)           || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Error&quot;)             || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Failed&quot;)            || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Validations Error&quot;) || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Scenarios Validations Error&quot;) || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Submission Error&quot;)  || 
	      strOrderStatus == TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Submission Error&quot;))
	  {
	  /*** WA#18: Begin - Quick enable submission of order for status other than Pending ***/ 
	  
		if (checkconfirmation == &quot;Y&quot;)
	    {
		  if (thisBC.GetFieldValue(&quot;Order Sub Type&quot;) == orderMnpPortIn &amp;&amp; thisBC.GetFieldValue(&quot;Order Type&quot;) == strAddNewServiceLOV)
		  {
		    var DealFlag = thisBC.GetFieldValue(&quot;Dealer Confirmation&quot;);
		    if (DealFlag == &quot;&quot;)
		    {
			  CanInvoke = &quot;FALSE&quot;;
		    }
		    else
		    {	
			  CanInvoke = &quot;TRUE&quot;;
		    }
	 	  }
		  else
		  {
		    CanInvoke = &quot;TRUE&quot;;
		  }
	    }
	    else
	    {
		  CanInvoke = &quot;TRUE&quot;;
	    }
	  }
	  else 
	  {
	    CanInvoke = &quot;FALSE&quot;;
	  }
	  // End of Dealer Confirmation check  		
 	  return (CancelOperation);				
    }
    /*** WA#18:20150114:MS: End block ***/	

	if (MethodName == &quot;CopyOrder&quot; )//WA#18 Port-in Canc //jmarques
	{
	  var OrderStatus     = thisBC.GetFieldValue(&quot;Status&quot;);
	  var RevisingFlag    = thisBC.GetFieldValue(&quot;Revising&quot;);
	  var OrderSubType    = thisBC.GetFieldValue(&quot;Order Sub Type&quot;);
	  var ParentOrder     = thisBC.GetFieldValue(&quot;Parent Order Id&quot;); // Added WA18 JSOUSA
		
	  var lovStatusPend  	   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;,&quot;Pending&quot;);
	  var lovStatusOpen      = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;,&quot;Open&quot;);
	  var lovStatusFailed   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;,&quot;Failed&quot;);
	  var lovStatusSubPend   = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;,&quot;Submitted-Pending&quot;);
	  var lovOrderSubType    = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;ORDER_SUB_TYPE&quot;,&quot;106&quot;);
	  var lovSRStatusClosed    = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;SR_STATUS&quot;,&quot;Closed&quot;);
	  
	  //for Migration Flag Validation - WA18

	  BoAcc = TheApplication().GetBusObject(&quot;Account&quot;);
	  bcAccount = BoAcc.GetBusComp(&quot;Account&quot;);
					
	  var AccountId = thisBC.GetFieldValue(&quot;Account Id&quot;);
	  var SRStatus = thisBC.GetFieldValue(&quot;Service Request Status&quot;);
	  var MigrationFlag = null;	
					
	  with(bcAccount)
	  {
		SetViewMode(AllView);
		ClearToQuery();
		SetSearchSpec (&quot;Id&quot;, AccountId);
		ExecuteQuery(ForwardOnly);
				
		if (FirstRecord())
		{	
		  MigrationFlag = GetFieldValue(&quot;VFTRCBU Migrating Flag&quot;);
		}
					
		if(MigrationFlag == &quot;Y&quot;)
		{
		  CanInvoke = &quot;FALSE&quot;;
		  return(CancelOperation);
		}
	  }
								
	  //End Migration Flag Validation
	  if (SRStatus != lovSRStatusClosed)
	  {
	    CanInvoke = &quot;FALSE&quot;;
	    return(CancelOperation);
	  }
						
	  if (OrderSubType == null || OrderSubType == &quot;&quot; || OrderSubType != lovOrderSubType)
	  {
		CanInvoke = &quot;FALSE&quot;;
		return(CancelOperation);
	  }
	  
	  if (OrderStatus == null || OrderStatus == &quot;&quot; || (OrderStatus != lovStatusOpen &amp;&amp; OrderStatus != lovStatusFailed))
	  {
		CanInvoke = &quot;FALSE&quot;;
		return(CancelOperation);
	  }
	  
	  psInputs.SetProperty(&quot;Function&quot;, &quot;0116MnpCancellation&quot;);
	  psInputs.SetProperty(&quot;Name&quot;, &quot;MnpCancellation&quot;);
	  psInputs.SetProperty(&quot;Value&quot;, strRoles);
	  bsGeneralOperations.InvokeMethod(&quot;GetParameter_3Inputs&quot;, psInputs, psOutputs);
	  var buttonVisibility = psOutputs.GetProperty(&quot;Cons1&quot;);
				
	  if (buttonVisibility == &quot;False&quot; || buttonVisibility == &quot;&quot;) 
	  {
		CanInvoke = &quot;FALSE&quot;;
		return(CancelOperation);
	  }
				
	  //var ParentOrder = thisBC.GetFieldValue (&quot;Parent Order Id&quot;); // Replace Revision number WA#18
	  if (ParentOrder != &quot;&quot;)
	  {
		CanInvoke = &quot;FALSE&quot;;
		return(CancelOperation);
	  }
				
	  if (RevisingFlag == &quot;Y&quot;)
	  {
		CanInvoke = &quot;FALSE&quot;;
		return(CancelOperation);
	  }
						  
	  var activeView = TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
	  var isReadOnly = TheApplication().InvokeMethod(&quot;IsViewReadOnly&quot;, activeView);
	  if (isReadOnly == &quot;TRUE&quot;)
	  { 
		CanInvoke = &quot;FALSE&quot;;
		return (CancelOperation);
	  }
								
	  CanInvoke = &quot;TRUE&quot;;
	  return (CancelOperation);
	}
	//JMARQUES

	//36516 (TC-4730) - Init
	if (MethodName == &quot;NewQuery&quot;)
	{
	  var psIn   : PropertySet  =  TheApplication().NewPropertySet();
	  var psOut  : PropertySet  =  TheApplication().NewPropertySet();		  
	  var bsWorkflowProcMgr : Service = TheApplication().GetService (&quot;Workflow Process Manager&quot;);
		
	  psIn.SetProperty (&quot;ProcessName&quot;, &quot;VF Check Responsibility Users WF&quot;);
	  psIn.SetProperty (&quot;ResponsibilityName&quot;, &quot;VF Customer Validation Management&quot;);
	  psIn.SetProperty (&quot;UserName&quot;, TheApplication().LoginName());
	  bsWorkflowProcMgr.InvokeMethod (&quot;RunProcess&quot;, psIn, psOut);
	  var hasResp = psOut.GetProperty(&quot;Validate&quot;);

	  //Disable Query button if user has responsibility &quot;VF Customer Validation Management&quot;
	  if (hasResp == &quot;Y&quot;)
		{ 
			CanInvoke = &quot;FALSE&quot;;
		}
		else
		{ 
			CanInvoke = &quot;TRUE&quot;;
		}		
		return (CancelOperation);
	}

	if (MethodName == &quot;CancelModify&quot;)
	{
	  // JIRA-19914:20151003:Begin:Added validation to prevent cancelling MNP Orders in case the related SR is already in status Closed
	  if (this.BusComp().GetFieldValue(&quot;Service Request Status&quot;)==TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;SR_STATUS&quot;,&quot;Closed&quot;) )
	  {                                                            
	    CanInvoke = &quot;FALSE&quot;;
	    return (CancelOperation);  			
      }
	  // JIRA-19914:20151003:End	
	    
	  CanInvoke = CanInvokeCancelOrder();
	  return (CancelOperation);  			
	}

	/****** WA#18-MNP: Begin: Deprecated method. Replaced by method CopyOrder    **************** 
	if (MethodName == &quot;ReviseOrder&quot; )
	{
	  if ( TheApplication().GetSharedGlobal(&quot;CANINVOKEREVISE&quot;) == &quot;TRUE&quot;)
	  {
		var RevisionNumber 	= thisBC.GetFieldValue (&quot;Revision&quot;);
		if(RevisionNumber == 1)
		{
		  var activeView = TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
		  var isReadOnly = TheApplication().InvokeMethod(&quot;IsViewReadOnly&quot;, activeView);
		  if (isReadOnly != &quot;TRUE&quot;)
		  { 
			CanInvoke = &quot;TRUE&quot;;
		  }
		  else
		  { 
			CanInvoke = &quot;FALSE&quot;;
		  }
		}			
	  }
	  return (CancelOperation);
	}
	/****** WA#18-MNP: End: Deprecated method. Replaced by method CopyOrder    *****************/
		
	
			
	}
	//catch(e) //GEN_REQ_121-CR106:HP:20/11/2012: Commented out as per Oracle Review comment
	//{throw(e);}
	finally
	{
		bsGeneralOperations	= null;
		psOutputs		= null;
		psInputs		= null;
		thisBC = null;
		bsWorkflowProcMgr = null;
		psIn = null;
		psOut = null;
		hasResp = null;
	}
	return (ContinueOperation);
}"

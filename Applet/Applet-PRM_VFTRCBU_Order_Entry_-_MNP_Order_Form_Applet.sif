		  //  bsOrderMng.InvokeMethod(&quot;UpdateItemPriceListNew&quot;, Inputs, Outputs);
		  return (ContinueOperation);
		}
	  }
	}
	***************************    WA#18:MNP End block-Deprecated method    ***********************/
	
	/*** WA#18:20150114:MS: Begin-Added this if block (as in WA#39 for Applet:Order Entry - Order Form Applet Dashboard (Sales)) similar to a standard activation process ***/	
	else if(MethodName==&quot;TELCOUserSubmitOrder&quot;) //// WA#39 - ACN was calling directly the deprecated SubmitOrder function (name == SubmitOrder)					
	{					

			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="SendBackSR"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function SendBackSR()
{
//VFTRCBU WA#18 - MPONTE 2015.01.09

	var TheApp = TheApplication();
	var thisBC = this.BusComp();
	var BoOrderEntry:BusObject;
	var bcDocumentum  : BusComp;
	var boServiceReq:BusObject;
	var bcServiceReq:BusComp;

	BoOrderEntry		= TheApp.GetBusObject(&quot;Order Entry (Sales)&quot;);

	var StrOrder = thisBC.GetFieldValue(&quot;Id&quot;);
	bcDocumentum = BoOrderEntry.GetBusComp(&quot;VF Mnp Documentum&quot;);
	bcDocumentum.SetViewMode(AllView);
	bcDocumentum.ClearToQuery();
  		bcDocumentum.SetSearchSpec (&quot;Order Id&quot;, StrOrder);
		bcDocumentum.ExecuteQuery(ForwardOnly);  
 		
  		var ItemBCExists     = bcDocumentum.FirstRecord();
	var allVerifyYes     = true;
 	var strVerifyFlag    = null;
	while(ItemBCExists){
	    strVerifyFlag = bcDocumentum.GetFieldValue(&quot;Calc_Verification&quot;);
		   	    
	 	if ( strVerifyFlag == &quot;N&quot;) {
	   		allVerifyYes = false;
	   	}
	   	ItemBCExists = bcDocumentum.NextRecord();
	} 
		    
	if (allVerifyYes == false ||  strVerifyFlag == null ) {
		var sMsg				= TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;008&quot;);
		TheApplication().RaiseErrorText(sMsg);					
		return (CancelOperation);
	}	
		
	var bcSR			= thisBC.GetFieldValue(&quot;Service Request Id&quot;);
	boServiceReq 		= TheApp.GetBusObject(&quot;Service Request&quot;);
	bcServiceReq 		= boServiceReq.GetBusComp(&quot;Service Request&quot;);
		
	if (bcSR != &quot;&quot;)
	{
		with(bcServiceReq)
		{
			SetViewMode(AllView);
			ClearToQuery();
			SetSearchSpec (&quot;Id&quot;, bcSR);
   			ExecuteQuery(ForwardOnly);
		
			if (FirstRecord())
			{
				var sId = GetFieldValue (&quot;Id&quot;);
				var sSendBackId = GetFieldValue (&quot;VF Send Back Id&quot;);
				
				var oBS = TheApplication().GetService(&quot;VF Service Request User Prop BS&quot;);
				var oInputs = TheApplication().NewPropertySet();
				var oOutputs = TheApplication().NewPropertySet();
	
				oInputs.SetProperty(&quot;SendBackId&quot;,sSendBackId);
				oInputs.SetProperty(&quot;ObjectId&quot;,sId);
	
				oBS.InvokeMethod(&quot;SendBackToOwner&quot;,oInputs,oOutputs);
				TheApplication().SetProfileAttr(&quot;FromSendBackToOwnerId&quot;,sId);
				return(CancelOperation);
			}
		}
	}
}"

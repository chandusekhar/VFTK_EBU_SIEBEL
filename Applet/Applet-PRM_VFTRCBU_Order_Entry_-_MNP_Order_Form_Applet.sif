	  this.InvokeMethod(&quot;TELCOSubmitOrder&quot;);				
      /*END WA#39*/					
    }					
    /*** WA#18:20150114:MS: End block ***/ 	
  }
  finally
  {
  
	bsOrderMng = null;
	Inputs = null;
	Outputs = null;
	bcDocumentum = null;
	boDocumentum = null;
	ParBusComp = null;
	BcOrderEntryLI = null;
	bcOrder = null;
	InputsMSI_ICCID = null;
	OutputsMSI_ICCID  = null;
	InputsCheckOM = null;
	OutputsCheckOM = null;
	bcServiceReq = null;
	boServiceReq = null;
	BcOrderEntryOrders = null;
	BoOrderEntry = null;
	TheApp = null;
	thisBC = null;	
  }	

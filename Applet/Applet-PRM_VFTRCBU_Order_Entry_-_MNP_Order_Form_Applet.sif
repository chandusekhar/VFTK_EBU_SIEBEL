			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CallGIB"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CallGIB()
{
	try
	{
	var TheApp = TheApplication();
	var thisBC = this.BusComp();
	var bsWfrMgr : Service;
	var WFInputs : PropertySet;
	var WFOutputs  : PropertySet;
	if(TheApp.GetProfileAttr(&quot;SubmitOrderRejection&quot;)==&quot;Y&quot;)
	{
		var strProfile = TheApp.InvokeMethod(&quot;LookupValue&quot;,&quot;GIB_SCENARIO&quot;,&quot;GIBCustomerVer&quot;);
		var strOrderId = thisBC.GetFieldValue(&quot;Id&quot;);
		bsWfrMgr = TheApp.GetService(&quot;Workflow Process Manager&quot;);
	   	WFInputs = TheApp.NewPropertySet (); 
	   	WFOutputs = TheApp.NewPropertySet (); 
	    WFInputs.SetProperty(&quot;Account Id&quot;,thisBC.GetFieldValue(&quot;Account Id&quot;));
	   	WFInputs.SetProperty(&quot;Order Id&quot;,strOrderId);
	   	WFInputs.SetProperty(&quot;Message Type&quot;,&quot;VF Multiple Button Test&quot;);
	   	WFInputs.SetProperty(&quot;GIB Actual Company Name&quot;, thisBC.GetFieldValue(&quot;Account&quot;));
	   	WFInputs.SetProperty(&quot;GIB Actual Tax Office&quot;, thisBC.GetFieldValue(&quot;VF Customer Tax Office&quot;));
		WFInputs.SetProperty(&quot;GIB Actual Company Type&quot;, thisBC.GetFieldValue(&quot;Flag PF&quot;));
		WFInputs.SetProperty(&quot;GIB Actual Verification Status&quot;, thisBC.GetFieldValue(&quot;VF GIB Ver Status&quot;));
	   	WFInputs.SetProperty(&quot;DCA Profile&quot;, thisBC.GetFieldValue(&quot;VF Authorized Profile&quot;));
	   	WFInputs.SetProperty(&quot;Profile Mode&quot;,strProfile);
	   	WFInputs.SetProperty(&quot;InquiryType&quot;,&quot;Undetailed&quot;);
	   	WFInputs.SetProperty(&quot;ProcessName&quot;,&quot;VF New GIB WF&quot;);
	   	bsWfrMgr.InvokeMethod(&quot;RunProcess&quot;,WFInputs,WFOutputs);
	   	var OrderSubmit = WFOutputs.GetProperty(&quot;OrderSubmit&quot;);
	    if(OrderSubmit==&quot;Y&quot;)
			{
					thisBC.SetFieldValue(&quot;VF GIB Call&quot;,&quot;Y&quot;);
					thisBC.WriteRecord();
			}  
	   	TheApp.SetProfileAttr(&quot;SubmitOrderRejection&quot;,&quot;N&quot;);
	 }
	 }
	 finally
	 {	
	 	TheApp = null;
	 	thisBC = null;
	 	WFOutputs = null;
	 	WFInputs = null;
	 	bsWfrMgr = null;
	 
	 }
}"

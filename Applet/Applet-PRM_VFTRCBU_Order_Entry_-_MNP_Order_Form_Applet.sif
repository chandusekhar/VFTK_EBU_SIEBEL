			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="GenerateReferenceForCancellation"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function GenerateReferenceForCancellation()
{

    try
    {
        var TheApp = TheApplication();
        var thisBC = this.BusComp();

        var result = CheckReviseAllowance();

        if (result == &quot;KO&quot;)
        {
            var strMessageCategory = &quot;VF Message Category&quot;;
            var strCode = &quot;122&quot;;
            var sMsg = TheApp.LookupMessage(strMessageCategory, strCode);
            TheApp.RaiseErrorText(sMsg);
            return result;
        }

        //VFTRCBU-35100: Added for the &quot;Is Cancellable&quot;
        if (result == &quot;KO1&quot;)
        {
            var strMessageCategory = &quot;VFTRCBU User Defined Errors&quot;;
            var strCode = &quot;0298&quot;;
            var sMsg = TheApp.LookupMessage(strMessageCategory, strCode);
            TheApp.RaiseErrorText(sMsg);
            return result;
        }
        //End		

        var today: Date = new Date;
        var daysToAdd = 2;
        var dDate: Date = new Date(today.getTime() + daysToAdd * 24 * 60 * 60 * 1000);

        var myBS: Service = TheApp.GetService(&quot;ACS Sequence Generator&quot;);
        var inputs: PropertySet = TheApp.NewPropertySet();
        var outputs: PropertySet = TheApp.NewPropertySet();

        inputs.SetProperty(&quot;SequenceType&quot;, &quot;REQ&quot;);
        myBS.InvokeMethod(&quot;GetSequence&quot;, inputs, outputs);
        var Out = outputs.GetProperty(&quot;Sequence&quot;);

        var year = dDate.getFullYear();
        thisBC.SetFieldValue(&quot;Reference Number Canc&quot;, &quot;VODAFONE-&quot; + year + &quot;-&quot; + Out);
        thisBC.SetFieldValue(&quot;Revising&quot;, &quot;Y&quot;);
        thisBC.WriteRecord();

        TheApp.SetSharedGlobal(&quot;CANINVOKEREVISE&quot;, &quot;FALSE&quot;);
        return result;

    }
    finally
    {

        TheApp = null;
        thisBC = null;
        outputs = null;
        inputs = null;
        myBS = null;
        today = null;
        dDate = null;
    }
    return result;
}"

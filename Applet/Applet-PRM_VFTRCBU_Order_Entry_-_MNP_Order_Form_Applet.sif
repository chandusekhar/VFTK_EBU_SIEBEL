			<APPLET_BROWSER_SCRIPT
			   BROWSER_GROUP="All"
			   INACTIVE="N"
			   NAME="Applet_Load"
			   NEEDS_HIGH_INTERACTIVITY="Y"
			   PROCEDURE="Applet_Load"
			   SCRIPT="function Applet_Load ()
{
	try
	{
		var strStatus = this.BusComp().GetFieldValue(&quot;Status&quot;);
		var svc_ACSNIGeneralOperations = theApplication().GetService (&quot;ACS NI General Operations&quot;);
		var pset_Inputs1  = theApplication().NewPropertySet();
		var pset_Outputs1 = theApplication().NewPropertySet();
		
		var cancelledStatus = &quot;&quot;;
		pset_Inputs1.SetProperty(&quot;LOV Type&quot;, &quot;FS_ORDER_STATUS&quot;);
		pset_Inputs1.SetProperty(&quot;LOV Name&quot;, &quot;Cancelled&quot;);
		pset_Outputs1 = svc_ACSNIGeneralOperations.InvokeMethod(&quot;GetLookupValue&quot;, pset_Inputs1);
					
		cancelledStatus = pset_Outputs1.GetProperty(&quot;LOV Value&quot;);
		
		if(strStatus == cancelledStatus)
		theApplication().SetProfileAttr(&quot;CancelledOrder&quot;, &quot;Y&quot;);
		else
		theApplication().SetProfileAttr(&quot;CancelledOrder&quot;, &quot;N&quot;);
		var strshowpopup = TheApplication().GetProfileAttr(&quot;ShowOrderPopup&quot;);
		//Setted in Application Prenavigate
		if(strshowpopup == &quot;Y&quot;)
		{
			TheApplication().SetProfileAttr(&quot;ShowOrderPopup&quot;, &quot;N&quot;);
			var pset_Inputs  = theApplication().NewPropertySet();
			var pset_Outputs = theApplication().NewPropertySet();
			var str_Message = &quot;&quot;;
				
			pset_Inputs.SetProperty(&quot;Code&quot;, &quot;072&quot;);
			pset_Inputs.SetProperty(&quot;MessageCategory&quot;, &quot;ACS Messages&quot;);
			pset_Outputs = svc_ACSNIGeneralOperations.InvokeMethod(&quot;GetLookupMsg&quot;, pset_Inputs);
					
			str_Message = pset_Outputs.GetProperty(&quot;Message&quot;);
			
			theApplication().SWEAlert(str_Message);
		}	
	}
	finally
	{
		svc_ACSNIGeneralOperations = null;
		pset_Inputs  = null;
		pset_Outputs = null;		
		pset_Inputs1  = null;
		pset_Outputs1 = null;	
	}
}"

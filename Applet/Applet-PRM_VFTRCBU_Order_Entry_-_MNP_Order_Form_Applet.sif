			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CanInvokeCancelOrder"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CanInvokeCancelOrder()
{
	try
	{
		var bsService:Service;
		var psInputs:PropertySet;
		var psOutputs:PropertySet;
		
		bsService = TheApplication().GetService(&quot;ACS General Operations&quot;);
		psInputs = TheApplication().NewPropertySet();
		psOutputs = TheApplication().NewPropertySet();
		
		var sStatus = this.BusComp().GetFieldValue(&quot;Status&quot;);
		var sStatusComplete = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;FS_ORDER_STATUS&quot;, &quot;Complete&quot;);
		var sStatusCancelled = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;FS_ORDER_STATUS&quot;, &quot;Cancelled&quot;);
		var sInvoke = &quot;&quot;;
			
		if (sStatus == sStatusComplete || sStatus == sStatusCancelled)
		{
			sInvoke = &quot;FALSE&quot;;
		}
		else 
		{	
			var activeView = TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
			var isReadOnly = TheApplication().InvokeMethod(&quot;IsViewReadOnly&quot;, activeView);
		
			 if (isReadOnly != &quot;TRUE&quot;) 
			 {        
				psInputs.SetProperty (&quot;Function&quot;, &quot;0010Cancel Order&quot;);
				psInputs.SetProperty (&quot;Name&quot;, &quot;Cancel Order&quot;);
				psInputs.SetProperty (&quot;Value&quot;, &quot;&quot;);
	
				bsService.InvokeMethod (&quot;GetParameter_3Inputs&quot;, psInputs, psOutputs);
				var buttonVisibility = psOutputs.GetProperty (&quot;Cons1&quot;);
		        
				if (buttonVisibility==&quot;False&quot; || buttonVisibility==&quot;&quot;) 
				{
					sInvoke = &quot;FALSE&quot;;
				}
	    		else 
			    {
			   		sInvoke = &quot;TRUE&quot;;
				}				
			}
			else 
			{
				sInvoke = &quot;FALSE&quot;;
			}
		}
		//Clear Vars
		psOutputs = null;
		psInputs = null;
		bsService = null;
		
		return sInvoke;
	}
	catch(e)
	{
		throw(e);
	}
}"

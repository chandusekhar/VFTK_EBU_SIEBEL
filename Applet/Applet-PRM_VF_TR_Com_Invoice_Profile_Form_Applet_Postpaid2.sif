			</APPLET_SERVER_SCRIPT>
			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="WebApplet_Load"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_Load ()
{
	try
	{
		var boBillingAccount : BusObject = this.BusObject();
		var bcBillingAccount : BusComp   = boBillingAccount.GetBusComp(&quot;VF PRM Billing Account&quot;);
		
		//BEGIN - JFONSECA ++
		
		var BAId = TheApplication().GetProfileAttr(&quot;NBABillingAccountid&quot;);
		
		with (bcBillingAccount)
			{
			   SetViewMode(AllView);
			   SetSearchSpec (&quot;Id&quot;, BAId);
			   ClearToQuery();
			   ExecuteQuery(ForwardBackward);
			   if (FirstRecord())
			   {
			   		var sTaxId = GetFieldValue(&quot;Tax Id&quot;);
			   		var sMediaType = GetFieldValue(&quot;VF Media Type CD&quot;);
			   }
			  }
		
		//END - JFONSECA

		//var sTaxId     = bcBillingAccount.GetFieldValue(&quot;Tax Id&quot;); //COMMENTED JFONSECA
		//var sMediaType = bcBillingAccount.GetFieldValue(&quot;VF Media Type CD&quot;); //COMMENTED JFONSECA

		var sMediaTypeMail      = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;BILL_MEDIA_TYPE&quot;,&quot;104&quot;);
		var sMediaTypeMOF       = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;BILL_MEDIA_TYPE&quot;,&quot;105&quot;);
		var sMediaTypeMOFReport = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;BILL_MEDIA_TYPE&quot;,&quot;107&quot;);

		if (sTaxId == &quot;NA&quot;)
		{
			customer_status = &quot;NORMAL&quot;;
			TheApplication().SetProfileAttr(&quot;MediaTypePreDefault&quot;, sMediaTypeMail);
			TheApplication().SetProfileAttr(&quot;MOFResult&quot;,           &quot;NORMAL&quot;);
			//this.BusComp().SetFieldValue(&quot;Media Type&quot;, sMediaTypeMail); //104= Mail /Posta
			//this.BusComp().WriteRecord();
			return;
		}

		customer_status = CheckMOF(sTaxId);
	                    	
		if (sMediaType == sMediaTypeMOF || sMediaType == sMediaTypeMOFReport) 
		{
			TheApplication().SetProfileAttr(&quot;MOFResult&quot;, &quot;MOF&quot;);
			if (customer_status == &quot;ERROR&quot;)
				customer_status = &quot;MOF&quot;;
		}
		else 
		{
			TheApplication().SetProfileAttr(&quot;MOFResult&quot;, &quot;NORMAL&quot;);
			if (customer_status == &quot;ERROR&quot;)
				customer_status = &quot;NORMAL&quot;; 
		}

		// Set predefault value for component
		if (customer_status==&quot;NORMAL&quot;)
			TheApplication().SetProfileAttr(&quot;MediaTypePreDefault&quot;, sMediaTypeMail);
		if (customer_status==&quot;MOF&quot;)
			TheApplication().SetProfileAttr(&quot;MediaTypePreDefault&quot;, sMediaTypeMOF);

		if (customer_status == &quot;MOF&quot; &amp;&amp; (sMediaType != sMediaTypeMOF &amp;&amp; sMediaType != sMediaTypeMOFReport))
		{
			TheApplication().SetProfileAttr(&quot;MOFResult&quot;, &quot;MOF&quot;);
			this.BusComp().SetFieldValue(&quot;Media Type&quot;, sMediaTypeMOF); //105=Maliye Bakanlığı/ Ministry of Finance
			this.BusComp().WriteRecord();
		}
		else if (customer_status == &quot;NORMAL&quot; &amp;&amp; (sMediaType == sMediaTypeMOF || sMediaType == sMediaTypeMOFReport))
		{
			TheApplication().SetProfileAttr(&quot;MOFResult&quot;, &quot;NORMAL&quot;);
			this.BusComp().SetFieldValue(&quot;Media Type&quot;, sMediaTypeMail); 
			this.BusComp().WriteRecord(); 
		}
    }
	catch(e) 
	{
	}
	finally
	{
		bcBillingAccount = null;
		boBillingAccount = null;
	}	
}"

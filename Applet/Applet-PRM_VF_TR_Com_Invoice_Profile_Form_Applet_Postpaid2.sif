			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CheckMOF"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CheckMOF(sTaxId)
{
	//TC-5996 E-ARCHIVE
	try 
	{
		var bsManager    : Service     = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
		var psManagerIn  : PropertySet = TheApplication().NewPropertySet();
		var psManagerOut : PropertySet = TheApplication().NewPropertySet();

		psManagerIn.SetProperty(&quot;ProcessName&quot;,    &quot;VF MOFInquiry WF&quot;);
		psManagerIn.SetProperty(&quot;TAX_ID&quot;,         sTaxId);
		psManagerIn.SetProperty(&quot;CHANNEL&quot;,        &quot;SIEBEL&quot;);
		psManagerIn.SetProperty(&quot;TRANSACTION_ID&quot;, &quot;&quot;);

		bsManager.InvokeMethod(&quot;RunProcess&quot;, psManagerIn, psManagerOut);
				
		customer_status = psManagerOut.GetProperty(&quot;MOF_STATUS&quot;);
				
		return customer_status;
	}
	catch(e)
	{
		customer_status = &quot;ERROR&quot;;
		return customer_status;
	}
	finally
	{
		psManagerOut = null;
		psManagerIn  = null;
		bsManager    = null;
	}
}"
			   SEQUENCE="2"
			   UPDATED="08/06/2015 23:35:38"

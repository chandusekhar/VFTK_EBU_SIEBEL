			<APPLET_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="WebApplet_PreCanInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function WebApplet_PreCanInvokeMethod (MethodName, &amp;CanInvoke)
{
	try
	{
		var bsOperations    : Service     = null;
		var psOperationsIn  : PropertySet = null;
		var psOperationsOut : PropertySet = null;

		var sButtonEnabled = &quot;&quot;;

		if (MethodName == &quot;EditRecord&quot;)
		{
			bsOperations    = TheApplication().GetService(&quot;ACS General Operations&quot;);
			psOperationsIn  = TheApplication().NewPropertySet();
			psOperationsOut = TheApplication().NewPropertySet();

			psOperationsIn.SetProperty(&quot;Function&quot;, &quot;PRMEditRecord&quot;);
			psOperationsIn.SetProperty(&quot;Name&quot;,     &quot;Modify&quot;);

			bsOperations.InvokeMethod(&quot;GetParameter_3Inputs&quot;, psOperationsIn, psOperationsOut);

			sButtonEnabled = psOperationsOut.GetProperty(&quot;Cons1&quot;);

			if (sButtonEnabled == &quot;True&quot;)
				CanInvoke = &quot;TRUE&quot;;
			else
				CanInvoke = &quot;FALSE&quot;;

			return (CancelOperation);
		}

		return (ContinueOperation);
	}
	finally
	{
		psOperationsOut = null;
		psOperationsIn  = null;
		bsOperations    = null;
	}

	return (ContinueOperation);
}"

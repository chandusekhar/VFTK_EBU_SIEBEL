			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="MeropolisControl"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function 	MeropolisControl() // WP1 YGhazi 07/06/2011
{
	try
	{
		var AssetBO	: BusObject	= TheApplication().GetBusObject(&quot;X Order Entry&quot;);
		var AssetBC	: BusComp	= AssetBO.GetBusComp(&quot;HP Submit - Asset Mgmt - Asset - Header&quot;);
		var BcOrderEntry : BusComp		= AssetBO.GetBusComp(&quot;HP Submit - Order Entry - Orders&quot;);
		var BcOrderEntryLI : BusComp 		= AssetBO.GetBusComp(&quot;HP Submit - Order Entry - Line Items&quot;);
		var AccountBO : BusObject = TheApplication().GetBusObject(&quot;Account&quot;);
		var InvProfileBC : BusComp =  AccountBO.GetBusComp(&quot;Billing Account&quot;);
		var strAdd 			= TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;DELTA_ACTION_CODE&quot;,&quot;New&quot;);
		var strPending = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Pending&quot;);
		var strComplete = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;FS_ORDER_STATUS&quot;, &quot;Complete&quot;);
		var strStatus = &quot;[Status] &lt;&gt; &apos;&quot; + strPending + &quot;&apos; AND  [Status] &lt;&gt; &apos;&quot; + strComplete + &quot;&apos;&quot;;
		var strOrderId = &quot;&quot;;
		var strBilAccountId = null;//this.GetFieldValue(&quot;Billing Account Id&quot;);
		var strActive = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;IMPL_PHASE&quot;, &quot;Active&quot;);
		var profileID = this.GetFieldValue(&quot;Com Invoice Profile Id&quot;);
		
		with(InvProfileBC){
			ClearToQuery();
			SetViewMode(AllView);
			SetSearchSpec(&quot;Billing Profile Id&quot;,profileID);
			ExecuteQuery(ForwardOnly);
			if (FirstRecord()){
				strBilAccountId = InvProfileBC.GetFieldValue(&quot;Id&quot;);
				TheApplication().SetProfileAttr(&quot;billAcc&quot;,strBilAccountId);
			}
		}

		with(BcOrderEntryLI)
		{
			ClearToQuery();
			SetViewMode(AllView);
			SetSearchSpec(&quot;Billing Account Id&quot;,strBilAccountId);
			SetSearchSpec (&quot;Part Number&quot;, &quot;14100&quot;); 
			SetSearchSpec(&quot;Action Code&quot;, strAdd);
			ExecuteQuery(ForwardOnly);	
			if (FirstRecord())
				TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;R142_010&quot;));
		}
	
		with(AssetBC)
		{
			ClearToQuery();
			SetViewMode(AllView);
			SetSearchSpec(&quot;Billing Account Id&quot;,strBilAccountId)
	 		SetSearchSpec (&quot;Product Part Number&quot;, &quot;14100&quot;); 
	 		SetSearchSpec(&quot;Status&quot;, strActive);
			ExecuteQuery(ForwardOnly);	
	 		if (FirstRecord())
	 			TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;R142_010&quot;));
		}
	}
	finally
	{
		InvProfileBC = null;
		AssetBC		= null;
		BcOrderEntry = null;
		BcOrderEntryLI  = null;
		AssetBO		= null;
		AccountBO = null;
	}

}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="CancelRequest"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function CancelRequest()
{
//Oracle Review October 2011 - Call from PreInvokeMethod

	//Added VFTRCBU WA#04 - JFONSECA 12.02.2015
	var bssvcWFProcMgr :Service = TheApplication().GetService(&quot;Workflow Process Manager&quot;); 
	var ps_inputsBillCycleChange :PropertySet = TheApplication().NewPropertySet();
	var ps_outputsBillCycleChange :PropertySet = TheApplication().NewPropertySet();
		
	var strId = this.GetFieldValue(&quot;Id&quot;);
	
	this.ActivateField(&quot;VFTRCBU Order Id&quot;);
	var strCancelledStatus = TheApplication().InvokeMethod (&quot;LookupValue&quot;, &quot;VF_REQ_STATUS&quot;, &quot;Cancelled&quot;);
	var strOrderID = this.GetFieldValue(&quot;VFTRCBU Order Id&quot;); //VFTRCBU WA#04 - JFONSECA 12.02.2015
	if (strOrderID == &quot;&quot;)
	{
		this.SetFieldValue(&quot;VF Request Status&quot;,strCancelledStatus);
		return (CancelOperation);
	}
	this.ActivateField(&quot;VF Billing Account Code&quot;);
	var strBillingAccountID = this.GetFieldValue(&quot;VF Billing Account Code&quot;); //VFTRCBU WA#04 - JFONSECA 19.02.2015
	var strReqStatus = this.GetFieldValue(&quot;VF Request Status&quot;);
	if(strReqStatus != TheApplication().InvokeMethod (&quot;LookupValue&quot;, &quot;VF_REQ_STATUS&quot;, &quot;Completed&quot;)&amp;&amp;
		strReqStatus != strCancelledStatus)
 	{
 		ps_inputsBillCycleChange.SetProperty(&quot;Order Id&quot;,strOrderID);
 		ps_inputsBillCycleChange.SetProperty(&quot;Request Id&quot;,strId);
 		ps_inputsBillCycleChange.SetProperty(&quot;Billing Account Id&quot;,strBillingAccountID);
		ps_inputsBillCycleChange.SetProperty(&quot;ProcessName&quot;,&quot;VFTRCBU Cancel Bill Cycle Change Request WF&quot;);
		bssvcWFProcMgr.InvokeMethod(&quot;RunProcess&quot;,ps_inputsBillCycleChange,ps_outputsBillCycleChange);
 		//Commented VFTRCBU WA#04 - JFONSECA 12.02.2015
		//this.SetFieldValue(&quot;VF Request Status&quot;, strCancelledStatus);
		//this.WriteRecord();
	}
}"

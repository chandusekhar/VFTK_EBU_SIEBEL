			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreNewRecord"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreNewRecord() {
	try {
		if (FirstRecord()) {
			var LstBillCycleChange = this.GetFieldValue(&quot;VF Com Inv Date&quot;);
			var Interval = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;VF_BC_CHANGE_INTERVAL&quot;, &quot;Interval&quot;);
			var boQuery = TheApplication().GetBusObject(&quot;VFTRCBU SIM Card BO&quot;);
			var bcQuery = boQuery.GetBusComp(&quot;VFTRCBU Role PickList Generic&quot;);

			if (LstBillCycleChange != &quot;&quot;) {
				//Operations with Dates in order to prevent a Bill Cycle Change Request before a determined Interval of days:
				//Get Date value from field
				var VFInterval = this.GetFieldValue(&quot;VF Interval&quot;);
				//Assign it to Date Object
				var sDate1 = new Date(VFInterval);
				//Convert date object to integer equivalent
				var sInterval = sDate1.toSystem();
				//find time difference between current time and date opened in seconds
				var sTimeDiff = Clib.difftime(sInterval, Clib.time());

				if (sTimeDiff &gt; 0) {
				
					//VFTRCBU-27821 - ACAMBAS - Start
					/*var strMessageCategory = &quot;VF Message Category&quot;;
					var strCode = &quot;136&quot;;
					var sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
					var strCode1 = &quot;164&quot;;
					var sMsg1 = TheApplication().LookupMessage(strMessageCategory, strCode1);*/
					var sMsg = TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;BC_CHANGE_ERROR_1&quot;, Interval);
					//VFTRCBU-27821 - ACAMBAS - End

					//VFTRCBU-27393 - HRODRIGUES
					var strSearchExpressionBypass = &quot;[VFTRCBU Type] = &apos;BILL_CYCLE_NEW_BUTTON_BYPASS&apos; &quot;;
					bcQuery.SetViewMode(AllView);
					bcQuery.ActivateField(&quot;VFTRCBU Type&quot;);
					bcQuery.ClearToQuery();
					bcQuery.SetSearchExpr(strSearchExpressionBypass);
					bcQuery.ExecuteQuery(ForwardOnly);
					var existsRecordBypass = bcQuery.FirstRecord();

					if (!existsRecordBypass) {
						//VFTRCBU-27821 - ACAMBAS - Start
						//TheApplication().RaiseErrorText(sMsg + &quot; &quot; + Interval + &quot; &quot; + sMsg1);
						TheApplication().RaiseErrorText(sMsg);
						//VFTRCBU-27821 - ACAMBAS - End
					}
				}
			}
		}
		return (ContinueOperation);
	} catch (e) {
		throw e;
	}
	finally {
		bcQuery = null;
		boQuery = null;
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="Confirm"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function Confirm()
{
	try
	{
		var app					= TheApplication();
		var bsCheckOrder		: Service = app.GetService(&quot;VF Check Order&quot;);
		var ACSNI 				: Service = app.GetService(&quot;ACS NI General Operations&quot;); //Added WA#04 - JFONSECA - 31.03.2015
		var bsInputs			:PropertySet = app.NewPropertySet();
		var bsOutputs			:PropertySet = app.NewPropertySet();
		//var wSSh = COMCreateObject(&quot;WScript.Shell&quot;); VFTRCBU-10525 | TiagoRocha
		var strAccId			= &quot;&quot;;
		var strCheck			= &quot;&quot;;
		var strMessageCategory;
		var strCode;
		var sMsg;
		var aux = &quot;&quot;;
		var strChangeBillCycleID = &quot;&quot;; //Added WA#04 - JFONSECA - 02.03.2015
		var strAccountID = &quot;&quot;; //Added WA#04 - JFONSECA - 02.03.2015
		var strBillingFrequency = &quot;&quot;; //Added WA#04 - JFONSECA - 02.03.2015
		var strNow = &quot;&quot;;
		var psIn  : PropertySet = TheApplication().NewPropertySet(); //Added WA#04 - JFONSECA - 31.03.2015
        var psOut : PropertySet = TheApplication().NewPropertySet(); //Added WA#04 - JFONSECA - 31.03.2015
        		
		var bssvcWFProcMgr :Service = TheApplication().GetService(&quot;Workflow Process Manager&quot;);  //Added WA#04 - JFONSECA - 02.03.2015
		var ps_inputsBillCycleChange :PropertySet = TheApplication().NewPropertySet(); //Added WA#04 - JFONSECA - 02.03.2015
		var ps_outputsBillCycleChange :PropertySet = TheApplication().NewPropertySet(); //Added WA#04 - JFONSECA - 02.03.2015
		
		//Added WA#04 - JFONSECA - 27.03.2015 - From Here
		var strToday = new Date;
		var strYear = strToday.getFullYear();
		var strMonth = strToday.getMonth() + 1;		
		if (strMonth == &quot;12&quot;)
		{
			var strNextYear = strToday.getFullYear() + 1;
			var strNextMonth = &quot;01&quot;;
		}
		else
		{
			var strNextMonth = strToday.getMonth() + 2;	//VFTRCBU-35208
			if (strNextMonth &lt; 10) {strNextMonth = &quot;0&quot; + strNextMonth;}
		}
		
		if (strMonth &lt; 10) {strMonth = &quot;0&quot; + strMonth;}
		var strDate = strToday.getDate();
		if (strDate &lt; 10) {strDate = &quot;0&quot; + strDate;}
		var strHour = strToday.getHours();
		if (strHour &lt; 10) {strHour = &quot;0&quot; + strHour;}
		var strMinute = strToday.getMinutes();
		if (strMinute &lt; 10) {strMinute = &quot;0&quot; + strMinute;}
		var strSecond = strToday.getSeconds();
		if (strSecond &lt; 10) {strSecond = &quot;0&quot; + strSecond;}
		
		strNow = strMonth + &quot;/&quot; + strDate + &quot;/&quot; + strYear + &quot; &quot; + strHour + &quot;:&quot; + strMinute + &quot;:&quot; + strSecond;
		var CurBillCycle = this.ParentBusComp().GetFieldValue(&quot;Bill Cycle&quot;); //VFTRCBU-13148
		var strCustType = this.ParentBusComp().GetFieldValue(&quot;VFTRCBU Owner Account Type&quot;);
		var strCustCBU = TheApplication().InvokeMethod (&quot;LookupValue&quot;, &quot;ACCOUNT_TYPE&quot;, &quot;102&quot;);
		var strCustEBU = TheApplication().InvokeMethod (&quot;LookupValue&quot;, &quot;ACCOUNT_TYPE&quot;, &quot;101&quot;);	
		
		//BillCycle = 1, is a special Bill Cycle, should be calculated differently. //START
		var aux = 0;
		if(CurBillCycle == 1)
		{
			var retDate = new Date(strNow);
				if(strMonth == &quot;11&quot;)
				{
					strMonth = (retDate.getMonth() + 2);
					strNextMonth = &quot;01&quot;;
					strYear = (retDate.getFullYear());
					strNextYear = (retDate.getFullYear() + 1);
				}
				else if(strMonth == &quot;12&quot;)
				{
					strMonth = &quot;01&quot;;
					strNextMonth = &quot;02&quot;;
					strYear = (retDate.getFullYear());
					strNextYear = (retDate.getFullYear() + 1);
					var aux = 1;
				}
				else{
				strMonth = (retDate.getMonth() + 2);
				strNextMonth = (retDate.getMonth() + 3);
				strYear = strYear;
				}
		}
		//END
		
		//Calculate Day Before CurBillCycle -&gt; 24 Special Hours
		
		var strCurBillCycleAUX = strMonth + &quot;/&quot; + CurBillCycle + &quot;/&quot; + strYear + &quot; &quot; + &quot;00:00:00&quot;;
		var retDate = new Date(strCurBillCycleAUX);
		retDate.setDate(retDate.getDate() - 1);
		var strCurBillCycleAUX = (retDate.getMonth() + 1) + &quot;/&quot; + (retDate.getDate()) + &quot;/&quot; + (retDate.getFullYear()) + &quot; &quot; + &quot;00:00:00&quot;; 
		var CurBillCycleLess1 = (retDate.getDate());
		var strCheckDate2 = strDate - CurBillCycleLess1;
			
		if(strDate &gt;= CurBillCycleLess1)
		{
			if(strMonth == &quot;12&quot;)
			{
				strNextMonth = &quot;01&quot;;
				var strCurBillCycle = strNextMonth + &quot;/&quot; + CurBillCycle + &quot;/&quot; + strNextYear + &quot; &quot; + &quot;00:00:00&quot;; //VFTRCBU-13148
			}
			else
			{
				var strCurBillCycle = strNextMonth + &quot;/&quot; + CurBillCycle + &quot;/&quot; + strYear + &quot; &quot; + &quot;00:00:00&quot;; //VFTRCBU-13148
			}
		}

		else
		{
			if(aux == &quot;1&quot;)
			{
			var strCurBillCycle = strMonth + &quot;/&quot; + CurBillCycle + &quot;/&quot; + strNextYear + &quot; &quot; + &quot;00:00:00&quot;; //VFTRCBU-13148
			}
			else{
			var strCurBillCycle = strMonth + &quot;/&quot; + CurBillCycle + &quot;/&quot; + strYear + &quot; &quot; + &quot;00:00:00&quot;; //VFTRCBU-13148
			}
		}	
		
		
		//if ((strCheckDate1 == &quot;0&quot;) || (strCheckDate2 == &quot;0&quot;)) //JFONSECA
		if (strCheckDate2 == &quot;0&quot;)
		{
			if(strCustType == strCustCBU)
			{
				
				//var strDayBefore = strNextMonth + &quot;/&quot; + (CurBillCycle - 1) + &quot;/&quot; + strYear + &quot; &quot; + 00 + &quot;:&quot; + 00 + &quot;:&quot; + 00;
				
				var retDate = new Date(strCurBillCycle);
				retDate.setDate(retDate.getDate() - 1);
				var strDayBefore = (retDate.getMonth() + 1) + &quot;/&quot; + (retDate.getDate()) + &quot;/&quot; + (retDate.getFullYear()) + &quot; &quot; + &quot;00:00:00&quot;; //VFTRCBU-13148
				
				ps_inputsBillCycleChange.SetProperty(&quot;OrderDueDate&quot;, strDayBefore); 
				ps_inputsBillCycleChange.SetProperty(&quot;BillCycleDueDate&quot;, strCurBillCycle); 
			}
			
			else if(strCustType == strCustEBU)
			{
				ps_inputsBillCycleChange.SetProperty(&quot;OrderDueDate&quot;, strCurBillCycle); 
				ps_inputsBillCycleChange.SetProperty(&quot;BillCycleDueDate&quot;, strCurBillCycle); 
			}
			
			TheApplication().SetProfileAttr(&quot;VFTRCBU CheckDate&quot;, &quot;Y&quot;);
			/* VFTRCBU-10525 - Script Depricated due to Unix Server incapability to complete this process
			strMessageCategory = &quot;VFTRCBU User Defined Errors&quot;;
	 		strCode = &quot;0286&quot;;
			sMsg = app.LookupMessage(strMessageCategory, strCode);
	 		wSSh.Popup(sMsg);*/
		}
		
		else
		{
			if(strCustType == strCustCBU)
			{
				ps_inputsBillCycleChange.SetProperty(&quot;OrderDueDate&quot;, strNow); 
				ps_inputsBillCycleChange.SetProperty(&quot;BillCycleDueDate&quot;, strCurBillCycle); 
			}
			else if(strCustType == strCustEBU)
			{
				ps_inputsBillCycleChange.SetProperty(&quot;OrderDueDate&quot;, strCurBillCycle); 
				ps_inputsBillCycleChange.SetProperty(&quot;BillCycleDueDate&quot;, strCurBillCycle); 
			}
		}
		
		//Added WA#04 - JFONSECA - 27.03.2015 - To Here
		this.InvokeMethod(&quot;RefreshBusComp&quot;);
	 	strAccId	= this.GetFieldValue(&quot;Billing Account Id&quot;);
		bsInputs.SetProperty(&quot;Billing Account Id&quot;,strAccId);
		bsCheckOrder.InvokeMethod(&quot;checkBillingAccountStatus&quot;,bsInputs,bsOutputs);
		strCheck = bsOutputs.GetProperty(&quot;Result&quot;);		
		if (strCheck==&quot;KO&quot;)
		{
			strMessageCategory = &quot;VF Message Category&quot;;
	 		strCode = &quot;137&quot;;
			sMsg = app.LookupMessage(strMessageCategory, strCode);
	 		app.RaiseErrorText(sMsg);
		}
		var NewBillCycle=this.GetFieldValue(&quot;VF New Bill Cycle Value&quot;);
		//var CurBillCycle=this.GetFieldValue(&quot;Bill Cycle&quot;);
		var strOldValue = app.InvokeMethod (&quot;LookupValue&quot;, &quot;VF_BC_DISPLAY&quot;, CurBillCycle);
		var strNewValueDisplayed = this.GetFieldValue(&quot;VF New Bill Cycle Displayed&quot;);
		var strNewBillCycle = this.GetFieldValue(&quot;VF New Bill Cycle Value&quot;); //Added WA#04 - JFONSECA - 31.03.2015
		var strChangeBillCycleID = this.GetFieldValue(&quot;Id&quot;); //Added WA#04 - JFONSECA - 02.03.2015
		var strAccountID = this.GetFieldValue(&quot;VF Billing Account Code&quot;); //Added WA#04 - JFONSECA - 02.03.2015
		var strBillingFrequency = this.ParentBusComp().GetFieldValue(&quot;Bill Frequency&quot;); //Added WA#04 - JFONSECA - 02.03.2015
		
		psIn.SetProperty(&quot;LOV&quot;,&quot;BILL_FREQUENCY&quot;); //Added WA#04 - JFONSECA - 16.07.2015
		psIn.SetProperty(&quot;Val&quot;,strBillingFrequency); //Added WA#04 - JFONSECA - 16.07.2015
		var strAux2 = ACSNI.InvokeMethod (&quot;LookupName&quot;, psIn, psOut); //Added WA#04 - JFONSECA - 16.07.2015
		var strBillingFrequencyLOV = psOut.GetProperty(&quot;Name&quot;); //Added WA#04 - JFONSECA - 16.07.2015
		
		var strBillTypeInvoice = this.ParentBusComp().GetFieldValue(&quot;Bill Type&quot;); //Added WA#04 - JFONSECA - 17.07.2015
		
		psIn.SetProperty(&quot;LOV&quot;,&quot;BILL_TYPE&quot;); //Added WA#04 - JFONSECA - 17.07.2015
		psIn.SetProperty(&quot;Val&quot;,strBillTypeInvoice); //Added WA#04 - JFONSECA - 17.07.2015
		var strAux3 = ACSNI.InvokeMethod (&quot;LookupName&quot;, psIn, psOut); //Added WA#04 - JFONSECA - 17.07.2015
		var strBillTypeInvoiceLOV = psOut.GetProperty(&quot;Name&quot;); //Added WA#04 - JFONSECA - 17.07.2015
		
		var strBillType = this.ParentBusComp().GetFieldValue(&quot;TeXCo Account Type&quot;); //Added WA#04 - JFONSECA - 31.03.2015
		psIn.SetProperty(&quot;LOV&quot;,&quot;X_PAYMENT_MODALITY&quot;); //Added WA#04 - JFONSECA - 17.07.2015
		psIn.SetProperty(&quot;Val&quot;,strBillType); //Added WA#04 - JFONSECA - 17.07.2015
		var strAux4 = ACSNI.InvokeMethod (&quot;LookupName&quot;, psIn, psOut); //Added WA#04 - JFONSECA - 17.07.2015
		strBillType = psOut.GetProperty(&quot;Name&quot;); //Added WA#04 - JFONSECA - 17.07.2015
		
		this.ParentBusComp().ActivateField(&quot;Customer Account Number&quot;);
		var strCustomerId = this.ParentBusComp().GetFieldValue(&quot;Customer Account Number&quot;); //Added WA#04 - JFONSECA - 10.04.2015
		var strObjectId = this.ParentBusComp().GetFieldValue(&quot;Id&quot;); //Added WA#04 - JFONSECA - 15.04.2015
		
		psIn.SetProperty(&quot;LOV&quot;,&quot;BILL_CYCLE&quot;); //Added WA#04 - JFONSECA - 31.03.2015
		psIn.SetProperty(&quot;Val&quot;,strNewBillCycle); //Added WA#04 - JFONSECA - 31.03.2015
		var strAux = ACSNI.InvokeMethod (&quot;LookupName&quot;, psIn, psOut); //Added WA#04 - JFONSECA - 31.03.2015
		var strCurBillCycleLOV = psOut.GetProperty(&quot;Name&quot;); //Added WA#04 - JFONSECA - 31.03.2015
		if( strOldValue == strNewValueDisplayed || NewBillCycle==CurBillCycle)			
	  	{
			strMessageCategory = &quot;VF Message Category&quot;;
		 	strCode = &quot;135&quot;;
			sMsg = app.LookupMessage(strMessageCategory, strCode);
	 		app.RaiseErrorText(sMsg);
	  	}
	  	//var strParked =  app.InvokeMethod(&quot;LookupValue&quot;,&quot;VF_REQ_STATUS&quot;, &quot;Parked&quot;);
	  	//this.SetFieldValue(&quot;VF Request Status&quot;, strParked);
	  	//this.WriteRecord();
	  	ps_inputsBillCycleChange.SetProperty(&quot;ChangeBillCycleID&quot;, strChangeBillCycleID); //Added WA#04 - JFONSECA - 02.03.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;AccountID&quot;, strAccountID); //Added WA#04 - JFONSECA - 02.03.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;BillPeriod&quot;, strCurBillCycleLOV); //Added WA#04 - JFONSECA - 02.03.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;BillingFrequency&quot;, strBillingFrequencyLOV); //Added WA#04 - JFONSECA - 02.03.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;CurBillCycle&quot;, CurBillCycle); //Added WA#04 - JFONSECA - 25.03.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;BillAccountType&quot;, strBillType); //Added WA#04 - JFONSECA - 31.03.2015
		ps_inputsBillCycleChange.SetProperty(&quot;BillType&quot;, strBillTypeInvoiceLOV); //Added WA#04 - JFONSECA - 17.07.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;CustomerId&quot;, strCustomerId); //Added WA#04 - JFONSECA - 10.04.2015
	  	ps_inputsBillCycleChange.SetProperty(&quot;Object Id&quot;, strObjectId); //Added WA#04 - JFONSECA - 15.04.2015
		ps_inputsBillCycleChange.SetProperty(&quot;ProcessName&quot;,&quot;VFTRCBU Submit Bill Cycle Change Request WF&quot;); //Added WA#04 - JFONSECA - 02.03.2015
		bssvcWFProcMgr.InvokeMethod(&quot;RunProcess&quot;,ps_inputsBillCycleChange,ps_outputsBillCycleChange); //Added WA#04 - JFONSECA - 02.03.2015	  
  	}
  	finally
  	{
	  	bsCheckOrder		= null;
		bsInputs			= null;
		bsOutputs			= null;
  	}
}"

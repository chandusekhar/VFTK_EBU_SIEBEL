			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="SetStatus"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function SetStatus(strActId)
{
//Oracle Review October 2011 - Call from PreInvokeMethod
	try
	{
		var strBC : BusComp = this.BusObject().GetBusComp (&quot;Activity Plan&quot;);
		with(strBC)
		{
			SetViewMode(AllView);
			ClearToQuery();
			ActivateField(&quot;VF Dunning Action Status&quot;);

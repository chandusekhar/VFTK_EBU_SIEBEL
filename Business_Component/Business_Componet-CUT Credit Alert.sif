			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="fnCheckEnablePTP"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function fnCheckEnablePTP(isSkipped)
{
//Oracle Review October 2011 - Call from PreCanInvoke
	try
	{
		var oBOAccount : BusObject;
		var oBCBillingAccount : BusComp;
		var oBCAssetMgmtHeader : BusComp;
		var strCheckConsistency1 = &quot;&quot;;
		var strCheckConsistency2 = &quot;&quot;;
		var strCheckBehaviour = &quot;&quot;;
		var strCurrentBACode = &quot;&quot;;
		var strOldInvoiceCreditLevel = &quot;&quot;;
		var strCheckActiveCounter = 0;
		var strCheckLFCounter = 0;
		var strCheckUCounter = 0;
		var strSearchExpr = &quot;&quot;;
		//ACAMBAS - 2014-08-29: VFTRCBU WA#27 ------------
		var strCustomerType = &quot;&quot;;
		var strBusinessType = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;ACCOUNT_TYPE&quot;, &quot;101&quot;);
		var strConsumerType = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;ACCOUNT_TYPE&quot;, &quot;102&quot;);
		//------------------------------------------------
		
		
		strCurrentBACode = this.GetFieldValue(&quot;Billing Account By Id&quot;);
		strOldInvoiceCreditLevel = this.GetFieldValue(&quot;Statement Id&quot;);
		
		oBOAccount = TheApplication().GetBusObject(&quot;Account&quot;);
		oBCBillingAccount = oBOAccount.GetBusComp(&quot;Billing Account&quot;);
		oBCAssetMgmtHeader = oBOAccount.GetBusComp(&quot;HP Submit - Asset Mgmt - Asset - Header&quot;);
		var strInactive	= TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;IMPL_PHASE&quot;, &quot;Inactive&quot;);//VFTK Venkata 20120406:PROD00030252
	
			with(oBCAssetMgmtHeader)
			{
				ActivateField(&quot;VF Dunning Status&quot;);
				ActivateField(&quot;Owner Account Type&quot;);	//ACAMBAS - 2014-08-29: VFTRCBU WA#27
				ClearToQuery();
				SetViewMode(AllView);
				strSearchExpr = &quot;([Product Part Number] = &apos;1000&apos; OR [Product Part Number] = &apos;18400&apos; OR [Product Part Number] = &apos;70000&apos;) AND [Status] &lt;&gt; &apos;&quot;+strInactive+&quot;&apos; AND [Billing Account Code] = &apos;&quot;+strCurrentBACode+&quot;&apos; AND [Root Asset Id] = [Id]&quot;; //VFTK Venkata Added Status &lt;&gt; Inactive in Search 20120406:PROD00030252
				SetSearchExpr(strSearchExpr);
				ExecuteQuery(ForwardOnly);
				
				var iRecordExists = FirstRecord();
				while(iRecordExists)
				{

					strCheckConsistency1 = GetFieldValue(&quot;VF Dunning Status&quot;);
					strCustomerType = GetFieldValue(&quot;Owner Account Type&quot;);	//ACAMBAS - 2014-08-29: VFTRCBU WA#27
									 
					if(strCheckConsistency2 == &quot;&quot;)
					{
						strCheckConsistency2 = strCheckConsistency1;
						iRecordExists = NextRecord();
					}
					
					else if(strCheckConsistency2 != &quot;&quot; &amp;&amp; strCheckConsistency2 == strCheckConsistency1)
					{
						strCheckConsistency2 = strCheckConsistency1;
						iRecordExists = NextRecord();
					}						
						
					else 
					{
						strCheckBehaviour = &quot;INCOHERENT&quot;;
						strBehavr = strCheckBehaviour;
						break;
					}
				}//End of while
					if(strCheckConsistency2 == strCheckConsistency1)
					{
						strCheckBehaviour = &quot;COHERENT&quot;;
						strBehavr = strCheckBehaviour;
					}
					
			}// with(oBCAsset..

			
		if(strCheckBehaviour ==&quot;COHERENT&quot;)
		{	
			//ACAMBAS - 2014-08-29: VFTRCBU WA#27 ------------
			
			//(Customer Status = &quot;Business&quot; AND Dunning Status in (&apos;Limited-Dunning&apos;, &apos;Frozen-Dunning&apos;, 
			//&apos;Limited-Credit for Broken PTP&apos;, &apos;Frozen-Credit due to BrokenPTP&apos;)) OR (Customer Status = &quot;Consumer&quot; AND 
			//Dunning Status in (&apos;Limited-Dunning&apos;, &apos;Limited-Credit for Broken PTP&apos;))
		
			//if(strCheckConsistency1 == &quot;204&quot; || strCheckConsistency1 == &quot;209&quot; || strCheckConsistency1 == &quot;1503&quot;  || strCheckConsistency1 == &quot;222&quot;)
		
			if((strCustomerType == strBusinessType &amp;&amp; (strCheckConsistency1 == &quot;204&quot; || strCheckConsistency1 == &quot;209&quot; || strCheckConsistency1 == &quot;1503&quot;  || strCheckConsistency1 == &quot;222&quot;)) || 
			   (strCustomerType == strConsumerType &amp;&amp; (strCheckConsistency1 == &quot;204&quot; || strCheckConsistency1 == &quot;1503&quot;)))
			{
			//------------------------------------------------
			
				strGconsistency = strCheckConsistency1;
				
					with(oBCBillingAccount)
					{
						ActivateField(&quot;VF PTP Counter Active&quot;);
						ActivateField(&quot;VF PTP Old Invoice Associated&quot;);
						ActivateField(&quot;VF PTP Counter LF&quot;);	
						ClearToQuery();										
						SetViewMode(AllView);
						SetSearchSpec(&quot;Account Code&quot; , strCurrentBACode);
						ExecuteQuery(ForwardOnly);
					
						if(FirstRecord())
						{
							strCheckActiveCounter = 1;
							SetFieldValue(&quot;VF PTP Counter Active&quot;, strCheckActiveCounter);
							WriteRecord();
							var strOldInvoiceAssoc = &quot;&quot;;
							strOldInvoiceAssoc = GetFieldValue(&quot;VF PTP Old Invoice Associated&quot;);
							strCheckLFCounter = GetFieldValue(&quot;VF PTP Counter LF&quot;);	
							
							if(strOldInvoiceAssoc == strOldInvoiceCreditLevel)
							{
									if(strCheckLFCounter &gt;= 3)
									{
										TheApplication().SetProfileAttr(&quot;CustomErrorCode&quot;,&quot;SBL-20002&quot;);
										TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;R21_181&quot;));
										return(CancelOperation);
									}
									else
									{
									
										if (isSkipped != &quot;Y&quot;) //Retrofit DEF-1455
								 		{
											strCheckLFCounter++;
											SetFieldValue(&quot;VF PTP Counter LF&quot;, strCheckLFCounter);
											WriteRecord();
											return true; //IBM_TFERREIRA - 20180102 DEF-21456
										}
									}
							}
							else
							{
								strOldInvoiceAssoc = strOldInvoiceCreditLevel;
								strCheckLFCounter = 0; //VFTK Diva 20120403: Resetting the value to 0 - PROD00030251.
								strCheckLFCounter++;
								SetFieldValue(&quot;VF PTP Old Invoice Associated&quot;, strOldInvoiceAssoc);							 	
								SetFieldValue(&quot;VF PTP Counter LF&quot;, strCheckLFCounter);
								WriteRecord();
								return true;
							}								 	
						}//if(First..	
					}//with(oBC..
			}//if(strCheckConsi..
			else {// User is blocked if it is coherent and not all the line are in Limited Dunning status in dunning status
				
				TheApplication().SetProfileAttr(&quot;CustomErrorCode&quot;,&quot;SBL-20003&quot;);
				var error = TheApplication().LookupMessage(&quot;VF Message Category&quot;,&quot;R21_215&quot;); 
				TheApplication().RaiseErrorText(error);
			}
				
		}//if(strCheckBehaviour..
		else if (strCheckBehaviour == &quot;INCOHERENT&quot;)
		{
				with(oBCBillingAccount)
					{
						ActivateField(&quot;VF PTP Counter U&quot;);
						ActivateField(&quot;VF PTP Old Invoice Associated&quot;);	
						ClearToQuery();				
						SetViewMode(AllView);
						SetSearchSpec(&quot;Account Code&quot; , strCurrentBACode);
						ExecuteQuery(ForwardOnly);
						if(FirstRecord())
						{
							strOldInvoiceAssoc = &quot;&quot;;
							strOldInvoiceAssoc = GetFieldValue(&quot;VF PTP Old Invoice Associated&quot;);
							strCheckUCounter = GetFieldValue(&quot;VF PTP Counter U&quot;);
							if(strOldInvoiceAssoc == strOldInvoiceCreditLevel)
							{
								if(strCheckUCounter &gt;= 1)
								{
									TheApplication().SetProfileAttr(&quot;CustomErrorCode&quot;,&quot;SBL-20001&quot;);
									TheApplication().RaiseErrorText(TheApplication().LookupMessage(&quot;VF Message Category&quot;, &quot;R21_180&quot;));
									return(CancelOperation);
								}
								else
								{
							 		strCheckUCounter++;
									SetFieldValue(&quot;VF PTP Counter U&quot;, strCheckUCounter);
									WriteRecord();
									return true;
								}
							}//if(strOld..
							else
							{
								strOldInvoiceAssoc = strOldInvoiceCreditLevel;
								strCheckUCounter++;
								SetFieldValue(&quot;VF PTP Old Invoice Associated&quot;, strOldInvoiceAssoc);						 		
								SetFieldValue(&quot;VF PTP Counter U&quot;, strCheckUCounter);
								WriteRecord();
								return true;
							}								 	
					}//if(First..	
				}//with(oBC..
		}//else if(strCheckBehaviour..
	}//END of try
	finally
	{
		oBCAssetMgmtHeader = null;
		oBCBillingAccount = null;
		oBOAccount = null;	
	}
}"

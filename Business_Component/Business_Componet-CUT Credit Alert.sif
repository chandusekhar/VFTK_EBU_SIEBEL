			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreInvokeMethod (MethodName)
{   
 try {
 		var psInputs : PropertySet;
		var psOutputs : PropertySet;
		var oWF : Service;
		var strBS : Service;
		var obj_Lov_Bo : BusObject;
		var obj_Lov_Bc : BusComp;
		var strBC : BusComp;
		var svcWF : Service;
		var strdatedue = &quot;&quot;;
		var ptpDate=&quot;&quot;;
		
 		if(MethodName == &quot;TotalDebtBA&quot;)
		{
			if(this.FirstRecord())
			{
				var strBACode = this.GetFieldValue(&quot;Billing Account By Id&quot;);
				var strCustCode = this.GetFieldValue(&quot;Account Number&quot;);
				psInputs= TheApplication().NewPropertySet();
				psOutputs=TheApplication().NewPropertySet();
				oWF= TheApplication().GetService(&quot;Workflow Process Manager&quot;);
				psInputs.SetProperty(&quot;ProcessName&quot;, &quot;VF Retrieve Outstanding Balance WF&quot;);
				psInputs.SetProperty(&quot;BILLING_ACCOUNT_CODE&quot;, strBACode);//Lilanath 20/04/2012 :C_MSP0001182: Replaced BILLING_ACCOUNT_NO with BILLING_ACCOUNT_CODE as the Property Name
				psInputs.SetProperty(&quot;CUSTOMER_CODE&quot;, strCustCode);
				psInputs.SetProperty(&quot;sOutBalVBCReq&quot;, &quot;N&quot;);	
				oWF.InvokeMethod(&quot;RunProcess&quot;, psInputs, psOutputs);
				if(psOutputs.GetProperty(&quot;Error Code&quot;)==0)
				{
					var sOutBalance = psOutputs.GetProperty(&quot;sOutBalance&quot;);
					if(sOutBalance &gt; 0)
						sOutBalance = sOutBalance/100;
					this.SetFieldValue(&quot;VF Total Debt of the Billing Account&quot;,sOutBalance); //Oracle Review October 2011 - Client Request
					this.WriteRecord();				
				}
			}
			return (CancelOperation);			
		}
		
		if(MethodName == &quot;PromiseToPay&quot;)
		{								
		
			ptpDate = TheApplication().GetService(&quot;TELCO DateTime Utils&quot;).AddDays(&quot;&quot;,2,&quot;&quot;,&quot;&quot;,&quot;N&quot;);
			
			
			var strErr = TheApplication().GetProfileAttr(&quot;PTPMsg&quot;);
			var strSuccess = TheApplication().GetProfileAttr(&quot;SuccesPTP&quot;);
			var strCheckPTP = &quot;&quot;;
			var strActionType = &quot;&quot;;
			if(strErr != &quot;&quot;)
			{
				TheApplication().SetProfileAttr(&quot;PTPMsg&quot;,&quot;&quot;);
				TheApplication().RaiseErrorText(strErr);
			}
			else if(strSuccess != &quot;&quot;)
			{
				var strActionTypeLook		= this.GetFieldValue(&quot;VF Action Type&quot;);
				obj_Lov_Bo = TheApplication().GetBusObject(&quot;List Of Values&quot;);
		     	obj_Lov_Bc = obj_Lov_Bo.GetBusComp(&quot;List Of Values&quot;);
				with(obj_Lov_Bc)
				{
					var strCallSrcExpr = &quot;[Type] = &apos;VF_CUT_FRAUD_TYPE&apos; AND [Value]=&apos;&quot;+strActionTypeLook+&quot;&apos; AND [Active]=&apos;Y&apos;&quot;;
					SetViewMode(AllView);
					ActivateField(&quot;Name&quot;);
					ClearToQuery();					
					SetSearchExpr(strCallSrcExpr);
					ExecuteQuery(ForwardOnly);
					if(FirstRecord())
					{
						strActionType = GetFieldValue(&quot;Name&quot;);
					}
				}
				
				//Start Retrofits - VFTRCBU-4990 - 06012015 - RCRISPIM
				strBC = this.BusObject().GetBusComp (&quot;Activity Plan&quot;);
				var isSkipped = &quot;N&quot;;    //Retrofit DEF-1455
				with(strBC)
				{
					ClearToQuery ();
					SetViewMode (AllView);
					ActivateField(&quot;Alert Id&quot;);
					
					SetSearchSpec (&quot;Alert Id&quot;, this.GetFieldValue(&quot;Id&quot;));
		 			ExecuteQuery(ForwardOnly);
					var ItemBCExists     = FirstRecord();
		            var strDunSkipStat = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_DUN_ACTION_STATUS&quot;,&quot;Skipped&quot;);   //Retrofit DEF-1455
					while(ItemBCExists){
			   	    	var dueDate = GetFieldValue(&quot;Due&quot;);
			   	    	var strtoday = new Date();
			   	    	var actStat = GetFieldValue(&quot;VF Dunning Action Status&quot;);   //Retrofit DEF-1455
			   	    	var strdate = (strtoday.getMonth()+1) + &apos;/&apos; + strtoday.getDate() +&apos;/&apos; + strtoday.getFullYear()+&apos; &apos;+ strtoday.getHours()+&apos;:&apos;+strtoday.getMinutes()+&apos;:&apos;+strtoday.getSeconds();
			   	    	
			   	    	if (actStat == strDunSkipStat) { //Retrofit DEF-1455
			   	    		isSkipped = &quot;Y&quot;;
			   	    	}
			   	    	
			   	    	if (Date.parse(dueDate) &gt;= Date.parse(strdate) &amp;&amp; isSkipped != &quot;Y&quot;) //Retrofit DEF-1455
						{
							//Hata mesajı basılacak
							var strMessageCategory 	= &quot;VF Message Category&quot;;
							var strCode				= &quot;X_VALID_DUE_DATE&quot;;
							var sMsg				= TheApplication().LookupMessage(strMessageCategory, strCode);
							TheApplication().RaiseErrorText(sMsg);					
							return (CancelOperation);
							
						}
						else if (Date.parse (strdate) &gt; Date.parse (dueDate))
						{
							// the second date is greater
						}
							
				   		ItemBCExists = NextRecord();
			    	}
					
				}
				//End Retrofits - VFTRCBU-4990 - 06012015 - RCRISPIM
				
			    strCheckPTP = fnCheckEnablePTP(isSkipped); //Retrofit DEF-1455
				if(strCheckPTP == true) // VFTK Prateek 201111214 R2.1: Added for CR291
				{
					var strStatementNumber  = this.GetFieldValue(&quot;Statement Id&quot;);
					var strAccountId		= this.GetFieldValue(&quot;Account By Id&quot;);
					var strAccCode			= this.GetFieldValue(&quot;Account Number&quot;);
					var strBillAccCode		= this.GetFieldValue(&quot;Billing Account By Id&quot;);
					var strtoday = new Date();
			    	var strdate = (strtoday.getMonth()+1) + &apos;/&apos; + strtoday.getDate() +&apos;/&apos; + strtoday.getFullYear()+&apos; &apos;+ strtoday.getHours()+&apos;:&apos;+strtoday.getMinutes()+&apos;:&apos;+strtoday.getSeconds();
					if ((TheApplication().GetProfileAttr(&quot;strDunStat&quot;)==&quot;2&quot;)||(TheApplication().GetProfileAttr(&quot;strDunStat&quot;)==&quot;3&quot;))
						strdatedue = (strtoday.getMonth()+1) + &apos;/&apos; + (strtoday.getDate()+2) +&apos;/&apos; + strtoday.getFullYear()+&apos; &apos;+ strtoday.getHours()+&apos;:&apos;+strtoday.getMinutes()+&apos;:&apos;+strtoday.getSeconds();
					else
					    strdatedue = (strtoday.getMonth()+1) + &apos;/&apos; + (strtoday.getDate()+1) +&apos;/&apos; + strtoday.getFullYear()+&apos; &apos;+ strtoday.getHours()+&apos;:&apos;+strtoday.getMinutes()+&apos;:&apos;+strtoday.getSeconds();
					var strlogin = TheApplication().LoginName();
					var strCategory = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_TODO_TYPE&quot;,&quot;ContactLog&quot;);
					//strBC = this.BusObject().GetBusComp (&quot;Activity Plan&quot;); 	//Retrofits - VFTRCBU-4990 - 06012015 - RCRISPIM
					var strDunSkipStatus = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_DUN_ACTION_STATUS&quot;,&quot;Skipped&quot;);
					var strActId = &quot;&quot;;
					var strTimeStamp = Clib.time();
					
					fnSetTypeArea(); // VFTK Prateek 20111215 R2.1: Added for CR-291
					
					if(this.GetUserProperty(&quot;PromiseToPayFromWS&quot;) != &quot;Y&quot;)
					{
						with(strBC)
						{
							NewRecord (NewAfter);
							SetFieldValue(&quot;VF Dunning Action Status&quot;,strDunSkipStatus);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;VF User Id&quot;,strlogin);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;Dunning Step&quot;,TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_DUNNING_STEP&quot;,&quot;PromiseCreated&quot;));//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;Type&quot;, strActionType);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;SR Type&quot;, strSetType); //Oracle Review October 2011 - Client Request
							strSetType = &quot;&quot;;
							SetFieldValue(&quot;Area&quot;, strSetArea);//Oracle Review October 2011 - Client Request
							strSetArea = &quot;&quot;;
							SetFieldValue(&quot;Category&quot;, strSetSubArea); //Oracle Review October 2011 - Client Request
							strSetSubArea = &quot;&quot;;
							SetFieldValue(&quot;VF Scheduled Date&quot;,strdate);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;VF Invoice No&quot;,strStatementNumber);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;Due&quot;,strdatedue);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;VF Category&quot;,strCategory);//Oracle Review October 2011 - Client Request
							SetFieldValue(&quot;Planned&quot;,strdate);//Oracle Review October 2011 - Client Request
							WriteRecord();
							strActId = GetFieldValue(&quot;Id&quot;);
						} // GEN_REQ_568: IDC-AS :21/11/2012: Commented out as in TD, &quot;VF_GP_STA_Drop_2_CRM_CROSS_TD_v2.0.docx&quot;	
						
						
						
						psInputs  = TheApplication().NewPropertySet ();
						psOutputs = TheApplication().NewPropertySet ();
						svcWF		= TheApplication().GetService(&quot;Workflow Process Manager&quot;);
						TheApplication().SetProfileAttr(&quot;strDate&quot;,&quot;&quot;);
						var sDunStat = TheApplication().GetProfileAttr(&quot;strDunStat&quot;);
						TheApplication().SetProfileAttr(&quot;strDunStat&quot;,&quot;&quot;);
						psInputs.SetProperty(&quot;ProcessName&quot;, &quot;VF Promise To Pay Request&quot;);
						psInputs.SetProperty(&quot;BILLING_ACCOUNT_CODE&quot;, strBillAccCode);
						psInputs.SetProperty(&quot;CHANNEL&quot;, &quot;SIEBEL&quot;);
			      		psInputs.SetProperty(&quot;DUNNING_STATUS&quot;, sDunStat);
			      		psInputs.SetProperty(&quot;TRANSACTION_ID&quot;,&quot;SIEBEL-&quot;+strTimeStamp);
			      		psInputs.SetProperty(&quot;PROMISE_TO_PAY_DATE&quot;,ptpDate)
			        	svcWF.InvokeMethod(&quot;RunProcess&quot;, psInputs, psOutputs);
			        	TheApplication().SetProfileAttr(&quot;PTPErr&quot;,psOutputs.GetProperty(&quot;Error Message&quot;));
					
						if(psOutputs.GetProperty(&quot;Error Message&quot;) == &quot;&quot;)
						{
							//VFTRCBU-34285 -- START
							this.SetFieldValue(&quot;VFTRCBU PTP Next Date&quot;, ptpDate);
							this.WriteRecord();
						/*
							with (sBA) {
						
								var strCallSrcExpr = &quot;[Account Code] = &apos;&quot; + TheApplication().GetProfileAttr(&quot;AccountCode&quot;)+&quot;&apos;&quot;;
								SetViewMode(AllView);
								ActivateField(&quot;VFTRCBU PTP Next Date&quot;);
								ClearToQuery();					
								SetSearchExpr(strCallSrcExpr);
								ExecuteQuery(ForwardOnly);
								if(FirstRecord())
								{
									SetFieldValue(&quot;VFTRCBU PTP Next Date&quot;, ptpDate);
									WriteRecord();	
								}
							}	
							*/
							//VFTRCBU-34285 -- END
						
						
							SetStatus(strActId);
							psInputs  = TheApplication().NewPropertySet ();
							psOutputs = TheApplication().NewPropertySet ();
							strBS	  = TheApplication().GetService(&quot;VF Dunning BS&quot;);
							psInputs.SetProperty (&quot;DunningStep&quot;, TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_DUNNING_STEP&quot;,&quot;PromiseCreated&quot;));
							psInputs.SetProperty (&quot;Account Id&quot;, strAccountId);
							psInputs.SetProperty (&quot;BA Code&quot;, strBillAccCode);
							psInputs.SetProperty (&quot;AccountCode&quot;,strAccCode);
							psInputs.SetProperty (&quot;InpActionType&quot;,strActionType);
							//CR172b
							if(sDunStat == &quot;3&quot;)
							{
								psInputs.SetProperty (&quot;InpOrderReason&quot;,&quot;Resume from Frozen due to PTP&quot;);
							}	
								//end of CR172b
							else
							{					
								psInputs.SetProperty (&quot;InpOrderReason&quot;,&quot;Resume from Limited with PTP&quot;);
							}	
							psInputs.SetProperty (&quot;InpCategory&quot;,strCategory);
							//TLG-6472
							//strBS.InvokeMethod (&quot;CreateOrder&quot;, psInputs, psOutputs);
						}
						// GEN_REQ_568: IDC-AS :21/11/2012: Commented out as in TD, &quot;VF_GP_STA_Drop_2_CRM_CROSS_TD_v2.0.docx&quot;
					}
				}//if(strCheck..
			}
			return (CancelOperation);	
		}
		return (ContinueOperation);
	}//try..
	catch(e)
	{
		TheApplication().SetProfileAttr(&quot;PTPMsg&quot;,&quot;&quot;);
		TheApplication().SetProfileAttr(&quot;strDunStat&quot;,&quot;&quot;);
		throw(e);
	}
	finally
	{
		strBS = null;
		psInputs = null;
		svcWF = null;
		strBC = null;
		strtoday = null;
		psOutputs = null;
		obj_Lov_Bc = null;
		obj_Lov_Bo = null;
		oWF = null;
	}
}"

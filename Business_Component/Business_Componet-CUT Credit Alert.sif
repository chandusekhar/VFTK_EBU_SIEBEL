			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="fnSetTypeArea"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function fnSetTypeArea()
{
 try
 {
 	var strBehaviour = strBehavr;
 	var strConstncy = strGconsistency;
	var boParameters : BusObject = TheApplication ().GetBusObject (&quot;ACS NI Parameter&quot;);
	var bcParameters : BusComp = boParameters.GetBusComp (&quot;ACS NI Parameter&quot;);
	var strParamSearchExpr = &quot;&quot;;

 	
 		if(strBehaviour == &quot;INCOHERENT&quot;)
 		{
 			strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;INCOHERENT&apos; AND [Value] = &apos;INCOHERENT&apos;&quot;;
 		}//if
 	
 		else if (strBehaviour = &quot;COHERENT&quot;)
 		{
 			switch(strConstncy)
 			{	
 				case &quot;000&quot;:
				strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;000&apos; AND [Value] = &apos;000&apos;&quot;;
 				break;
 		
 				case &quot;204&quot;:
 				strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;204&apos; AND [Value] = &apos;204&apos;&quot;;
 				break;
 		
 				case &quot;209&quot;:
 				strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;209&apos; AND [Value] = &apos;209&apos;&quot;;
				break; 	
				
				case &quot;222&quot;:
 				strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;222&apos; AND [Value] = &apos;222&apos;&quot;;
				break; 	
				
				case &quot;1503&quot;:
 				strParamSearchExpr = &quot;[Function] = &apos;PTPActivity&apos; AND [Name] = &apos;1503&apos; AND [Value] = &apos;1503&apos;&quot;;
				break; 		 		
 		
 				default:
 				break;
 			}//Switch			
 		}
 	
 			with(bcParameters)
 			{
				ClearToQuery();
				SetViewMode(AllView);
				SetSearchExpr(strParamSearchExpr);
				ExecuteQuery(ForwardOnly); //DIVA 20120315 - Oracle review comments: ForwardOnly
		
					if(FirstRecord())
					{
						strSetType = GetFieldValue(&quot;Condition1&quot;);
						strSetArea = GetFieldValue(&quot;Condition2&quot;);
						strSetSubArea = GetFieldValue(&quot;Condition3&quot;);
  					}
  			}//with
 }
 finally
 {
 	bcParameters = null;
 	boParameters = null;
 }
}"

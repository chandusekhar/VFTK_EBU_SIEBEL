			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_SetFieldValue"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_SetFieldValue (FieldName)
{
	try
	{
		var sBillAccntId 		= &quot;&quot;;
		var sBillAccntCurrCode	= &quot;&quot;;
		var boBillingAccount : BusObject;
		var bcBillingAccount : BusComp;
		
		if(FieldName == &quot;Amount Owed&quot;)
		{
		
	
		
			boBillingAccount = TheApplication().GetBusObject(&quot;PLW Bill Inquiry&quot;);
			bcBillingAccount = boBillingAccount.GetBusComp(&quot;Billing Account&quot;);
			sBillAccntId = this.GetFieldValue(&quot;Billing Account By Id&quot;);
			with(bcBillingAccount){			
				ClearToQuery();
				ActivateField (&quot;Currency Code&quot;);
				SetViewMode(AllView);
				SetSearchSpec(&quot;Id&quot;,sBillAccntId);
				ExecuteQuery(ForwardOnly);
				if(FirstRecord())
				{
					sBillAccntCurrCode = GetFieldValue(&quot;Currency Code&quot;);				
					if (sBillAccntCurrCode != &quot;&quot;)
					{
						this.SetFieldValue(&quot;Currency Code&quot;,sBillAccntCurrCode);//Oracle Review October 2011 - SetFieldValue in this BC
					}
				}
			}
		}
	}	
	finally
	{
		bcBillingAccount    = null;
		boBillingAccount    = null;
	}	
}"

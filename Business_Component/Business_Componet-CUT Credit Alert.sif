				SetFieldValue(&quot;VF Dunning Action Status&quot;,TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_DUN_ACTION_STATUS&quot;,&quot;Completed&quot;));
				WriteRecord();
		}
	}
	finally
	{
		strBC = null;
	}
}"

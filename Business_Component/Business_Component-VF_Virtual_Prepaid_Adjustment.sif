			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="isPositionEnabled"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function isPositionEnabled(sLovType, sLovName)
{
	//VFTRCBU WA04 - Check user number of roles - TiagoRocha - 14.10.2014
	var oBS:Service = TheApplication().GetService(&quot;VF TRK Utilities&quot;);
	var oPSInputs:PropertySet = TheApplication().NewPropertySet();
	var oPSOutputs:PropertySet = TheApplication().NewPropertySet();
	var sHasRole = &quot;&quot;;
	
	oPSInputs.SetProperty(&quot;Type&quot;, sLovType);
	oPSInputs.SetProperty(&quot;Name&quot;, sLovName);
	
	oBS.InvokeMethod(&quot;HasRolesLOV&quot;,oPSInputs, oPSOutputs);
	
	sHasRole = oPSOutputs.GetProperty(&quot;hasRole&quot;);
	
	if (sHasRole == &quot;True&quot;)
		return true;
	else
		return false;
	
	oPSInputs = null;
	oPSOutputs = null;
	oBS = null;
}"

				accountType = bcBillAsset.GetFieldValue(&quot;TeXCo Account Type&quot;);
				strAccountId = bcBillAsset.GetFieldValue(&quot;Parent Account Id&quot;);
			}	
		}
		else
		{
			var sDay = ComponentBA.GetFieldValue(&quot;VF Bill Cycle&quot;);
			var sBAId = ComponentBA.GetFieldValue(&quot;Id&quot;);
			accountType = ComponentBA.GetFieldValue(&quot;TeXCo Account Type&quot;);
			strAccountId = ComponentBA.GetFieldValue(&quot;Parent Account Id&quot;);
		}
				
		// VFTRCBU WA04 TiagoRocha  || ALINO VFTRCBU-27764 16-05-2016
		//VFTRCBU-31349 - disable bypass validation for Limit Checks
		//if(!isPositionEnabled(&quot;VFTRCBU_ADJ_VALIDATION_ROLES&quot;,&quot;No Active Lines&quot;)) //Role27
		//{
//TheApplication().Trace(&quot;ENTREI NO LIMITCHECK&quot;);
		var rErrorLimit = LimitCheck(BACode, sDay, var3, ErrorMessage);						
		if (rErrorLimit != &quot;&quot;) 
		{
			//TheApplication().Trace(&quot;ENTREI NO LIMITCHECK ERROR&quot;);		
			TheApplication().RaiseErrorText(rErrorLimit);
			return(ContinueOperation);
		}
		//}//END VFTRCBU-31349 - disable bypass validation for Limit Checks

		
//TheApplication().Trace(&quot;BEFORE SUBMIT ADJUSTMENT&quot;);				
		oBS.InvokeMethod(&quot;RunProcess&quot;, oInputs, oOutputs);
		
		var sErrorMessage = oOutputs.GetProperty(&quot;Error Message&quot;);
//TheApplication().Trace(&quot;AFTER SUBMIT ADJUSTMENT - Error Message:&quot; + sErrorMessage);
		
		if(sErrorMessage == &quot;&quot;)
		{
			var submittedSt = TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;PRE_ADJ_STATUS&quot;,&quot;Submitted&quot;);


			//VFTRCBU - TiagoRocha
			if(isPositionEnabled(&quot;VFTRCBU_ADJ_VALIDATION_ROLES&quot;,&quot;Special Role List&quot;)) //VFTRCBU Retrofits | Tiago Rocha | 26.01.2015 //Role7, Role28, Role29
			{
				//FCARRIL: VFTRCBU-30913
				AdjustmentInquiry(); 
				//this.InvokeMethod(&quot;AdjustmentInquiry&quot;);

			}	
			
			this.SetFieldValue(&quot;Status&quot;,submittedSt);
			this.WriteRecord(); 
						
			// VFTRCBU WA04 TiagoRocha
			if(isPositionEnabled(&quot;VFTRCBU_ADJ_VALIDATION_ROLES&quot;,&quot;Special Role List&quot;))
			{
//TheApplication().Trace(&quot;ENTER TO CREATE SR&quot;);		
			//  SR will be created here
			
				//var accountType = ComponentBA.GetFieldValue(&quot;TeXCo Account Type&quot;);
				var sPrepaid = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_PAYMENT_MODALITY&quot;,&quot;Prepaid&quot;);
				var sPostpaid = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;X_PAYMENT_MODALITY&quot;,&quot;Postpaid&quot;);				
			
				var psInputs			= 	TheApplication().NewPropertySet();
				var psOutputs			= 	TheApplication().NewPropertySet();
				var bsGeneralOperations	=	TheApplication().GetService (&quot;ACS General Operations&quot;);
				var maximoInputs		= 	TheApplication().NewPropertySet();
				var maximoOutputs		= 	TheApplication().NewPropertySet();
				
				//var strAccountId 	= ComponentBA.GetFieldValue(&quot;Parent Account Id&quot;);
				var strSubStatus   	= TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;SR_SUB_STATUS&quot;,&quot;Assigned&quot;);
				var strStatus	   	= TheApplication().InvokeMethod (&quot;LookupValue&quot;,&quot;SR_STATUS&quot;,&quot;Closed&quot;);
				var strType		   	= &quot;&quot;;
				var strArea	       	= &quot;&quot;;
				var strSubArea	   	= &quot;&quot;;
				var strSRCategory	= &quot;&quot;;
				var strOwnerGroup  	= TheApplication().PositionName();
				var strDescription 	= &quot;&quot;;
				
				/*if (accountType == sPostpaid)
				{
					strType		= &quot;ŞIKAYET&quot;;
					strArea		= &quot;FATURA İŞLEMLERI&quot;;
					//strSubArea	    = &quot;CC OTOMATIK EMP. IADE-FATURALI&quot;;
					strSubArea	    = &quot;CC OTOMATIK EMP. İADE-FATURALI&quot;; //FCARRIL: VFTRCBU-30913
				}
				else if (accountType == sPrepaid)
				{*/
					strType		= &quot;ŞIKAYET&quot;;
					//strArea		= &quot;CEP LIRA&quot;;
					strArea		= &quot;FATURA İŞLEMLERI&quot;;//FCARRIL: VFTRCBU-30913
					//strSubArea	    = &quot;CC OTOMATIK EMP IADE-FATURASIZ&quot;; 
					strSubArea	    = &quot;CC OTOMATIK EMP. İADE-FATURALI&quot;; //FCARRIL: VFTRCBU-30913
				//}
			
				psInputs.SetProperty (&quot;Type&quot;, strType);
				psInputs.SetProperty (&quot;Area&quot;, strArea);
				psInputs.SetProperty (&quot;Sub Area&quot;, strSubArea);
				psInputs.SetProperty (&quot;Status&quot;, strStatus);
				psInputs.SetProperty (&quot;Sub Status&quot;, strSubStatus);
				psInputs.SetProperty (&quot;VF SR Category&quot;, strSRCategory);
				psInputs.SetProperty (&quot;Owner Group&quot;, strOwnerGroup);
				psInputs.SetProperty (&quot;Description&quot;, strDescription);
				psInputs.SetProperty (&quot;Account Id&quot;, strAccountId);
				
				bsGeneralOperations.InvokeMethod (&quot;CreateSR&quot;, psInputs, psOutputs);
				var strSRId = psOutputs.GetProperty(&quot;Service Request Id&quot;);
//TheApplication().Trace(&quot;SR CREATED:&quot;+strSRId);
				
				// find new created SR nummber to send maximo ...
				//
				//
				var boServiceRequest = TheApplication().GetBusObject(&quot;VF Maximo Ticket Flat BO&quot;);	
				var bcServiceRequest = boServiceRequest.GetBusComp(&quot;VF Service Request Maximo&quot;);
				
				with(bcServiceRequest)
				{
					ClearToQuery();
					SetViewMode(AllView);
					SetSearchExpr(&quot;[Id]=&apos;&quot;+strSRId+&quot;&apos;&quot;);
					ActivateField(&quot;SR Number&quot;);
					ActivateField(&quot;Billing Account Code&quot;);
					ActivateField(&quot;Triplet No&quot;);
					ActivateField(&quot;SR Type&quot;);
					ActivateField(&quot;Area&quot;);
					ActivateField(&quot;Sub-Area&quot;);					
					ActivateField(&quot;Status&quot;);										
					ExecuteQuery(ForwardOnly);
					if(FirstRecord())
					{
					    var strSRNumber	= GetFieldValue(&quot;SR Number&quot;);
					    SetFieldValue(&quot;Billing Account Id&quot;,sBAId);
					    
						//if (accountType == sPostpaid) //FCARRIL: VFTRCBU-30913
						if (CustTypeCalc == &quot;101&quot;) // EBU
						{
							SetFieldValue(&quot;Triplet No&quot;,&quot;12205&quot;);
							SetFieldValue(&quot;SR Type&quot;,&quot;ŞIKAYET&quot;);
							SetFieldValue(&quot;Area&quot;,&quot;FATURA İŞLEMLERI&quot;); //FCARRIL: VFTRCBU-30913
							SetFieldValue(&quot;Sub-Area&quot;,&quot;CC OTOMATIK EMP. İADE-FATURALI&quot;); //FCARRIL: VFTRCBU-30913
							//SetFieldValue(&quot;Status&quot;,&quot;Closed&quot;);
																
						}
						//else if (accountType == sPrepaid) //FCARRIL: VFTRCBU-30913
						else if (CustTypeCalc == &quot;102&quot;) // CBU
						{
//TheApplication().Trace(&quot;SR MAXIMO VALUES - CBU&quot;);
							SetFieldValue(&quot;Triplet No&quot;,&quot;1908&quot;); //FCARRIL: VFTRCBU-30913
							SetFieldValue(&quot;SR Type&quot;,&quot;ŞIKAYET&quot;); //FCARRIL: VFTRCBU-30913
							SetFieldValue(&quot;Area&quot;,&quot;FATURA İŞLEMLERI&quot;); //FCARRIL: VFTRCBU-30913
							SetFieldValue(&quot;Sub-Area&quot;,&quot;CC OTOMATIK EMP. İADE-FATURALI&quot;);	 //FCARRIL: VFTRCBU-30913
							//SetFieldValue(&quot;Status&quot;,&quot;Closed&quot;);															
						}					    
					    
					    WriteRecord(); 
					    		
					}
				}					
//TheApplication().Trace(&quot;SR MAXIMO - WRITE RECORD&quot;);				
				//
				
				//create note
				
				var boNote = TheApplication().GetBusObject(&quot;Service Request&quot;);	
				var bcNote = boNote.GetBusComp(&quot;PLW Request Note&quot;);
				
				bcNote.NewRecord(NewBefore);
				bcNote.SetFieldValue(&quot;Note&quot;,var3+&quot; TL&quot;);
				bcNote.SetFieldValue(&quot;Source Id&quot;,strSRId);
				
				bcNote.NewRecord(NewBefore);
				bcNote.SetFieldValue(&quot;Note&quot;,sRightness);
				bcNote.SetFieldValue(&quot;Source Id&quot;,strSRId);	
				
				bcNote.WriteRecord(); 		
				
				//

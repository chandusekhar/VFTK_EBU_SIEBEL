			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="Replace"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function Replace(src, trg, rep) //ITSL-2105
{ 
var i = src.indexOf(trg); 
while (i &gt;= 0) 
{ 
src = src.substring(0, i) + rep + src.substring(i + trg.length, src.length); 
i = src.indexOf(trg); 
} 
return(src); 
}"

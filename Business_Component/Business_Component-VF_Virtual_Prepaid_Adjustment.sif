		var outProp :PropertySet= TheApplication().NewPropertySet();
              
        var CustType;
        var InvoiceItem;
		var ErrorCode = 0;
		var ErrorMessage = &quot;&quot;;
		
		if (BACode == null || BACode == &quot;&quot;)
		{
			return;
		}
		
		//ALINO VFTRCBU-27764 16-05-2016

		CustType = this.GetFieldValue(&quot;VFTRCBU Customer Type Calc&quot;);
        InvoiceItem  = this.GetFieldValue(&quot;VF Invoice Item&quot;);

		inProp.SetProperty(&quot;Request Date&quot;,  sDay);
		
        inProp.SetProperty(&quot;Request Amt&quot;, var3);
        inProp.SetProperty(&quot;Account Code&quot;, BACode);
        inProp.SetProperty(&quot;Customer Type&quot;, CustType);
        inProp.SetProperty(&quot;Invoice Item&quot;, InvoiceItem);
        
		BS.InvokeMethod(&quot;LimitCheckInquiry&quot;,inProp, outProp);
        ErrorCode = outProp.GetProperty(&quot;Error Code&quot;);
        ErrorMessage = outProp.GetProperty(&quot;Error Message&quot;);
        
        return(ErrorMessage);
	
	}
	finally
	{
		CustType = null;
		InvoiceItem = null;
				
	}

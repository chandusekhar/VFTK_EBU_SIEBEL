			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreInvokeMethod (MethodName)
{
	
	
	if (MethodName == &quot;AdjustmentInquiry&quot;)
	{	
		this.SetSearchSpec(&quot;CustomMethodName&quot;, &quot;Adjustment Inquiry&quot;);
		this.ExecuteQuery(ForwardBackward);
		return (CancelOperation);
	}
	

	if(MethodName == &quot;Submit1&quot;)
	{
		Submit();
		return(CancelOperation);
	}
	if(MethodName == &quot;Cancel&quot;)
	{
		Cancel();
		return(CancelOperation);
	}
	
	/*if (MethodName == &quot;AdjustmentInquiry&quot;)
    {
        AdjustmentInquiry();
        return(CancelOperation);
  	 }*/

	
	return (ContinueOperation);
}"

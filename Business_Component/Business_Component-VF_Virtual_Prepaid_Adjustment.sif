			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="AdjustmentInquiry"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function AdjustmentInquiry()
{
//FCARRIL: VFTRCBU-30913 - This function calls AdjustmentInquiryInternal because AdjustmentInquiry
//uses GetCustomMethodName method
	try
	{
		 var BS     : Service = TheApplication().GetService(&quot;VF Virtual Prepaid Adjustment BS&quot;);
         var inProp :PropertySet        = TheApplication().NewPropertySet();
         var outProp :PropertySet= TheApplication().NewPropertySet();
         var outMessage :PropertySet= TheApplication().NewPropertySet();
         var BACode;
         
         BACode = this.ParentBusComp().GetFieldValue(&quot;Account Code&quot;);     
//TheApplication().Trace(&quot;ADJUSTMENT INQUIRY - BC - BA Code:&quot;+BACode);

         var ErrorCode = 0;
         var ErrorMessage = &quot;&quot;;
         inProp.SetProperty(&quot;Account Code&quot;,   BACode);

         BS.InvokeMethod(&quot;AdjustmentInquiryInternal&quot;,inProp, outProp); //FCARRIL: VFTRCBU-30913
         
         ErrorCode =  outProp.GetProperty(&quot;Error Code&quot;);
         ErrorMessage =  outProp.GetProperty(&quot;Error Message&quot;);
 
//TheApplication().Trace(&quot;ADJUSTMENT INQUIRY - Called BS:&quot;+ErrorCode+&quot; Error Message:&quot;+ErrorMessage);
         
         if (ErrorMessage != &quot;&quot;)
		{
   			return(ErrorMessage);
		}
		
    
	return(ContinueOperation);
	}
	finally
	{
			
	}
}"

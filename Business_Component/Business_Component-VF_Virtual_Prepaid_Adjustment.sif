			var sBAId = ComponentBA.GetFieldValue(&quot;Billing Account Id&quot;);
			var boBillAsset : BusObject = TheApplication().GetBusObject(&quot;Billing Account&quot;);
			var bcBillAsset : BusComp   = boBillAsset.GetBusComp(&quot;Billing Account&quot;);
			bcBillAsset.SetViewMode(AllView);
			bcBillAsset.ClearToQuery();
			bcBillAsset.ActivateField(&quot;Id&quot;);
			bcBillAsset.SetSearchSpec(&quot;Id&quot;, sBAId);
			bcBillAsset.ExecuteQuery(ForwardOnly);

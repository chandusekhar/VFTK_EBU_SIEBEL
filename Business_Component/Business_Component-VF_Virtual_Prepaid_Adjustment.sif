			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_NewRecord ()
{	
	this.SetFieldValue(&quot;Created By Login&quot;, TheApplication().LoginName());
	this.SetFieldValue(&quot;Request Date&quot;, this.GetFieldValue(&quot;VF Timestamp&quot;));
	this.SetFieldValue(&quot;Status&quot;, TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;PRE_ADJ_STATUS&quot;, &quot;Open&quot;));


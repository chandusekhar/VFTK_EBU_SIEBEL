	{
		this.SetFieldValue(&quot;Type&quot;, TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;INVC_ADJ_REQ_TYPE_CD&quot;, &quot;Credit&quot;));
	}
}"

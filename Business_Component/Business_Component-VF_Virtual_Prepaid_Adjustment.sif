				}
//TheApplication().Trace(&quot;CALL SUBMIT TO MAXIMO&quot;);				
				maximoInputs.SetProperty (&quot;SR_NUMBER&quot;, strSRNumber);
				SubmitSR_ToMaximo(maximoInputs, maximoOutputs);

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="SubmitSR_ToMaximo"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function SubmitSR_ToMaximo(Inputs,Outputs)
{
	try
	{
		var boServiceRequest = TheApplication().GetBusObject(&quot;VF Maximo Ticket Flat BO&quot;);	
		var bcServiceRequest = boServiceRequest.GetBusComp(&quot;VF Service Request Maximo&quot;);
		
		var boServiceRequest1= TheApplication().GetBusObject(&quot;VF Maximo Ticket Flat BO&quot;);	
		var bcServiceRequest1= boServiceRequest1.GetBusComp(&quot;VF Service Request Maximo&quot;);
		
		var BS	: Service = TheApplication().GetService(&quot;VF TRK Utilities&quot;);
		var oBS	: Service = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
		
		var inProp :PropertySet	= TheApplication().NewPropertySet();
		var outProp :PropertySet= TheApplication().NewPropertySet();
		
		var strREFUND_AMOUNT= Inputs.GetProperty(&quot;REFUND_AMOUNT&quot;);
		var strREFUND_STATUS= Inputs.GetProperty(&quot;REFUND_STAUS&quot;);
		var strSR_NUMBER	= Inputs.GetProperty(&quot;SR_NUMBER&quot;);
		var strVFTripletNo	= &quot;&quot;;
		var strTripletLOV	= &quot;&quot;;
		var strBA_CODE		= &quot;&quot;;
		var strCONTACT_NAME = &quot;&quot;;
		var strCUSTOMER_NAME= &quot;&quot;;
		var strREQUEST_DATE	= &quot;&quot;;
		var strSUBSCRIPTION_DATE	= &quot;&quot;;
		var strCUSTOMER_TYPE		= &quot;&quot;;
		var strSERVICE_TYPE			= &quot;&quot;;
		var strREVENUE_SEGMENT		= &quot;&quot;;
		var strPREVIOUS_REQUESTS	= &quot;&quot;;
		var strPROBLEM_NAME			= &quot;&quot;;
		
		var bNextRecord  = false;
		var strAccountId = &quot;&quot;;
		var strPreContactName = &quot;&quot;;
		var strPreProblemName = &quot;&quot;;
		var strUser = TheApplication().LoginName();
				
//		Validate mandatory fields
		inProp.SetProperty(&quot;SR_NUMBER&quot;,strSR_NUMBER);
		BS.InvokeMethod(&quot;Input Validation&quot;,inProp, outProp);

//TheApplication().Trace(&quot;SUBMIT TO MAXIMO - BEFORE QUERY&quot;);
		
		with(bcServiceRequest)
		{
			ClearToQuery();
			SetViewMode(AllView);
			SetSearchExpr(&quot;[SR Number]=&apos;&quot;+strSR_NUMBER+&quot;&apos;&quot;);
			ExecuteQuery(ForwardOnly);
			if(FirstRecord())
			{
				strVFTripletNo	= GetFieldValue(&quot;Triplet No&quot;);
				strBA_CODE		= GetFieldValue(&quot;Billing Account Code&quot;);
				strCONTACT_NAME	= GetFieldValue(&quot;Pim Contact Name&quot;);	
				strCUSTOMER_NAME= GetFieldValue(&quot;Account Last Name&quot;);
				strPROBLEM_NAME	= GetFieldValue(&quot;Sub-Area&quot;);
				strREQUEST_DATE	= GetFieldValue(&quot;Opened Date&quot;);
				strSUBSCRIPTION_DATE=GetFieldValue(&quot;Customer Since&quot;);
				strREVENUE_SEGMENT	= GetFieldValue(&quot;Customer Class&quot;);
				strCUSTOMER_TYPE	= GetFieldValue(&quot;Customer Type&quot;);
				strSERVICE_TYPE		= GetFieldValue(&quot;Billing Account Type&quot;);
				
				strAccountId = GetFieldValue(&quot;Account Id&quot;);
				
				with(bcServiceRequest1)
				{
					ClearToQuery();
					SetViewMode(AllView);
					SetSearchExpr(&quot;[Account Id]=&apos;&quot;+strAccountId+&quot;&apos; AND [SR Number] &lt;&gt;&apos;&quot;+strSR_NUMBER+&quot;&apos;AND [Created]&gt;=Today()-365&quot;);
					ExecuteQuery(ForwardOnly);
					bNextRecord = FirstRecord();
					while(bNextRecord)
					{
						strPreProblemName= GetFieldValue(&quot;Sub-Area&quot;);
						if(strPreProblemName != null &amp;&amp; strPreProblemName!=&quot;&quot;)
						{
							strPREVIOUS_REQUESTS = strPREVIOUS_REQUESTS + strPreProblemName+&quot;\n&quot;;
						}
						bNextRecord = NextRecord();
					}
				}
				inProp.Reset();
				outProp.Reset();
				
				inProp.SetProperty(&quot;SR_NUMBER&quot;,strSR_NUMBER);
				inProp.SetProperty(&quot;BA_CODE&quot;,strBA_CODE);
				inProp.SetProperty(&quot;CUSTOMER_NAME&quot;,strCUSTOMER_NAME);
				inProp.SetProperty(&quot;PROBLEM_NAME&quot;,strPROBLEM_NAME);
				inProp.SetProperty(&quot;REQ_DATE&quot;,strREQUEST_DATE);
				inProp.SetProperty(&quot;SUBS_DATE&quot;,strSUBSCRIPTION_DATE);
				inProp.SetProperty(&quot;CUSTOMER_SEGMENT&quot;,strREVENUE_SEGMENT);
				inProp.SetProperty(&quot;CUSTOMER_TYPE&quot;,strCUSTOMER_TYPE);
				inProp.SetProperty(&quot;SERVICE_TYPE&quot;,strSERVICE_TYPE);
				inProp.SetProperty(&quot;PREVIOS_REQS&quot;,strPREVIOUS_REQUESTS);
				inProp.SetProperty (&quot;REFUND_AMOUNT&quot;,strREFUND_AMOUNT );
				inProp.SetProperty (&quot;REFUND_STATUS&quot;,strREFUND_STATUS);
				inProp.SetProperty (&quot;USER&quot;, strUser);
//TheApplication().Trace(&quot;SUBMIT TO MAXIMO - BEFORE WF CALL&quot;);				
				inProp.SetProperty(&quot;ProcessName&quot;, &quot;VF Submit SR To Maximo&quot;);
				oBS.InvokeMethod(&quot;RunProcess&quot;, inProp, outProp);
				
				var sErrorMessage = outProp.GetProperty(&quot;Error Message&quot;);

//TheApplication().Trace(&quot;SUBMIT TO MAXIMO - AFTER WF CALL. ERROR:&quot;+sErrorMessage);	
	
				if(sErrorMessage != &quot;&quot; &amp;&amp; sErrorMessage != null)
				{
					TheApplication().RaiseErrorText(sErrorMessage);
				}
			}
			else
			{
				TheApplication().RaiseErrorText(&quot;There is no service request found!!!&quot;);
			}
		}
	}
	catch(e)
	{
		//TheApplication().RaiseErrorText(e.ToString()); //FCARRIL:VFTRCBU-31243
		TheApplication().RaiseErrorText(e.errText);
	}
	finally
	{		
		boServiceRequest	= null;
		boServiceRequest1	= null;
		bcServiceRequest	= null;
		bcServiceRequest1	= null;
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_SetFieldValue"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_SetFieldValue (FieldName)
{	
	if(FieldName==&quot;Reason For Request&quot;)
	{
		var Reason = this.GetFieldValue(&quot;Reason For Request&quot;);
		if(Reason == TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;PRE_ADJ_REASON&quot;,&quot;Interim payment&quot;))
		{
	   		var retDateTemp = new Date();
	   		var strTime = retDateTemp.getTime()+(48*3600*1000);
	   		var retDateT = new Date(strTime); 
	   		//code retrofits 20150427 - February Minor
			var stringDate =  (retDateT.getMonth()+1)+&quot;/&quot;+(retDateT.getDate())+&quot;/&quot;+retDateT.getFullYear()+&quot; 23:58:59&quot;;	
	   	   	//this.SetFieldValue(&quot;VF Due Date&quot;,stringDate);//Oracle Review October 2011 - SetFieldValue in this BC
	   	    
	        //Find the calendar
		    var SchedBO:BusObject = TheApplication().GetBusObject(&quot;Shift&quot;);
			var SchedBC:BusComp	= SchedBO.GetBusComp(&quot;Shift&quot;);
			var CalendName		= &quot;VF Complaint Schedule&quot;;
			var CalendId= &quot;&quot;;
			var isHoliday =&quot;&quot;;
			var month : float;
			var day : float;
			var year : float;
		    var stringDate2=&quot;&quot;;
				
			SchedBC.ClearToQuery();
			SchedBC.ActivateField(&quot;Name&quot;);
			SchedBC.ActivateField(&quot;Id&quot;);
			SchedBC.SetSearchSpec(&quot;Name&quot;, CalendName);
			SchedBC.ExecuteQuery(ForwardOnly);
	
			if(SchedBC.FirstRecord())
				CalendId   = SchedBC.GetFieldValue(&quot;Id&quot;);
			
				var BS =  TheApplication().GetService(&quot;FS Holiday API Service&quot;);
				var PSOut = TheApplication().NewPropertySet();
				var PSIn = TheApplication().NewPropertySet();
				var count = 1;
				PSIn.SetProperty(&quot;Date Time&quot;, stringDate);
				PSIn.SetProperty(&quot;Calender Id&quot;,CalendId);
				PSIn.SetProperty(&quot;Time Zone&quot;,&quot;GTB Standard Time&quot;);
			    BS.InvokeMethod(&quot;IsHoliday&quot;,PSIn, PSOut);
				isHoliday = PSOut.GetProperty(&quot;Is Holiday&quot;);
			    stringDate2 = (retDateT.getMonth()+1)+ &quot;/&quot;+ (retDateT.getDate() ) + &quot;/&quot;+retDateT.getFullYear() + &quot; 23:58:59&quot;;
			
			    day= retDateT.getDate();
			    month=retDateT.getMonth()+1;
			    year= retDateT.getFullYear(); 
			    
				while (isHoliday==&quot;TRUE&quot; &amp;&amp; count &lt;15)	   	    	
		   	    {	   	    	
		   	    	if ( (month ==&quot;1&quot; &amp;&amp; day == &quot;31&quot;) || (month ==&quot;2&quot; &amp;&amp; day == &quot;28&quot;) || (month ==&quot;3&quot; &amp;&amp; day == &quot;31&quot;) || (month ==&quot;4&quot; &amp;&amp; day == &quot;30&quot;) || (month ==&quot;5&quot; &amp;&amp; day == &quot;31&quot;) || (month ==&quot;6&quot; &amp;&amp; day == &quot;30&quot;) || (month ==&quot;7&quot; &amp;&amp; day == &quot;31&quot;) || (month ==&quot;8&quot; &amp;&amp; day == &quot;31&quot;) || (month ==&quot;9&quot; &amp;&amp; day == &quot;30&quot;) || (month ==&quot;10&quot; &amp;&amp; day == &quot;30&quot;) || (month ==&quot;11&quot; &amp;&amp; day == &quot;30&quot;) || (month ==&quot;12&quot; &amp;&amp; day == &quot;31&quot;) ) 
	 		        { 		            
	 		        	if (month ==&quot;12&quot; &amp;&amp; day == &quot;31&quot;)   
	 		             {
	 		               day=1;
	 		               month = 1;
	 		               year= year +1;
	 		             } 		               
	 		               else 
	 		             {
	 		               day=1;
	 		               month = month +1;
	 		             } 		             
	 		            }
	 		          
	 		            else {
	       		
	 		            day = day +1;
	 		                 }
	 		           
	 		            stringDate2 = (month)+ &quot;/&quot;+ (day) + &quot;/&quot;+year+ &quot; 23:58:59&quot;;
	 		            
	 		            var PSOut = TheApplication().NewPropertySet();
				        var PSIn = TheApplication().NewPropertySet();
	       				PSIn.SetProperty(&quot;Date Time&quot;, stringDate2);
	       				PSIn.SetProperty(&quot;Calender Id&quot;,CalendId);
			         	PSIn.SetProperty(&quot;Time Zone&quot;,&quot;GTB Standard Time&quot;);
				        BS.InvokeMethod(&quot;IsHoliday&quot;,PSIn, PSOut);  
				        isHoliday = PSOut.GetProperty(&quot;Is Holiday&quot;);
				        count=count +1;
			           
		   	    	}
		   	    		
				PSOut = null;
				PSIn = null;
				
			    this.SetFieldValue(&quot;VF Due Date&quot;,stringDate2);//Oracle Review October 2011 - SetFieldValue in this BC
				this.SetFieldValue(&quot;İnvoice Due Date&quot;,stringDate2);//Oracle Review October 2011 - SetFieldValue in this BC
				//end code retrofits 20150427 - February Minor
		}
		if(Reason == TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;PRE_ADJ_REASON&quot;,&quot;Donation Refund&quot;))
		{
			var sValue = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_ADJ_TAX_PERCENT&quot;,&quot;Tahsilatına aracılık edilen&quot;);
			var oBCPick = GetPicklistBusComp(&quot;VF Invoice Item&quot;);
			with (oBCPick)
			{
			  ClearToQuery();
			  SetSearchSpec(&quot;VFTRCBU Value&quot;,sValue);
			  ExecuteQuery(ForwardOnly);
			  if (FirstRecord())
				 Pick();
			}
			oBCPick = null;
		}
	}
}"

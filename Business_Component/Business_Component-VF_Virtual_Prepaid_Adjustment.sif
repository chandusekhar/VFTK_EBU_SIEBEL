			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="Cancel"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function Cancel()
{
	var Inputs : PropertySet;
	var Outputs : PropertySet;
	var BSAdjustCancel : Service;
	
	
	try
	{
		var iRecord = this.FirstSelected();
	
		var sStatus;
		
		var sErrorMessage;
	
		var sCancelled = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;PRE_ADJ_STATUS&quot;, &quot;Cancelled&quot;);
		
		while (iRecord)
		{                     
			Inputs = TheApplication().NewPropertySet();
			Outputs = TheApplication().NewPropertySet();
			
			BSAdjustCancel = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
			
			Inputs.SetProperty(&quot;ProcessName&quot;,&quot;VF Adjustment Cancellation&quot;);
			Inputs.SetProperty(&quot;Adjustment ID&quot;, this.GetFieldValue(&quot;Adjustment ID&quot;));
			Inputs.SetProperty(&quot;Account Code&quot;, this.GetFieldValue(&quot;Account Code&quot;));
			
			BSAdjustCancel.InvokeMethod(&quot;RunProcess&quot;,Inputs,Outputs);
			
			sStatus = Outputs.GetProperty(&quot;STATUS_CODE&quot;);
			
			if(sStatus == &quot;1&quot;)
			{
				sErrorMessage = Outputs.GetProperty(&quot;ERROR_REASON&quot;);
				
				TheApplication().RaiseErrorText(sErrorMessage);
			}
			else
			{
				this.SetFieldValue(&quot;Status&quot;, sCancelled);
			}
			
			iRecord = NextSelected();
		}
	}
	finally
	{
		Inputs = null;
		Outputs = null;
		BSAdjustCancel = null;
	}
			
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="ValidateDate"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function ValidateDate(Field,ErrMsg,check)
{
	// Modified with VFTRCBU-4420
	try
	{
		var dateBS : Service = TheApplication().GetService(&quot;TELCO DateTime Utils&quot;);
		var oPSInputs  : PropertySet = TheApplication().NewPropertySet();
		var oPSOutputs : PropertySet = TheApplication().NewPropertySet();
		var sToday:chars = &quot;&quot;;
	    Clib.strftime(sToday, &quot;%m/%d/%Y&quot;, Clib.localtime(Clib.time()));

		oPSInputs.SetProperty(&quot;Date1&quot;,sToday);
		oPSInputs.SetProperty(&quot;Date2&quot;,Field);
		dateBS.InvokeMethod(&quot;CompareDates&quot;,oPSInputs,oPSOutputs);
		var result = oPSOutputs.GetProperty(&quot;Result&quot;);
		//var equalDates = Outputs.GetProperty(&quot;equalDates&quot;);
		
		if((result == 2 &amp;&amp; check == 1)||(result == 0 &amp;&amp; check == 2)){
		  	  var strMessageCategory = &quot;VF Message Category&quot;;
	          var strCode = ErrMsg;
	          var sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
	          TheApplication().RaiseErrorText(sMsg);              
	          return (CancelOperation);
		}
	}
	finally
	{
		dateBS = null;
		oPSInputs = null;
		oPSOutputs = null;
	}	
}"

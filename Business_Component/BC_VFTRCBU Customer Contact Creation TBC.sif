			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="ValidateField"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function ValidateField (Field,ErrMsg)
{
	var sNation = this.GetFieldValue(&quot;VF Contact Citizenship&quot;);
	var sTurkish = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;X_CITIZENSHIP&quot;, &quot;Turkish&quot;);
	var strMessageCategory = &quot;&quot;;
	var strCode = &quot;&quot;;
	var sMsg = &quot;&quot;;
	if (sTurkish == sNation)
	{
		if (Field.match(/^[9][0][0-9]{10}$/) == null)
		{
			strMessageCategory = &quot;VF Message Category&quot;;
			strCode = ErrMsg;
			sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
			TheApplication().RaiseErrorText(sMsg);
			return (CancelOperation);
		}
	}
	else
	{
		if (Field.indexOf(&quot;90&quot;, 0) == 0)
		{
			if (Field.match(/^[9][0][0-9]{10}$/) == null)
			{
				strMessageCategory = &quot;VF Message Category&quot;;
				strCode = ErrMsg;
				sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
				TheApplication().RaiseErrorText(sMsg);
				return (CancelOperation);
			}
		}
		else
		{
			if (Field.match(/^[0-9]{12}$/) == null)
			{
				strMessageCategory = &quot;VF Message Category&quot;;
				strCode = ErrMsg;
				sMsg = &quot;Phone must have 12 digits&quot;;
				TheApplication().RaiseErrorText(sMsg);
				return (CancelOperation);
			}
		}
	}

}"

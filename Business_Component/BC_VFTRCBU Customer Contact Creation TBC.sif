			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="ValidateEmail"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function ValidateEmail (Field,ErrMsg)
{
	var OnlyChMail = Field.substr(1,Field.indexOf(&quot;@&quot;)-1);
	var strMessageCategory = &quot;VF Message Category&quot;;

	if(Field.match(/([çğıİöşüÇĞİÖŞÜ])/g) != null)
	{
		var strCode_1 = &quot;R21_199&quot;;
		var sMsg_1 = TheApplication().LookupMessage(strMessageCategory, strCode_1);
		TheApplication().RaiseErrorText(sMsg_1);              
		return (CancelOperation);
	}
		
	var strreplace = Field.replace(&apos;[&apos;,&quot;!&quot;);
	strreplace = strreplace.replace(&apos;]&apos;,&quot;!&quot;);
	strreplace = strreplace.replace(&apos;\\&apos;,&quot;!&quot;);
	
	if(OnlyChMail.match(/([!&apos;&quot;?,:&lt;&gt;() ;])/g) != null || strreplace.match(/([!])/g) == &quot;!&quot;)
	{ 
		var strCode_2 = &quot;R21_200&quot;;
		var sMsg_2 = TheApplication().LookupMessage(strMessageCategory, strCode_2);
		TheApplication().RaiseErrorText(sMsg_2);              
		return (CancelOperation);
	} 		
	
	if(Field.match(/(^.)/g) == &quot;.&quot; )
	{ 
		var strCode_3 = &quot;R21_203&quot;;
		var sMsg_3 = TheApplication().LookupMessage(strMessageCategory, strCode_3);
		TheApplication().RaiseErrorText(sMsg_3);              
		return (CancelOperation);
	} 
	
	if((Field.match(/(@.)/g) == &quot;@.&quot;) || (Field.match(/(.@)/g) == &quot;.@&quot;) )
	{ 
		var strCode_4 = &quot;R21_202&quot;;
		var sMsg_4 = TheApplication().LookupMessage(strMessageCategory, strCode_4);
		TheApplication().RaiseErrorText(sMsg_4);              
		return (CancelOperation);
	} 
	
	if(Field.match(/(.$)/g) == &quot;.&quot; )
	{ 
		var strCode_5 = &quot;R21_197&quot;;
		var sMsg_5 = TheApplication().LookupMessage(strMessageCategory, strCode_5);
		TheApplication().RaiseErrorText(sMsg_5);              
		return (CancelOperation);
	} 
	
	var strtwoats = Field.replace(&apos;@@&apos;,&quot;!&quot;);
	if(strtwoats.match(/([!])/g) == &quot;!&quot;)
	{ 
		var strCode_6 = &quot;R21_196&quot;;
		var sMsg_6 = TheApplication().LookupMessage(strMessageCategory, strCode_6);
		TheApplication().RaiseErrorText(sMsg_6);              
		return (CancelOperation);
	} 
	
	var strtwodots = Field.replace(&apos;..&apos;,&quot;!&quot;);
	if(strtwodots.match(/([!])/g) == &quot;!&quot;)
	{ 
		var strCode_7 = &quot;R21_204&quot;;
		var sMsg_7 = TheApplication().LookupMessage(strMessageCategory, strCode_7);
		TheApplication().RaiseErrorText(sMsg_7);              
		return (CancelOperation);
	}
		
	if(Field.match(/(^[\w\.\+\-=]+@[\w\.\-]+\.[\w\-]+$)/g) == null &amp;&amp; Field != &quot;&quot;)
	{ 	
		var strCode_8 = ErrMsg;
		var sMsg_8 = TheApplication().LookupMessage(strMessageCategory, strCode_8);
		TheApplication().RaiseErrorText(sMsg_8);              
		return (CancelOperation);		
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreSetFieldValue"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreSetFieldValue (FieldName, FieldValue)
{
	try
	{
		if (FieldName == &quot;Contact Date Of Birth&quot;)
		{
			if (FieldValue != null &amp;&amp; FieldValue != &quot;&quot;)
			{
				var Citizenship = this.GetFieldValue(&quot;VF Contact Citizenship&quot;);
				var sTurkish = TheApplication().InvokeMethod(&quot;LookupValue&quot;, &quot;X_CITIZENSHIP&quot;, &quot;Turkish&quot;);
				var oBS = null;
				var inputs = null;
				var outputs = null;
				var ErrMsg = &quot;&quot;;
	
				ErrMsg = &quot;0036&quot;;
				var sNumMonths = &quot;&quot;;
				if (Citizenship == sTurkish)
					sNumMonths = &quot;180&quot;;
				else
					sNumMonths = &quot;216&quot;;
				oBS = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
				inputs = TheApplication().NewPropertySet();
				outputs = TheApplication().NewPropertySet();
				inputs.SetProperty(&quot;ProcessName&quot;, &quot;VFTRCBU Check Birth Date&quot;);
				inputs.SetProperty(&quot;Birth Date&quot;, FieldValue);
				inputs.SetProperty(&quot;Num Months&quot;, sNumMonths);
				oBS.InvokeMethod(&quot;RunProcess&quot;, inputs, outputs);
				if (outputs.GetProperty(&quot;Error Code&quot;) == &apos;NOK&apos;)
				{
					var strMessageCategory = &quot;VFTRCBU User Defined Errors&quot;;
					var strCode = ErrMsg;
					var sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
					TheApplication().RaiseErrorText(sMsg);
					//VFTRCBU-4420 - return canceloperation
					return (CancelOperation);
				}
				else
				{
					return (ContinueOperation);
				}
			}
		}
		//always validate format of TCId when searching (initial task search view) or setting the TCId of new customer (new customer task view)
		if ((FieldName == &quot;VFTRCBU TCId&quot; || FieldName == &quot;VFTRCBU TCId Search Aux&quot;) &amp;&amp; FieldValue != &quot;&quot;)
		{
			var ErrMsg = &quot;&quot;;
			ErrMsg = &quot;0034&quot;;
			this.CheckTcId(FieldValue, ErrMsg);
		}
		if (FieldName==&quot;VF Issue Date&quot;)
		{	
			var ErrMsg = &quot;&quot;;
			ErrMsg= &quot;215&quot;;
			this.ValidateDate(FieldValue, ErrMsg,1);
		}		
		if (FieldName == &quot;Main Email Address&quot;)
		{
			var ErrMsg = &quot;&quot;;
			ErrMsg = &quot;090&quot;;
			this.ValidateEmail(FieldValue, ErrMsg);
		}
		if (FieldName == &quot;Main Phone Number&quot;)
		{
			var ErrMsg = &quot;&quot;;
			ErrMsg = &quot;078&quot;;
			this.ValidateField(FieldValue, ErrMsg);
		}
		if (FieldName == &quot;Cellular Phone #&quot;)
		{
			var ErrMsg = &quot;&quot;;
			ErrMsg = &quot;365&quot;;
			this.ValidateMobilePhone(FieldValue, ErrMsg);
		}
		return (ContinueOperation);
	}
	finally
	{
		oBS = null;
		inputs = null;
		outputs = null;
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreSetFieldValue"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreSetFieldValue (FieldName, FieldValue)
{
	try
	{
		var BillingAccountBC : BusComp;
		var AccountBC : BusComp;
		var nFaxNum = &quot;&quot;;
		//Setting the value to all record selected
		if(FieldName == &quot;REQUEST_TYPE&quot; || FieldName == &quot;DELIVERY_MODE&quot; || FieldName == &quot;POSTAL_ADDRESS&quot; || FieldName == &quot;FAX_NUMBER&quot; || FieldName == &quot;EMAIL_ADDRESS&quot;)
		{
			var oSelected = this.FirstSelected();
			while(oSelected)
			{
				this.SetFieldValue(FieldName,FieldValue);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
				oSelected = this.NextSelected();
			}
			oSelected = this.FirstSelected();
			//Setting default Fax Number if Delivery Mode = Fax
			if(FieldName == &quot;DELIVERY_MODE&quot; &amp;&amp; FieldValue == TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_REPRINT_DEL_MODE&quot;,&quot;Fax&quot;))
			{
				while(oSelected)
				{
					BillingAccountBC = this.ParentBusComp();
					if(BillingAccountBC.Name == &quot;Billing Account&quot; || BillingAccountBC.Name == &quot;VF PRM Billing Account&quot;){ //Oracle Review October 2011 - Check if ParentBusComp is Billing Account
						//AccountBC = BillingAccountBC.ParentBusComp();
						//if (AccountBC.Name == &quot;Account&quot;){ //Oracle Review October 2011 - Check if ParentBusComp is Account
							//nFaxNum = AccountBC.GetFieldValue(&quot;Main Fax Number&quot;);
							nFaxNum = BillingAccountBC.GetFieldValue(&quot;VF Main Fax Number&quot;); //GEN_REQ_059 14.12.2012
							this.SetFieldValue(&quot;FAX_NUMBER&quot;,nFaxNum);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
						//}
					}
					oSelected = this.NextSelected();
				}
			}
		}
		if(FieldName == &quot;DELIVERY_MODE&quot;)
		{
			var sFax = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_REPRINT_DEL_MODE&quot;,&quot;Fax&quot;);
			var sPostAddr = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_REPRINT_DEL_MODE&quot;,&quot;Mail&quot;);
			var sEmail = TheApplication().InvokeMethod(&quot;LookupValue&quot;,&quot;VF_REPRINT_DEL_MODE&quot;,&quot;E-mail&quot;);
			switch(FieldValue)
			{
				case sFax:
					BillingAccountBC = this.ParentBusComp();  
					if(BillingAccountBC.Name == &quot;Billing Account&quot; || BillingAccountBC.Name == &quot;VF PRM Billing Account&quot;){//Oracle Review October 2011 - Check if ParentBusComp is Billing Account
						//AccountBC = BillingAccountBC.ParentBusComp();
						//if (AccountBC.Name == &quot;Account&quot;){ //Oracle Review October 2011 - Check if ParentBusComp is Account
						//	nFaxNum = AccountBC.GetFieldValue(&quot;Main Fax Number&quot;);
							nFaxNum = BillingAccountBC.GetFieldValue(&quot;VF Main Fax Number&quot;); //GEN_REQ_059 14.12.2012
							this.SetFieldValue(&quot;FAX_NUMBER&quot;,nFaxNum);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
						//}
					}
					
					if(this.GetFieldValue(&quot;EMAIL_ADDRESS&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;EMAIL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

					if(this.GetFieldValue(&quot;POSTAL_ADDRESS&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;POSTAL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
				break;

				case sPostAddr:
					if(this.GetFieldValue(&quot;FAX_NUMBER&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;FAX_NUMBER&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

					if(this.GetFieldValue(&quot;EMAIL_ADDRESS&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;EMAIL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
				break;

				case sEmail:
					if(this.GetFieldValue(&quot;FAX_NUMBER&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;FAX_NUMBER&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

					if(this.GetFieldValue(&quot;POSTAL_ADDRESS&quot;) != &quot;&quot;)
						this.SetFieldValue(&quot;POSTAL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
				break;
			}
		}
		if(FieldName == &quot;REQUEST_TYPE&quot;)
		{
			if(this.GetFieldValue(&quot;DELIVERY_MODE&quot;) != &quot;&quot;)
				this.SetFieldValue(&quot;DELIVERY_MODE&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

			if(this.GetFieldValue(&quot;FAX_NUMBER&quot;) != &quot;&quot;)
				this.SetFieldValue(&quot;FAX_NUMBER&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

			if(this.GetFieldValue(&quot;EMAIL_ADDRESS&quot;) != &quot;&quot;)
				this.SetFieldValue(&quot;EMAIL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 

			if(this.GetFieldValue(&quot;POSTAL_ADDRESS&quot;) != &quot;&quot;)
				this.SetFieldValue(&quot;POSTAL_ADDRESS&quot;,&quot;&quot;);//Oracle Review October 2011 - SetFieldValue in this BC and FieldValue depending 
		}

		if (FieldName == &quot;FAX_NUMBER&quot;)
		{
			if(FieldValue != null &amp;&amp; FieldValue.match(/^[9][0][0-9]{10}$/) == null)
			{ 
				var strMessageCategory = &quot;VF Message Category&quot;;
				var strCode = &quot;075&quot;;
				var sMsg = TheApplication().LookupMessage(strMessageCategory, strCode);
				TheApplication().RaiseErrorText(sMsg);              
				return (CancelOperation);
			}
		}
	}
	finally
	{
		BillingAccountBC = null;
		AccountBC = null;
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreQuery"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreQuery ()
{
	TheApplication().SetProfileAttr(&quot;SkipIPQuery&quot;,&quot;Y&quot;); //RPIRES: Added to skip VBC extra query during PreQuery

	 try
	 {
	 	var bcBA : BusComp;
	 	var boName = this.BusObject().Name();	 	
	 	var strBillingCode = &quot;&quot;;
	 	var activeView = TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;);
	 	var prevIPBillingCode = TheApplication().GetProfileAttr(&quot;IPBillingCode&quot;);
			if(activeView != &quot;VF Payment History View&quot; &amp;&amp; activeView != &quot;VF Payment Installments View&quot; &amp;&amp; activeView != &quot;VF PDF Viewing View&quot; &amp;&amp; activeView != null &amp;&amp; activeView != &quot;&quot;)//Removed Reprint view condition for PROD00026624; RPIRES - added activeView validations for VFTRCBU-26944
		 	{
		 		if(TheApplication().GetProfileAttr(&quot;invoiceInquirySkipBCPreQuery&quot;) != &quot;Y&quot;) //RCRISPIM: VFTRCBU-11807
				{
					  bcBA = this.BusObject().GetBusComp(&quot;Billing Account&quot;);
					  if(bcBA!=null &amp;&amp; bcBA.FirstSelected())
					  {
					  	strBillingCode = bcBA.GetFieldValue(&quot;Account Code&quot;);
					  	TheApplication().SetProfileAttr(&quot;IPBillingCode&quot;,strBillingCode);
					  }
				  }
				  
				  //RCRISPIM: VFTRCBU-11138 - START
				  if(activeView == &quot;VFTRCBU Subscriber CDR Inquiry View&quot;)
				  {
				  	   strBillingCode = this.ParentBusComp().GetFieldValue(&quot;Billing Account Code&quot;);
				  	   TheApplication().SetProfileAttr(&quot;IPBillingCode&quot;,strBillingCode);
				  }
				  //RCRISPIM: VFTRCBU-11138 - END
			}
			if(activeView == &quot;VF Invoice Reprint View&quot; || activeView == &quot;VF Billing Account Payments Installment View&quot; || activeView == &quot;VFTRCBU Subscriber CDR Inquiry View&quot;) //GEN_REQ_059 13.12.2012
		 	{
		 		//Start comment by Parasuram for fixing PROD00025810
			 	if(TheApplication().GetProfileAttr(&quot;IPBillingCode&quot;)==&quot;&quot;)
			 	{
				  bcBA = this.BusObject().GetBusComp(&quot;Billing Account&quot;);
				  if(bcBA!=null &amp;&amp; bcBA.FirstRecord())
				  {
				  	strBillingCode = bcBA.GetFieldValue(&quot;Account Code&quot;);
				  	TheApplication().SetProfileAttr(&quot;IPBillingCode&quot;,strBillingCode);
				  }
				}
			 }
		 
		 if(activeView == &quot;VF Billing Account Portal Summary View&quot; &amp;&amp; TheApplication().GetProfileAttr(&quot;invoiceInquiryButton&quot;) == &quot;Y&quot;) //GEN_REQ_059 13.12.2012
	 	{
	 		//Start comment by Parasuram for fixing PROD00025810
		 	if(TheApplication().GetProfileAttr(&quot;IPBillingCode&quot;)==&quot;&quot;)
		 	{
			  bcBA = this.BusObject().GetBusComp(&quot;Billing Account&quot;);
			  if(bcBA!=null) // &amp;&amp; bcBA.FirstRecord())
			  {			  
			  	var baId = bcBA.GetFieldValue(&quot;Id&quot;);
			  	TheApplication().SetProfileAttr(&quot;IPBillingId&quot;,baId);
			  }
			}
		 }
		 			 		 
		 if ( boName != null)
		 {	 
		 	if(boName == &quot;Billing Account&quot; &amp;&amp; TheApplication().GetProfileAttr(&quot;invoiceInquiryButton&quot;) == &quot;Y&quot;)
		 	{	 		 		 	
		 		if(TheApplication().GetProfileAttr(&quot;IPBillingId&quot;)==&quot;&quot;)
		 		{	 			 			
				  bcBA = this.BusObject().GetBusComp(&quot;Billing Account&quot;);	 				  
				  if(bcBA!=null) // &amp;&amp; bcBA.FirstRecord())
				  {		 				  				  				  
				  	var baId = bcBA.GetFieldValue(&quot;Id&quot;);		 				  	
				  	TheApplication().SetProfileAttr(&quot;IPBillingId&quot;,baId);
				  }
				}
		 	}
		 	else if(boName == &quot;Asset Management - Asset Item&quot;)
		 	{	 
		 		TheApplication().SetProfileAttr(&quot;IPAssetContext&quot;,&quot;Y&quot;);		 	
		 		if(TheApplication().GetProfileAttr(&quot;IPBillingCode&quot;)==&quot;&quot;)
		 		{	 		 		
				  bcBA = this.BusObject().GetBusComp(&quot;Asset Mgmt - Asset&quot;);	 				  
				  if(bcBA!=null) // &amp;&amp; bcBA.FirstRecord())
				  {		 				  
				  	var baId = bcBA.GetFieldValue(&quot;Account Code&quot;);		 				  	
				  	TheApplication().SetProfileAttr(&quot;IPBillingCode&quot;,baId);
				  }
				}
		 	} 
		 }
	 }
	 catch(e){
	 	throw e;
	 }
	 finally
	 {
	 	if(prevIPBillingCode != strBillingCode)
		 	TheApplication().SetProfileAttr(&quot;IntError&quot;,&quot;&quot;);
	 	bcBA = null;
	 	TheApplication().SetProfileAttr(&quot;SkipIPQuery&quot;,&quot;N&quot;); //RPIRES: Added to skip VBC extra query during PreQuery
	 }
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="BusComp_PreInvokeMethod"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function BusComp_PreInvokeMethod (MethodName)
{
	try{
		var psInputs : PropertySet;
		var psOutputs : PropertySet;
		var psAddress : PropertySet;
		var sWF : Service;
		TheApplication().SetProfileAttr(&quot;VF Interim Payment&quot;,&quot;VF Interim Payment&quot;);//TLGA-4160

		if(TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;) == &quot;VF Invoice Reprint View&quot; ||
		   TheApplication().GetProfileAttr(&quot;ActiveViewName&quot;) == &quot;VF Billing Account Portal Summary View&quot;
		  ){
		  
			if(MethodName == &quot;Submit&quot;)
			{
				sWF  = TheApplication().GetService(&quot;Workflow Process Manager&quot;);
				psInputs  = TheApplication().NewPropertySet();
				psOutputs = TheApplication().NewPropertySet();
				var oBO = TheApplication().ActiveBusObject();
				var sInvoiceNr = &quot;&quot;;
				var sToDate = &quot;&quot;;
				var sInvoiceAmt = &quot;&quot;;
				var sAddr = &quot;&quot;;
				var sPostalCode = &quot;&quot;;
				var sProv = &quot;&quot;;
				var sCity = &quot;&quot;;
				var strTimeStamp=&quot;&quot;;
				var strSequenceNumberMNP = &quot;&quot;;
				var sMasterAccntId = &quot;&quot;;
				var strTransactionID   = &quot;SIEBEL-&quot;;
				var dtToday = new Date();
				Clib.strftime(strTimeStamp, &quot;%m/%d/%Y %X&quot;, Clib.localtime(Clib.time()));
				var strDateToday =(dtToday.getMonth()+1) + &apos;/&apos; + dtToday.getDate() +&apos;/&apos; + dtToday.getFullYear();
				var sPostalAddr = TheApplication().GetProfileAttr(&quot;InvReprintPostalAddr&quot;);
				if(sPostalAddr != &quot;&quot; &amp;&amp; sPostalAddr != null)
				{
					psAddress = TheApplication().NewPropertySet();
					if (oBO.Name() == &quot;Billing Account&quot;)
			      	{
			        	var bcBA = oBO.GetBusComp(&quot;Billing Account&quot;);
			        	sMasterAccntId = bcBA.GetFieldValue(&quot;Master Account Id&quot;);		
			       	}
					var sCustName = this.GetFieldValue(&quot;CUSTOMER_CODE&quot;);
					getAddressDetails(sPostalAddr,sMasterAccntId,psAddress);
					sAddr = psAddress.GetProperty(&quot;StreetAddress&quot;);
					sPostalCode = psAddress.GetProperty(&quot;PostalCode&quot;);
					sProv = psAddress.GetProperty(&quot;Province&quot;);
					sCity = psAddress.GetProperty(&quot;City&quot;);
				}
				strSequenceNumberMNP = getSequence();
				strTransactionID = strTransactionID + strSequenceNumberMNP;
				var Selected = this.FirstSelected();

				psInputs.SetProperty(&quot;ProcessName&quot;, &quot;VF Invoice Re-Print Request WF&quot;);
				psInputs.SetProperty(&quot;REQUEST_USER&quot;,this.GetFieldValue(&quot;REQUEST_USER&quot;));
				psInputs.SetProperty(&quot;BILLING_ACCOUNT_CODE&quot;,TheApplication().GetProfileAttr(&quot;InvReprintBillCode&quot;));
				psInputs.SetProperty(&quot;BILLING_ACCOUNT_ID&quot;,TheApplication().GetProfileAttr(&quot;InvReprintBillId&quot;));
				psInputs.SetProperty(&quot;DISTRIBUTION_CHANNEL&quot;,TheApplication().GetProfileAttr(&quot;InvReprintDelMode&quot;));
				psInputs.SetProperty(&quot;FAX_NUM&quot;,TheApplication().GetProfileAttr(&quot;InvReprintFaxNr&quot;));
				psInputs.SetProperty(&quot;EMAIL&quot;,TheApplication().GetProfileAttr(&quot;InvReprintEmail&quot;));
				
			    if (oBO.Name() == &quot;Billing Account&quot;)
			    {
			       var oBC = oBO.GetBusComp(&quot;Billing Account&quot;);
			       psInputs.SetProperty(&quot;NAME&quot;,oBC.GetFieldValue(&quot;Customer Account Name&quot;));			
			    }
			//	psInputs.SetProperty(&quot;NAME&quot;,this.GetFieldValue(&quot;CUSTOMER_NAME&quot;));
				psInputs.SetProperty(&quot;ADDRESS1&quot;,sAddr);
				psInputs.SetProperty(&quot;TIMESTAMP&quot;,strTimeStamp);
				psInputs.SetProperty(&quot;TRANSACTION_ID&quot;,strTransactionID);
				psInputs.SetProperty(&quot;CHANNEL&quot;, &quot;SIEBEL&quot;);
				psInputs.SetProperty(&quot;POSTAL_CODE&quot;,sPostalCode);
				psInputs.SetProperty(&quot;PROVINCE&quot;,sProv);
				psInputs.SetProperty(&quot;CITY&quot;,sCity);
				//psInputs.SetProperty(&quot;ACCOUNT_ID&quot;,this.GetFieldValue(&quot;CUSTOMER_CODE&quot;));//TLGA-4160
				psInputs.SetProperty(&quot;ACCOUNT_ID&quot;,this.GetFieldValue(&quot;CRM_CUSTOMER_CODE&quot;));//TLGA-4160
				psInputs.SetProperty(&quot;CreationDate&quot;,strDateToday);
				psInputs.SetProperty(&quot;REQUEST_TYPE&quot;,TheApplication().GetProfileAttr(&quot;InvReprintReqType&quot;));
				while(Selected){
					sInvoiceNr = this.GetFieldValue(&quot;INVOICE_NUMBER&quot;);
					sToDate = this.GetFieldValue(&quot;INVOICE_DUE_DATE&quot;);
					sInvoiceAmt = this.GetFieldValue(&quot;INVOICE_DUE_AMOUNT&quot;);
					psInputs.SetProperty(&quot;INVOICE_ID&quot;,sInvoiceNr);
					psInputs.SetProperty(&quot;INVOICE_DUE_DATE&quot;,sToDate);
					psInputs.SetProperty(&quot;INVOICE_AMOUNT&quot;,sInvoiceAmt);
					sWF.InvokeMethod(&quot;RunProcess&quot;, psInputs, psOutputs);
					sInvoiceNr = &quot;&quot;;
					sToDate = &quot;&quot;;
					sInvoiceAmt = &quot;&quot;;
					Selected = this.NextSelected();
				}
				TheApplication().SetProfileAttr(&quot;InvReprintBillCode&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintBillId&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintDelMode&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintFaxNr&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintEmail&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintReqType&quot;,&quot;&quot;);
				TheApplication().SetProfileAttr(&quot;InvReprintPostalAddr&quot;,&quot;&quot;);
				return (CancelOperation);
			}
		}
		return (ContinueOperation);
	}
	finally
	{
		sWF	= null;
		psInputs = null;
		psOutputs = null;
		psAddress = null;
		TheApplication().SetProfileAttr(&quot;VF Interim Payment&quot;,&quot;&quot;); //++ DEF-2353
	}
}"

			<BUSCOMP_SERVER_SCRIPT
			   INACTIVE="N"
			   NAME="getAddressDetails"
			   PROGRAM_LANGUAGE="JS"
			   SCRIPT="function getAddressDetails(sPostalAddr,sMasterAccntId,psAddress)
{
	try{
		var oBO : BusObject = TheApplication().GetBusObject(&quot;CUT Address&quot;);
		var oBC : BusComp = oBO.GetBusComp(&quot;CUT Address&quot;);
		var streetAddr1 = &quot;&quot;;
		var AptNr = &quot;&quot;;
		var Distr = &quot;&quot;;
		var NgbdVlg = &quot;&quot;;
		var Site = &quot;&quot;;
		var streetAddr = &quot;&quot;;
		var Aven = &quot;&quot;;
		var Vill = &quot;&quot;;
		var Place = &quot;&quot;;
		var Num = &quot;&quot;;
		with(oBC)
		{
			SetViewMode(AllView);
			ClearToQuery();
			ActivateField(&quot;Street Address&quot;);
			ActivateField(&quot;Apartment Number&quot;);
			ActivateField(&quot;District&quot;);
			ActivateField(&quot;VF Neighborhood/Village&quot;);
			ActivateField(&quot;VF Site&quot;);
			ActivateField(&quot;Postal Code&quot;);
			ActivateField(&quot;Province&quot;);
			ActivateField(&quot;City&quot;);
			ActivateField(&quot;Address Num&quot;);
			ActivateField(&quot;VF Place&quot;);
			ActivateField(&quot;VF Avenue Boulevar&quot;);
			ActivateField(&quot;VF Village&quot;);
			
			var cmdArray = sPostalAddr.split(&quot; Mah.,&quot;);
            var village = cmdArray[0];
            SetSearchExpr(&quot;[Client Id] = &apos;&quot;+ sMasterAccntId +&quot;&apos; and [VF Neighborhood/Village] = &apos;&quot;+ village +&quot;&apos;&quot;);
			
			//SetSearchExpr(&quot;[Client Id] = &apos;&quot;+ AccntName +&quot;&apos; and [Address Name] = &apos;&quot;+ sPostalAddr +&quot;&apos;&quot;);
			ExecuteQuery(ForwardOnly);
			if(FirstRecord())
			{
				streetAddr = GetFieldValue(&quot;Street Address&quot;);
				AptNr = GetFieldValue(&quot;Apartment Number&quot;);
				Distr = GetFieldValue(&quot;District&quot;);
				NgbdVlg = GetFieldValue(&quot;VF Neighborhood/Village&quot;);
				Site = GetFieldValue(&quot;VF Site&quot;);
				Aven = GetFieldValue(&quot;VF Avenue Boulevar&quot;);
				Vill = GetFieldValue(&quot;VF Village&quot;);
				Place = GetFieldValue(&quot;VF Place&quot;);
				Num = GetFieldValue(&quot;Address Num&quot;);
				streetAddr1 = NgbdVlg + &quot; Mah.&quot; + Vill + &quot; Köyü&quot; + Aven + &quot; &quot; + streetAddr + &quot; &quot; + Place + &quot; &quot; + Site + &quot; &quot; + AptNr + &quot; &quot; + Num + &quot; &quot; + Distr;
				psAddress.SetProperty(&quot;StreetAddress&quot;,streetAddr1);
				psAddress.SetProperty(&quot;PostalCode&quot;,GetFieldValue(&quot;Postal Code&quot;));
				psAddress.SetProperty(&quot;Province&quot;,GetFieldValue(&quot;Province&quot;));
				psAddress.SetProperty(&quot;City&quot;,GetFieldValue(&quot;City&quot;));
			}
		}
	}
	finally
	{
		oBC = null;
		oBO = null;
	}
}"
